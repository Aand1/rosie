svs -e
svs connect_viewer 65000

sp {propose-wait
  (state <s> ^superstate nil
              -^operator.name wait)
-->
  (<s> ^operator <o> +)
  (<o> ^name wait)
}

sp {create*filter*1
  (state <s> ^superstate nil
              ^svs.command <cmd>)
-->
  (<cmd> ^extract_once <e>)
  (<e> ^type intersect
        ^a <a> ^b <b>)
  (<a> ^type node ^id box1)
  (<b> ^type node ^id box2)
}

