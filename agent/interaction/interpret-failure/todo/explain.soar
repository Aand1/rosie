sp {interpret-status*propose*explanation
   (state <s> ^name interpret-status
   	      ^status <stat>
	      -^modify-segment-info
	      ^top-state.interaction.stack.segment.purpose.satisfaction.action-event <event1>)
   (<stat> ^action-event <event2>)
   (<event1> ^status learning
   	     ^type <v>)
   (<event2> ^type <v>)
-->
   (<s> ^operator <o> +)
   (<o> ^name explanation
   	^event <event1>)
}


sp {interpret-status*apply*explanation
   (state <s> ^name interpret-status
   	      ^operator <o>
	      ^top-state.interaction.stack.segment <seg>)
   (<o> ^name explanation
   	^event <event>)
   (<seg> ^purpose.parameters.indexable.result <ia>)
-->
   (<s> ^modify-segment-info <info>)
   (<info> ^segment <seg>
   	   ^modification <m>)
   (<m> ^type satisfaction
   	^delete <del>
	^add <add>)
   (<del> ^action-event <event>)
   (<add> ^learning-event <event>)
   (<ia> ^applied yes -)
}