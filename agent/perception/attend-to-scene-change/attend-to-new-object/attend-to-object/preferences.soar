
# Level 1: property
sp {attend-to-scene*level*preferences*property
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^category perception
         ^level property)
   (<o2> ^category perception
         ^level << value confidence >>)
-->
   (<s> ^operator <o1> > <o2>)
}

# Level 2: value
sp {attend-to-scene*level*preferences*value
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^category perception
         ^level value)
   (<o2> ^category perception
         ^level confidence)
-->
   (<s> ^operator <o1> > <o2>)
}

# Level 3: Confidences

# Level 4: complete-attend-to-object
sp {attend-to-object*prefer*all*over*complete-attend-to-object
   (state <s> ^name attend-to-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name <> complete-attend-to-object
         ^category perception)
   (<o2> ^name complete-attend-to-object)
-->
   (<s> ^operator <o1> > <o2>)
}


