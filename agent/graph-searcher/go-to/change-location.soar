sp {go-to*propose*change-location
   (state <s> ^name go-to
              ^destination.id <loc>)
-->
   (<s> ^operator <o> +)
   (<o> ^name change-location
        ^location <loc>)
}

sp {go-to*apply*change-location
   (state <s> ^name go-to
              ^operator <o>
              ^top-state.io.output-link <out>)
   (<o> ^name change-location
        ^location <loc>)
-->
   (<out> ^change-location.location <loc>)
}