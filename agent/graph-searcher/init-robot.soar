sp {clean-house*propose*init-robot
   (state <s> ^name robot
             -^problem-space)
-->
   (<s> ^operator <op> !)
   (<op> ^name init-robot)
}

sp {robot*apply*init-robot*directions
   (state <s> ^name robot
              ^operator.name init-robot)
-->
   (<s> ^problem-space <ps>
        ^desired <d>
        ^objects <b>
        ^path-cost 0)
   (<ps> ^name clean-house
         ^default-state-copy yes
         ^one-level-attributes areas current-location destination-area type
         ^one-level-attributes visited-area)
}

# Default mission = patrol areas
sp {apply*init-robot
   (state <s> ^name robot
              ^operator.name init-robot)
-->
   (<s> ^missions <mis>)
   (<mis> ^mission <m1>)
   (<m1> ^name patrol
         ^status proposed
         ^first-area <area1>)
   (<area1> ^next <area2>
            ^id 9)
   (<area2> ^next nil #<area3>
            ^id 1)
