sp {anystate*elaborate*name*from*superoperator
   (state <s> ^superstate.operator.name <name>)
-->
   (<s> ^name <name>)
}

sp {anystate*elaborate*type*from*superoperator
   (state <s> ^superstate.operator.type <name>)
-->
   (<s> ^type <name>)
}

sp {anystate*elaborate*io*from*superstate
   (state <s> ^superstate.io <io>)
-->
   (<s> ^io <io>)
}

sp {anystate*elaborate*current-location*from*topstate
   (state <s> ^name <> robot
              ^top-state.current-location <ca>)
-->
   (<s> ^current-location <ca>)
}
