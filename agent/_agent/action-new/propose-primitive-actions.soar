sp {action*propose*pickup*object
   (state <s> ^name action
              #^problemspace.action <act>
              ^world <world>)
   (<world> ^robot.arm <arm>
            ^objects.object <obj>)
   (<arm> -^grabbed <any>)
   (<obj> ^predicates.category block)
#   (<act> -^type none)
-->
   (<s> ^operator <o> +)
   (<o> ^name op_pick-up-object
        ^argument1 <obj>
        ^action-type primitive)
}

sp {propose*put-down
   (state <s> ^name action
              #^problemspace.action <act>
              ^world <world>)
   (<world> ^objects.object <grabbed>
            ^objects.object { <dest> <> <grabbed> })
   (<grabbed> ^predicates.arm-status grabbed)
   (<act> -^type none)
-->
   (<s> ^operator <op> +)
   (<op> ^name op_put-down-object
     ^argument1 <grabbed>
     ^argument2.argument <dest>
     ^action-type primitive)
}

