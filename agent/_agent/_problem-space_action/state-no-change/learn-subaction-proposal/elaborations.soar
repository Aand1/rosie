}

sp {learn-subaction-proposal*elaborate*argument-map*1*argument
   (state <s> ^name learn-subaction-proposal
              ^argument <arg1>)
   -{(<s> ^argument { <arg2> <> <arg1> })}
-->
   (<s> ^argument-map <map>)
   (<map> ^argument.source <arg1>)
}

sp {learn-subaction-proposal*elaborate*argument-map*2*arguments
   (state <s> ^name learn-subaction-proposal
              ^argument <arg1>
              ^argument { <arg2> > <arg1> })
   -{(<s> ^argument { <arg3> <> <arg1> <> <arg2> })}
-->
   (<s> ^argument-map <map>)
   (<map> ^argument.source <arg1>
          ^argument.source <arg2>)
}

sp {learn-subaction-proposal*elaborate*argument-map*3*arguments
   (state <s> ^name learn-subaction-proposal
              ^argument <arg1>
              ^argument { <arg2> > <arg1> }
              ^argument { <arg3> > <arg2> })
   -{(<s> ^argument { <arg4> <> <arg1> <> <arg2> <> <arg3> })}
