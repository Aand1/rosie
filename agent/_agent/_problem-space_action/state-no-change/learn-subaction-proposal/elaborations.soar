   (<map> ^argument.source <arg1>
          ^argument.source <arg2>
          ^argument.source <arg3>)
}

sp {learn-subaction-proposal*elaborate*argument-map*4*arguments
   (state <s> ^name learn-subaction-proposal
              ^argument <arg1>
              ^argument { <arg2> > <arg1> }
              ^argument { <arg3> > <arg2> }
              ^argument { <arg4> > <arg3> })
   -{(<s> ^argument { <arg5> <> <arg1> <> <arg2> <> <arg3> <> <arg4> })}
