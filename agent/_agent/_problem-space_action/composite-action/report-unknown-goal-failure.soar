sp {action*composite-action*propose*report-unknown-goal-failure
   (state <s> ^problem-space <ps>
              ^action-context <action>
              ^unconnected-argument true)
   (<ps> ^name action
         ^action-type composite)
-->
   (<s> ^operator <o> + = !)
   (<o> ^name report-unknown-goal-failure)
}

sp {action*composite-action*apply*report-unknown-goal-failure
   (state <s> ^problem-space <ps>
              ^operator.name report-unknown-goal-failure
              ^top-state.interaction.status <status>
              ^action-context <action>)
   (<ps> ^name action
         ^action-type composite)
-->
   (<status> ^failure <f>)
   (<f> ^type unknown-goal
        ^action <action>)
}

