sp {action*selection*evaluate-operator*elaborate*state-to-copy*supersuperstate
   (state <s> ^name evaluate-operator
              ^problem-space.name action
              ^superstate.superstate <sss>)
-->
   (<s> ^state-to-copy <sss>)
}

sp {action*selection*evaluate-operator*elaborate*desired*success
   (state <s> ^name evaluate-operator
              ^problem-space.name action
              ^desired <d>)
   (<d> ^satisfied true)
-->
   (<s> ^success <d>)
}

sp {action*selection*evaluate-operator*elaborate*operator*duplicate-of
   (state <s> ^name evaluate-operator
              ^problem-space.name action
              ^operator <copy-op> +
              ^copy-operator-map.<src-op> <copy-op>)
-->
   (<copy-op> ^duplicate-of <src-op>)
}

sp {action*selection*evaluate-operator*elaborate*look-ahead-operator
   (state <s> ^name evaluate-operator
              ^problem-space.name action
              ^superstate.operator <ss-op>
              ^copy-operator-map.<src-op> <copy-op>)
   (<ss-op> ^superoperator <src-op>)
-->
   (<s> ^look-ahead-operator <src-op>
        ^untried-tied-operator <src-op>)
}