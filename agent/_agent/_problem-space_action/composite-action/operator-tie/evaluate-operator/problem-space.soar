sp {action*selection*evaluate-operator*elaborate*problem-space
   (state <s> ^name evaluate-operator
              ^superstate.problem-space.name action)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action-type composite
         ^default-state-copy no
         ^default-operator-copy yes
         ^default-desired-copy yes
         ^action <action>
         ^state <state>
         ^learning <learn>)
   (<action> ^type simulate
             ^propose-action-operators true)
   (<state> ^type imagined
            ^world-copy deep)
}

sp {action*selection*evaluate-operator*elaborate*problem-space*learning*type*superstate
   (state <s> ^name evaluate-operator
              ^problem-space <ps>
              ^superstate.problem-space.learning.type <type>)
   (<ps> ^name action
         ^learning <learn>)
-->
   (<learn> ^type <type>)
}

