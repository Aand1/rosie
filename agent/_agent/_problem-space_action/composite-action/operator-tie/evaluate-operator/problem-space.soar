sp {action*selection*evaluate-operator*elaborate*problem-space
   (state <s> ^name evaluate-operator
              ^superstate.problem-space.name action)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^default-state-copy no
         ^default-operator-copy yes
         ^default-desired-copy yes
         ^action <action>
         ^state <state>
         ^learning <learn>)
   (<action> ^type simulate
             ^propose-action-operators true)
   (<state> ^type imagined
            ^world-copy deep)
}

sp {action*selection*evaluate-operator*elaborate*learning*retrospective
   (state <s> ^name evaluate-operator
              ^problem-space <ps>)
   (<ps> ^name action
         ^learning.type retrospective
         ^state <state>)
-->
   (<state> ^subactions-copy deep)
}


