sp {action*operator-tie*elaborate*selection*problem-space
   (state <s> ^superstate.problem-space.name action
              ^superstate.problem-space.action-type composite
              ^impasse tie
              ^attribute operator)
-->
   (<s> ^name selection
        ^problem-space <ps>)
   (<ps> ^name action
         ^default-state-copy no
         ^action <action>
         ^state <state>
         ^learning <learn>)
   (<action> ^type simulate)
   (<state> ^world-copy deep)
}

sp {action*selection*elaborate*problem-space*learning*type*from*superstate
   (state <s> ^name selection
              ^problem-space <ps>
              ^superstate.problem-space.learning.type <type>)
   (<ps> ^name action
         ^learning <learn>)
-->
   (<learn> ^type <type>)
}




