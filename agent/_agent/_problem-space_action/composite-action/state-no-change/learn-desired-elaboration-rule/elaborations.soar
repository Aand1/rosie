sp {learn-desired-elaboration-rule*elaborate*action-context*from*superstate
   (state <s> ^name learn-desired-elaboration-rule
              ^superstate.superstate.operator <action>)
-->
   (<s> ^action-context <action>)
}

sp {learn-desired-elaboration-rule*elaborate*predicate
   (state <s> ^name learn-desired-elaboration-rule
              ^action-id.goal.predicate <pred>)
-->
   (<s> ^predicate <gp>)
   (<gp> ^smem-rep <pred>)
}

sp {learn-desired-elaboration-rule*elaborate*desired
   (state <s> ^name learn-desired-elaboration-rule)
-->
   (<s> ^desired <desired>)
}

sp {learn-desired-elaboration-rule*elaborate*action-id
   (state <s> ^name learn-desired-elaboration-rule
              ^superstate.action-id <id>)
-->
   (<s> ^action-id <id>)
}

sp {learn-desired-elaborate-rule*elaborate*all-predicates-grounded*true
   (state <s> ^name learn-desired-elaboration-rule
              ^predicate <p>)
   -{(<s> ^predicate <pred>)
     (<pred> -^grounded-rep)}
-->
   (<s> ^all-predicates-grounded true)
}