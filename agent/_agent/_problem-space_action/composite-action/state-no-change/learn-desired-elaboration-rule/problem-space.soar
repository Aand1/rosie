sp {learn-desired-elaboration-rule*elaborate*problem-space
   (state <s> ^name learn-desired-elaboration-rule
              ^goal-id <goal>)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^world.copy-type shallow)
}

sp {learn-desired-elaboration-rule*elaborate*problem-space*learning*use*chunking
   (state <s> ^name learn-desired-elaboration-rule
              ^problem-space <ps>
              ^all-predicates-grounded true
              ^goal-id <goal>)
-->
   (<ps> ^learning.use chunking)
}
