sp {learn-desired-elaboration-rule*propose*report-unknown-goal-failure*no*goal
   (state <s> ^name learn-desired-elaboration-rule
              -^goal-id <goal>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name report-unknown-goal-failure)
}

sp {learn-desired-elaboration-rule*propose*report-unknown-goal-failure*grounding-failure
   (state <s> ^name learn-desired-elaboration-rule
              ^predicate.grounding-failure true)
-->
   (<s> ^operator <o> + =)
   (<o> ^name report-unknown-goal-failure)
}

sp {learn-desired-elaboration-rule*apply*report-unknown-goal-failure
   (state <s> ^name learn-desired-elaboration-rule
              ^operator.name report-unknown-goal-failure
              ^top-state.interaction.status <status>
              ^superstate.superstate.operator <action>)
-->
   (<status> ^failure <f>)
   (<f> ^type unknown-goal
        ^action <action>)
}

