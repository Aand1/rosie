sp {action*composite-action*elaborate*smem-query
   (state <s> ^problem-space <ps>
              ^action-context.action-handle <action-handle>)
   (<ps> ^name action
         ^action-type composite)
-->
   (<s> ^smem-query <query>)
   (<query> ^cue <cue>
            ^depth 8)
   (<cue> ^handle <action-handle>)
}

sp {action*composite-action*elaborate*action-id*from*query*result
   (state <s> ^problem-space <ps>
              ^smem-query <query>
              ^action-context.action-handle <action-handle>)
   (<query> ^cue.handle <action-handle>
            ^result <id>)
-->
   (<s> ^action-id <id>)
}