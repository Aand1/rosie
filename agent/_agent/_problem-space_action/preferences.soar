sp {action*reject*non-matched*operators
   (state <s> ^problem-space.name action
              ^operator <o> +
              ^operator-matches.match-info <info>)
   (<info> ^operator <o>
           ^op-matched no)
-->
   (<s> ^operator <o> -)
}

sp {composite-action*execute*prefer*report-performed-action*over*others
   (state <s> ^problem-space.name action
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name report-performed-action)
   (<o2> ^name <> report-performed-action)
-->
   (<s> ^operator <o1> > <o2>)
}
         