sp {initiate-find1*propose*initiate-go1*to*starting-location
   (state <s> ^name initiate-find1
              ^object-to-find.predicates.arm-status grabbed
              ^current-waypoint <> <start-wp>
              ^starting-waypoint <start-wp>)
   (<start-wp> ^handle <start-handle>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name initiate-go1
        ^action-type composite
        ^arg2 <loc-arg>)
   (<loc-arg> ^type predicate
              ^handle to1
              ^2 <loc>)
   (<loc> ^waypoint <start-handle>)
}

sp {initiate-find1*propose*initiate-go1*to*fringe*waypoint
   (state <s> ^name initiate-find1
              ^object-to-find.predicates.visible false
              ^fringe <wp>
             -^visited <wp>)
   (<wp> ^handle <wp-handle>)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name initiate-go1
        ^action-type composite
        ^arg2 <loc-arg>)
   (<loc-arg> ^type predicate
              ^predicate to1
              ^2 <loc>)
   (<loc> ^waypoint <wp-handle>)
}

sp {initiate-find1*apply*initiate-go1*mark*current-waypoint*visited
   (state <s> ^name initiate-find1
              ^operator.name initiate-go1
              ^action-context <a>
              ^current-waypoint <wp>)
-->
   (<a> ^visited <wp>)
}
