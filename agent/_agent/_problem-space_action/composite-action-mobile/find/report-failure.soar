sp {initiate-find1*propose*report-failure
   (state <s> ^name initiate-find1
              ^object-to-find.predicates.visible false
             -^fringe-edge <wp>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-failure)
}

sp {initiate-find1*apply*report-failure*report
   (state <s> ^name initiate-find1
              ^operator.name report-failure
              ^top-state.interaction.status <status>)
-->
   (<status> ^failure.type no-proposed-action)
}

sp {initiate-find1*apply*report-failure*pop-segment
   (state <s> ^name initiate-find1
              ^operator.name report-failure
              ^top-state.interaction.changes <c>)
-->
   (<c> ^terminate-segment true)
}