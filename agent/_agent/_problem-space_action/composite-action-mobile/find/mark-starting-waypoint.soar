sp {initiate-find*propose*mark-starting-waypoint
   (state <s> ^name initiate-find1
              ^action-context <a>)
   (<a> -^starting-waypoint)
-->
   (<s> ^operator <o> + >)
   (<o> ^name mark-starting-waypoint)
}

sp {initiate-find*apply*mark-starting-waypoint
   (state <s> ^name initiate-find1
              ^operator.name mark-starting-waypoint
              ^world.robot.current-waypoint <wp>
              ^action-context <a>)
-->
   (<a> ^starting-waypoint <wp>)
}
