sp {initiate-deliver*elaborate*problem-space*propose-actions
   (state <s> ^name initiate-deliver1
              ^problem-space.action <action>)
-->
   (<action> ^propose-actions true)
}

sp {initiate-deliver*elaborate*object-to-deliver*from*superoperator
   (state <s> ^name initiate-deliver1
              ^superstate.operator.arg1.id <obj>)
-->
   (<s> ^object-to-deliver <obj>)
}

sp {initiate-deliver*elaborate*delivery-location*from*superoperator
   (state <s> ^name initiate-deliver1
              ^superstate.operator.arg2.{2} <loc>)
-->
   (<s> ^delivery-location <loc>)
}

sp {initiate-deliver*elaborate*delivery-waypoint*from*top-state
   (state <s> ^name initiate-deliver1
              ^delivery-location.waypoint <wp-handle>
              ^top-state.maps.map.waypoint <wp>)
   (<wp> ^handle <wp-handle>)
-->
   (<s> ^delivery-waypoint <wp>)
}

sp {initiate-deliver*elaborate*current-waypoint*from*top-state
   (state <s> ^name initiate-deliver1
              ^world.robot.current-waypoint <wp>)
-->
   (<s> ^current-waypoint <wp>)
}
