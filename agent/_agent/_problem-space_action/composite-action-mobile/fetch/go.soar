sp {initiate-fetch1*propose*initiate-go1*to*fetch-location
   (state <s> ^name initiate-fetch1
              ^object-to-fetch.predicates.visible false
              ^fetch-location <loc>
              ^current-waypoint <wp>
              -^fetch-waypoint <wp>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name initiate-go1
        ^action-type composite
        ^arg2 <loc-arg>)
   (<loc-arg> ^type predicate
              ^predicate to1
              ^2 <loc>)
}

sp {initiate-fetch1*propose*initiate-go1*to*starting-location
   (state <s> ^name initiate-fetch1
              ^object-to-fetch.predicates.arm-status grabbed
              ^current-waypoint <> <start-wp>
              ^starting-waypoint <start-wp>)
   (<start-wp> ^handle <start-handle>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name initiate-go1
        ^action-type composite
        ^arg2 <loc-arg>)
   (<loc-arg> ^type predicate
              ^predicate to1
              ^2 <loc>)
   (<loc> ^waypoint <start-handle>)
}
