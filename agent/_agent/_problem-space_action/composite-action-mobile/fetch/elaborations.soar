sp {initiate-fetch*elaborate*problem-space*propose-actions
   (state <s> ^name initiate-fetch1
              ^problem-space.action <action>)
-->
   (<action> ^propose-actions true)
}

sp {initiate-fetch*elaborate*action-context*from*superoperator
   (state <s> ^initiate-fetch1
              ^superstate.operator <o>)
-->
   (<s> ^action-context <o>)
}

sp {initiate-fetch*elaborate*object-to-fetch*from*superoperator
   (state <s> ^name initiate-fetch1
              ^action-context.arg1.object <obj>)
-->
   (<s> ^object-to-fetch <obj>)
}

sp {initiate-fetch*elaborate*fetch-location*from*superoperator
   (state <s> ^name initiate-fetch1)
-->
   (<s> ^fetch-location <loc>)
   (<loc> ^name kitchen1 ^waypoint wp25)
}

sp {initiate-fetch*elaborate*fetch-waypoint*from*top-state
   (state <s> ^name initiate-fetch1
              ^fetch-location.waypoint <wp-handle>
              ^top-state.maps.map.waypoint <wp>)
   (<wp> ^handle <wp-handle>)
-->
   (<s> ^fetch-waypoint <wp>)
}

sp {initiate-fetch*elaborate*current-waypoint*from*top-state
   (state <s> ^name initiate-fetch1
              ^world.robot.current-waypoint <wp>)
-->
   (<s> ^current-waypoint <wp>)
}

sp {initiate-fetch*elaborate*starting-waypoint*from*action-context
   (state <s> ^name initiate-fetch1
              ^action-context.starting-waypoint <wp>)
-->
   (<s> ^starting-waypoint <wp>)
}
