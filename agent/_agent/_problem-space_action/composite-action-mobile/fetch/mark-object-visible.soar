sp {initiate-fetch*propose*mark-object-visible
   (state <s> ^name initiate-fetch1
              ^fetch-waypoint <wp>
              ^current-waypoint <wp>
              ^object-to-fetch <obj>)
   (<obj> ^predicates <preds>)
   (<preds> ^visible false)
-->
   (<s> ^operator <o> + >)
   (<o> ^name mark-object-visible)
}

sp {initiate-fetch*apply*mark-object-visible
   (state <s> ^name initiate-fetch1
              ^operator.name mark-object-visible
              ^object-to-fetch <obj>)
   (<obj> ^predicates <preds>)
   (<preds> ^visible false)
-->
   (<preds> ^visible true
            ^visible false -)
}
        