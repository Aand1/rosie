
sp {action*problem-space*elaborate*action-type*from*superstate
   (state <s> ^problem-space <ps>
             -^superstate.superstate.problem-space <ps>
              ^superstate.problem-space.action.type <type>)
   (<ps> ^name action
         ^action <action>)
   -{(<action> ^type <> <type>)}
-->
   (<action> ^type <type>)
}

sp {action*problem-space*elaborate*learning-type*from*superstate
   (state <s> ^problem-space <ps>
             -^superstate.superstate.problem-space <ps>
              ^superstate.problem-space.learning.type <type>)
   (<ps> ^name action
         ^learning <learn>)
   -{(<learn> ^type <> <type>)}
-->
   (<learn> ^type <type>)
}