sp {topstate*elaborate*agent-info
   (state <s> ^topstate <s>)
-->
   (<s> ^agent-info <info>)
}

sp {topstate*elaborate*agent-info*category-info
   (state <s> ^topstate <s>
              ^agent-info <info>)
-->
   (<info> ^category-info <cat>)
   (<cat> ^table location1
           ^garbage location1
           ^pantry location1
           ^stove location1
           ^scale location1
           ^thermometer location1
           ^home location1
           ^goal location1
           ^open-spot location1
           ^grid-1 location1
           ^grid-2 location1
           ^grid-3 location1
           ^grid-4 location1
           ^grid-5 location1
           ^grid-6 location1
           ^grid-7 location1
           ^grid-8 location1
           ^grid-9 location1
           ^grid-10 location1
           ^grid-11 location1
           ^grid-12 location1
           ^grid-red location1
           ^grid-orange location1
           ^grid-yellow location1
           ^grid-green location1
           ^grid-blue location1
           ^yellow-grid location1
           ^green-grid location1
           ^blue-grid location1)
}

sp {topstate*elaborate*world*object*category*default*block
   (state <s> ^topstate <s>
              ^world.objects.object.predicates <preds>)
   (<preds> -^category <> block1)
-->
   (<preds> ^category block1
            ^movable true)
}

sp {topstate*elaborate*world*object*category*from*info
   (state <s> ^topstate <s>
              ^world.objects.object.predicates <preds>
              ^agent-info.category-info.<name> <cat>)
   (<preds> ^name <name>)
-->
   (<preds> ^category <cat>)
}
   