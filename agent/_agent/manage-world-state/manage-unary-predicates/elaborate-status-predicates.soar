sp {topstate*elaborate*world*object*status*live
   (state <s> ^topstate <s>
              ^scene-monitor.object <mon-obj>)
   (<mon-obj> ^differences.is-stale false
              ^wm-obj.predicates <preds>)
-->
   (<preds> ^status live)
}

sp {topstate*elaborate*world*object*status*stale
   (state <s> ^topstate <s>
              ^scene-monitor.object <mon-obj>)
   (<mon-obj> ^differences.is-stale true
              ^wm-obj.predicates <preds>)
-->
   (<preds> ^status stale)
}

          