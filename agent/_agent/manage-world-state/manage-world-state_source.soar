# Source child problem space "world"
pushd world
source world_source.soar
popd

# ROSIE_ENV = magicbot or tabletop
if { $ROSIE_ENV == "magicbot" || $ROSIE_ENV == "tabletop" } {

	# Source child problem space "perception-monitor"
	pushd perception-monitor
	source perception-monitor_source.soar
	popd

	# Source child problem space "attend-to-scene-change"
	pushd attend-to-scene-change
	source attend-to-scene-change_source.soar
	popd

}
# END magicbot/tabletop

# ROSIE_ENV = internal
if { $ROSIE_ENV == "internal" } {

	pushd internal-world
	source internal-world_source.soar
	popd

}
# END internal