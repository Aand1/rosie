sp {top-state*apply*init-agent*create*world-state
   (state <s> ^top-state <s>
              ^operator.name init-agent
              ^world-usage external)
-->
   (<s> ^world <w>)
   (<w> ^objects <objs>
        ^predicates <preds>
        ^robot <rob>)
   (<rob> ^arm <arm>
          ^handle rosie
          ^predicates.name rosie
          ^moving-state stopped)
   (<arm> ^state external)
}
