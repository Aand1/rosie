sp {top-state*world*elaborate*object*from*perception-monitor
   (state <s> ^top-state <s>
              ^world.objects <objs>
              ^perception-monitor.object-monitor.object-info <obj-info>)
   (<obj-info> ^object-handle <obj-handle>
               ^belief-obj <bel-obj>)
-->
   (<objs> ^object <obj>)
   (<obj> ^handle <obj-handle>
          ^item-type object
          ^real true
          ^svs-obj <bel-obj>
          ^predicates <preds>)
   (<obj-info> ^wm-obj <obj>)
   (<preds> ^visible true)
}
