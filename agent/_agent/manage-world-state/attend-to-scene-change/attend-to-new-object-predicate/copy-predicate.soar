sp {attend-to-new-object-predciate*propose*copy-predicate
   (state <s> ^name attend-to-new-object-predicate
              ^object-info.wm-obj.predicates <preds>
              ^property-handle <prop>
              ^predicate-handle <pred>)
-->
   (<s> ^operator <o> +)
   (<o> ^name copy-predicate)
}

sp {attend-to-new-object-predciate*apply*copy-predicate
   (state <s> ^name attend-to-new-object-predicate
              ^operator.name copy-predicate
              ^object-info.wm-obj.predicates <preds>
              ^property-handle <prop>
              ^predicate-handle <pred>)
-->
   (<preds> ^<prop> <pred>)
}