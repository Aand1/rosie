sp {attend-to-new-stable-object*propose*match-known-object
   (state <s> ^name attend-to-new-stable-object
              ^object-info <obj-info>
             -^matches-known-object false)
-->
   (<s> ^operator <o> +)
   (<o> ^name match-known-object
        ^object-info <obj-info>)
}

