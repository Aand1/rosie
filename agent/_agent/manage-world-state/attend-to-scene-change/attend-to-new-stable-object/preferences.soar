# Operator ordering:
# 1. match-known-object
# 2. match-missing-object
# 3. create-new-object

sp {attend-to-new-stable-object*prefer*match-known-object*over*others
   (state <s> ^name attend-to-new-stable-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name match-known-object)
   (<o2> ^name << match-missing-object create-new-object >>) 
-->
   (<s> ^operator <o1> > <o2>)
}

sp {attend-to-new-stable-object*prefer*match-missing-object*over*others
   (state <s> ^name attend-to-new-stable-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name match-missing-object)
   (<o2> ^name << create-new-object >>)
-->
   (<s> ^operator <o1> > <o2>)
}


