sp {match-world-object*propose*complete-match-world-object
   (state <s> ^name match-world-object)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-match-world-object)
}

sp {match-world-object*apply*complete-match-world-object
   (state <s> ^name match-world-object
              ^operator.name complete-match-world-object
              ^superstate <ss>)
-->
   (<ss> ^matches-world-object none)
}
