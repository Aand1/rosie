sp {attend-to-new-object*best*preferences
   (state <s> ^name attend-to-new-object
              ^operator <o> +)
   (<o> ^name << link-to-existing-object ignore-new-object reject-smem-match create-belief-object >> )
-->
   (<s> ^operator <o> >)
}

sp {attend-to-new-object*prefer*other*bests*over*ignore-new-object
   (state <s> ^name attend-to-new-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << link-to-existing-object reject-smem-match create-belief-object >>)
   (<o2> ^name ignore-new-object)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {attend-to-new-object*prefer*match-world-object*over*others
   (state <s> ^name attend-to-new-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name match-world-object)
   (<o2> ^name << match-smem-object create-world-object create-object-info >> )
-->
   (<s> ^operator <o1> > <o2>)
}

sp {attend-to-new-object*prefer*match-smem-object*over*others
   (state <s> ^name attend-to-new-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name match-smem-object)
   (<o2> ^name << create-world-object create-object-info >> )
-->
   (<s> ^operator <o1> > <o2>)
}

sp {attend-to-new-object*prefer*create-world-object*over*others
   (state <s> ^name attend-to-new-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name create-world-object)
   (<o2> ^name << create-object-info >> )
-->
   (<s> ^operator <o1> > <o2>)
}