sp {match-known-object*propose*intersect-command
   (state <s> ^name match-known-object
              ^object-info.perception-obj.id <id>
             -^intersect-command.result)
-->
   (<s> ^operator <o> +)
   (<o> ^name intersect-command
        ^object-id <id>
        ^candidate-set <cand>)
   (<cand> ^type tag_select 
           ^a.type all_nodes
           ^tag_name object-source
           ^tag_value belief)
}

