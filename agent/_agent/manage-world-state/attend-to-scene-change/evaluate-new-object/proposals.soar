sp {evaluate-new-object*propose*match-known-object
   (state <s> ^name evaluate-new-object
               ^object-info <obj-info>
             -^matches-known-object)
-->
   (<s> ^operator <o> +)
   (<o> ^name match-known-object
        ^object-info <obj-info>)
}

sp {evaluate-new-object*propose*match-missing-object
   (state <s> ^name evaluate-new-object
              ^object-info <obj-info>
              ^match-missing-object-info <missing>
             -^matches-missing-object)
   (<missing> ^wm-obj.predicates.visibility not-visible1) # The object may be no longer missing
-->
   (<s> ^operator <o> +)
   (<o> ^name match-missing-object
        ^object-info <obj-info>
        ^missing-object-info <missing>)
}

sp {evaluate-new-object*propose*create-new-object
   (state <s> ^name evaluate-new-object
              ^object-info.perception-obj <perc-obj>
              ^object-info.object-handle <obj-handle>)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-new-object
        ^perception-obj <perc-obj>
        ^object-handle <obj-handle>)
}
