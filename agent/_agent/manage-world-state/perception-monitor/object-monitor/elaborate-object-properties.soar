# object-monitor.object-info.properties <props>
#   Contains information about the properties of the object

# Elaborate the properties identifier
sp {top-state*perception-monitor*object-monitor*object-info*elaborate*differences
   (state <s> ^superstate nil
              ^perception-monitor.object-monitor.{ << object-info new-object-info >> } <obj-info>)
-->
   (<s> ^properties <props>)
}

# Copy properties from input-link-obj
sp {top-state*perception-monitor*object-monitor*elaborate*properties*from*input-link-obj
   (state <s> ^superstate nil
              ^perception-monitor.object-monitor.object-info <info>)
   (<info> ^input-link-obj <il-obj>
           ^properties <props>)
   (<il-obj> ^classifications.<prop> <pred>)
-->
   (<props> ^<prop> <pred>)
}
