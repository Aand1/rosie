sp {match-world-object*prefer*extract*over*match
   (state <s> ^name match-world-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name extract-distances)
   (<o2> ^name match-candidate)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {match-world-object*prefer*all*over*complete
   (state <s> ^name match-world-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << extract-distances match-candidate >>)
   (<o2> ^name complete-match-world-object)
-->
   (<s> ^operator <o1> > <o2>)
}