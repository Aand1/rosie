# The smem-obj we got already has a perception object associated with it, so remove
sp {attend-to-new-object*propose*reject-smem-match
   (state <s> ^name attend-to-new-object
              ^source-type perception-obj
              ^smem-obj.handle <handle>
              ^top-state.perception-monitor.object-monitor.object-info <obj-info>)
   (<obj-info> ^object-handle <handle>
               ^status.visible true)
-->
   (<s> ^operator <o> +)
   (<o> ^name reject-smem-match)
}

# Remove the object
sp {attend-to-new-object*apply*reject-smem-match
   (state <s> ^name attend-to-new-object
              ^operator.name reject-smem-match
              ^smem-obj <obj>)
-->
   (<s> ^smem-obj <obj> -
        ^no-smem-object-retrieved true)
}