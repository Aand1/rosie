sp {topstate*elaborate*smem-query-cue*on
   (state <s> ^topstate <s>)
-->
   (<s> ^smem-query-cue.name on1)
}

sp {topstate*elaborate*world*predicates*on
   (state <s> ^topstate <s>
              ^retrieved-lti-cue.retrieved-lti <on>
              ^world.predicates <preds>)
   (<on> ^name on1)
-->
   (<preds> ^predicate <pred>)
   (<pred> ^name on1
           ^predicate-info <on>)
}
           

