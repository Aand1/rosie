sp {top-state*elaborate*smem-query-cue*on
   (state <s> ^top-state <s>)
-->
   (<s> ^smem-query-cue.name on1)
#   (<s> ^smem-query-cue.name in1)
   (<s> ^smem-query-cue.name right-of1)
#   (<s> ^smem-query-cue.name left-of1)
}

sp {top-state*elaborate*world*predicates
   :o-support
   (state <s> ^top-state <s>
              ^world-usage external
              ^smem-query-cue <cue>
              ^retrieved-lti-cue <ret>
              ^world.predicates <preds>)
   (<ret> ^cue <cue>
          ^retrieved-lti <lti>)
   (<lti> ^name { <pred-name> << on1 in1 right-of1 left-of1 >>})
-->
   (<preds> ^predicate <p>)
   (<p> ^name <pred-name>
         ^predicate-info <lti>)
   (<s> ^smem-query-cue <cue> -
        ^retrieved-lti-cue <ret> -)
}
           

