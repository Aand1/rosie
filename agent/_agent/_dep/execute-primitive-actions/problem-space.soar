sp {execute-primitive-action*elaborate*problem-space
   (state <s> ^superstate.operator.problem-space execute-primitive-action)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name execute-primitive-action
         ^action-type primitive
         ^action <action>
         ^world <world>
         ^learning <learn>
         ^perception <perc>)
   (<action> ^type execute 
             ^propose-actions false)
   (<world> ^copy-type shallow)
   (<perc> ^attend-to-all-perception yes)
}
