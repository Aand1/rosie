sp {topstate*propose*init-agent
   (state <s> -^name
   	      ^superstate nil)
-->
   (<s> ^operator <op> + >)
   (<op> ^name init-agent)
}

sp {topstate*apply*init-agent
	(state <s> ^operator <op>)
    (<op> ^name init-agent)
-->
	(<s> ^topstate <s>
      	 ^name storage-agent
         ^put-down-locations <l>)
 	(<l> ^location <l1>
       	 ^location <l2>
         ^location <l3>
         ^location <l4>)
  	(<l1> ^x -0.1
          ^y -0.1)
   	(<l2> ^x -0.1
          ^y -0.2)
   	(<l3> ^x -0.2
          ^y -0.1)
   	(<l4> ^x -0.2
          ^y -0.2)
}

sp {topstate*elaborate*open-locations
	(state <s> ^superstate nil
               ^name storage-agent
               ^put-down-locations <list>)
 	(<list> ^location <loc>)
  	(<loc> -^used)
-->
	(<s> ^open-location <loc>)
}