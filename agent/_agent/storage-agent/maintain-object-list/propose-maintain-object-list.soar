# If the list doesn't already exist...
sp {topstate*propose*init-object-list
   (state <s> ^superstate nil
              ^name storage-agent
              ^svs <svs>
             -^to-store-list)
-->
   (<s> ^operator <op> + =)
   (<op> ^name init-object-list)
}

# If the list needs updating... Only update from top state
sp {propose*update-object-list
   (state <s> ^superstate nil
              ^name storage-agent
	     	  ^to-store-list <t>)
   (<t> ^update true)
-->
   (<s> ^operator <op> + <)
   (<op> ^name update-object-list)
}

sp {topstate*propose*request-update
	(state <s> ^superstate nil
               ^name storage-agent
               ^to-store-list <t>)
 	(<t> -^update)
-->
   (<op> ^name request-update)  
   (<s> ^operator <op> + = <)
}

sp {topstate*apply*request-update
	(state <s> ^superstate nil
               ^name storage-agent
               ^operator.name request-update
               ^to-store-list <list>)
-->
	(<list> ^update true)
}