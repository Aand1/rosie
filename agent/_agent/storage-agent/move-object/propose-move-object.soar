sp {move-object*propose*request-pick-up
	(state <s> ^superstate nil
			   ^name storage-agent
      		   ^selected-object <id>
              -^pick-up <id>
              -^request)
-->
	(<s> ^operator <op> + =)
 	(<op> ^name request-pick-up
          ^id <id>)
}

sp {move-object*propose*select-drop-off-location
	(state <s> ^superstate nil
               ^name storage-agent
               ^selected-object <id>
               ^pick-up <id>
              -^request
               ^put-down-locations <l>
              -^selected-location <loc>)
 	(<l> ^location <l1>)
  	(<l1> -^used)
-->
	(<s> ^operator <op> + =)
 	(<op> ^name select-drop-off-location
          ^location <l1>)
}

sp {move-object*propose*request-drop-off
	(state <s> ^superstate nil
			   ^name storage-agent
      		   ^selected-object <id>
           	   ^selected-location <loc>
               ^pick-up <id>
              -^request
              -^drop-off <id>
               ^io.output-link <ol>)
  	(<ol> -^plan
          -^postprocess
          -^execute)
-->
	(<s> ^operator <op> + =)
 	(<op> ^name request-drop-off
          ^id <id>
          ^location <loc>)
}

sp {move-object*propose*mark-stored
	(state <s> ^superstate nil
			   ^name storage-agent
      		   ^selected-object <id>
           	   ^selected-location <loc>
               ^pick-up <id>
               ^drop-off <id>
              -^stored-object <id>
              -^request)
-->	
	(<s> ^operator <op> +)
 	(<op> ^name mark-stored
          ^id <id>
          ^location <loc>)
}
