sp {move-object*apply*request-pick-up
	(state <s> ^superstate nil
			   ^name storage-agent
               ^operator <op>)
 	(<op> ^name request-pick-up
          ^id <id>)
-->
	(<s> ^request <r>
      	 ^pick-up <id>)
 	(<r> ^target <t>
         ^speed 1
         ^type grasp
         ^time-limit 3
         ^step-size 0.4
         ^hard-time-limit true)
  	(<t> ^id <id>)
}

sp {move-object*apply*request-drop-off
	(state <s> ^superstate nil
			   ^name storage-agent
               ^operator <op>)
 	(<op> ^name request-drop-off
          ^id <id>)
-->
	(<s> ^request <r>
      	 ^drop-off <id>)
 	(<r> ^target <t>
         ^speed 1
         ^type drop
         ^time-limit 3
         ^step-size 0.4
         ^hard-time-limit true)
  	(<t> ^x -0.2
         ^y -0.2
         ^z 0.0)
}

sp {move-object*apply*mark-stored
	(state <s> ^superstate nil
			   ^name storage-agent
               ^operator <op>)
	(<op> ^name mark-stored
          ^id <id>)
-->	
	(<s> ^stored-object <id>
     	 ^selected-object <id> -
         ^pick-up <id> -
         ^put-down <id> -)
}