sp {decide-pick-up-params*propose*init
	(state <s> ^name decide-pick-up-params
              -^param)
-->
	(<s> ^operator.name init +)
}

sp {decide-pick-up-params*apply*init
	(state <s> ^name decide-pick-up-params
               ^operator.name init)
-->
	(<s> ^param <p>)
}

sp {decide-pick-up-params*propose*set-step-size
	(state <s> ^name decide-pick-up-params
               ^param <p>)
 	(<p> -^algorithm)
-->
	(<s> ^operator.name set-step-size + =)
}

sp {decide-pick-up-params*apply*set-step-size-small
	(state <s> ^name decide-pick-up-params
               ^topstate.svs.spatial-scene <sc>
               ^param <p>)
 	(<s> ^operator.name set-step-size)
  	(<sc> ^child.main-color blue)
-->
	(<p> ^algorithm ara)
}

sp {decide-pick-up-params*apply*set-step-size-medium
	(state <s> ^name decide-pick-up-params
               ^topstate.svs.spatial-scene <sc>
               ^param <p>)
 	(<s> ^operator.name set-step-size)
  	(<sc> -^child.main-color blue
          ^child.main-color green)
-->
	(<p> ^algorithm rrt)
}

sp {decide-pick-up-params*apply*set-step-size-large
	(state <s> ^name decide-pick-up-params
               ^topstate.svs.spatial-scene <sc>
               ^param <p>)
 	(<s> ^operator.name set-step-size)
  	(<sc> -^child.main-color blue
          -^child.main-color green)
-->
	(<p> ^algorithm rrt)
}

sp {decide-pick-up-params*propose*finish
	(state <s> ^name decide-pick-up-params
               ^param <p>)
 	(<p> ^algorithm)
-->
	(<s> ^operator.name finish +)
}

sp {decide-pick-up-params*apply*finish
	(state <s> ^name decide-pick-up-params
               ^superstate <ss>
               ^operator.name finish
               ^param <p>)
-->
	(<ss> ^params <p>)
}