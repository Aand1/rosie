sp {evaluate-stability*propose*mark-stable
   (state <s> ^name evaluate-stability
              ^obj-is-stable true
              ^wm-object <wm-obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name mark-stable
        ^category perception
        ^wm-object <wm-obj>)
}

sp {evaluate-stability*apply*mark-stable*mark*unidentified
   (state <s> ^name evaluate-stability
              ^operator <o>)
   (<o> ^name mark-stable
        ^wm-object <wm-obj>)
   (<wm-obj> ^status uncertain)
-->
   (<wm-obj> ^status uncertain -
             ^status unidentified)
}

sp {evaluate-stability*apply*mark-stable*cleanup*object
   (state <s> ^name evaluate-stability
              ^operator <o>)
   (<o> ^name mark-stable
        ^wm-object <wm-obj>)
   (<wm-obj> ^number-checks <num>
             ^stability-check-time <t>
             ^stale-candidates <c>)
-->
   (<wm-obj> ^number-checks <num> -
             ^stability-check-time <t> -)
}