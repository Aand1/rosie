sp {evaluate-grown-object*propose*mark-for-copying
   (state <s> ^name evaluate-grown-object
              ^monitor-obj <obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name mark-for-copying
        ^category perception
        ^monitor-obj <obj>)
}

sp {evaluate-grown-object*apply*mark-for-copying
   (state <s> ^name evaluate-grown-object
              ^operator <o>)
   (<o> ^name mark-for-copying
        ^monitor-obj <obj>)
-->
   (<obj> ^mark-for-copying true)
}