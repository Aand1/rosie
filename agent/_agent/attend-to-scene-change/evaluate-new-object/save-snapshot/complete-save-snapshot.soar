sp {save-snapshot*propose*complete-save-snapshot
   (state <s> ^name save-snapshot
              ^snapshot-saved << true false >>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name complete-save-snapshot
        ^category perception)
}

sp {save-snapshot*apply*complete-save-snapshot*copy*snapshot-saved
   (state <s> ^name save-snapshot
              ^operator.name complete-save-snapshot
              ^snapshot-saved <bool>
              ^superstate <ss>)
-->
   (<ss> ^snapshot-saved <bool>)
}