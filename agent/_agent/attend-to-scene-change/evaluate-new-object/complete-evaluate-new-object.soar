sp {evaluate-new-object*propose*complete-evaluate-new-object
   (state <s> ^name evaluate-new-object
              ^wm-object <wm-obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-evaluate-new-object
        ^category perception
        ^wm-object <wm-obj>)
}

sp {evaluate-new-object*apply*complete-evaluate-new-object
   (state <s> ^name evaluate-new-object
              ^operator <o>)
   (<o> ^name complete-evaluate-new-object
        ^wm-object <obj>)
   (<obj> ^status unidentified)
-->
   (<obj> ^status unidentified -
          ^status certain)
}

