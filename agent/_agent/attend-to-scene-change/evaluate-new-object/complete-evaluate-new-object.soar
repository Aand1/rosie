sp {evaluate-new-object*apply*complete-evaluate-new-object*write*message
   (state <s> ^name evaluate-new-object
              ^operator <o>
              ^topstate.agent-params.debug-perception true)
   (<o> ^name complete-evaluate-new-object
        ^wm-object.id <id>)
-->
   (write |Creating new object | <id>)
}
