sp {match-stale-object*propose*distance-command
   (state <s> ^name match-stale-object
              ^svs-object.id <ida>
              ^closest-command.result <idb>
             -^distance-command.result)
-->
   (<s> ^operator <o> +)
   (<o> ^name distance-command
        ^category perception
        ^object-a <ida>
        ^object-b <idb>
        ^distance-type centroid_distance)
}