sp {match-stale-object*elaborate*svs-object
   (state <s> ^name match-stale-object
              ^superstate.operator.svs-object <svs-obj>)
-->
   (<s> ^svs-object <svs-obj>)
}

sp {match-stale-object*elaborate*wm-object
   (state <s> ^name match-stale-object
              ^superstate.operator.wm-object <wm-obj>)
-->
   (<s> ^wm-object <wm-obj>)
}

sp {match-stale-object*elaborate*candidates
   (state <s> ^name match-stale-object
              ^wm-object.stale-candidates.result <svs-id>
              ^intersect-command.result <svs-id>
              ^topstate.object <obj>)
   (<obj> ^svs-id <svs-id>)
-->
   (<s> ^stale-candidate.object <obj>)
}

