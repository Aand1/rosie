sp {evaluate-shrunken-object*propose*complete-evaluate-shrunken-object
   (state <s> ^name evaluate-shrunken-object
              ^clear-object-view <bool>)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-evaluate-shrunken-object
        ^category perception
        ^clear-object-view <bool>)
}

sp {evaluate-shrunken-object*apply*complete-evaluate-shrunken-object*occluded
   (state <s> ^name evaluate-shrunken-object
              ^operator <o>
              ^wm-object <obj>)
   (<o> ^name complete-evaluate-shrunken-object
        ^clear-object-view false)
-->
   (<obj> ^occluded true)
}

# If the object is not occluded, then a snapshot should have been taken which will retract this operator