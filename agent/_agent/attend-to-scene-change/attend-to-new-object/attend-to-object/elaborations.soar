sp {attend-to-object*elaborate*attended-object
   (state <s> ^name attend-to-object
              ^superstate.operator.object <obj>)
-->
   (<s> ^attended-object <obj>)
}

sp {attend-to-object*elaborate*object-pair
   (state <s> ^name attend-to-object
              ^attended-object <top-obj>
              ^topstate.svs.spatial-scene.child <svs-obj>)
   (<top-obj> ^id <id>)
   (<svs-obj> ^id-int <id>)
-->
   (<s> ^object-pair <op>)
   (<op> ^svs <svs-obj>
         ^ts <top-obj>)
}

sp {attend-to-object*elaborate*property-info
   (state <s> ^name attend-to-object)
-->
   (<s> ^property-info <pi>)
   (<pi> ^property <p1> <p2> <p3>)
   (<p1> ^name color
         ^confidence color-conf)
   (<p2> ^name shape
         ^confidence shape-conf)
   (<p3> ^name size
         ^confidence size-conf)
}