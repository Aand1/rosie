sp {attend-to-object*propose*complete-attend-to-object
   (state <s> ^name attend-to-object
              ^attended-object <obj>)
-->
   (<s> ^operator <o> + <)
   (<o> ^name complete-attend-to-object
        ^category perception
        ^object <obj>)
}

sp {attend-to-object*apply*complete-attend-to-object
   (state <s> ^name attend-to-object
              ^operator <o>
              ^superstate <ss>)
   (<o> ^name complete-attend-to-object
        ^object <obj>)
-->
   (<ss> ^attended-to-object <obj>)
}