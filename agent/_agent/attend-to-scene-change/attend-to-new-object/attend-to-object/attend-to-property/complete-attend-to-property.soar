sp {attend-to-property*propose*complete-attend-to-property
   (state <s> ^name attend-to-property)
-->
   (<s> ^operator <o> + <)
   (<o> ^name complete-attend-to-property)
}

sp {attend-to-property*apply*complete-attend-to-property
   (state <s> ^name attend-to-property
              ^operator.name complete-attend-to-property
              ^svs-property <svs-prop>
              ^superstate <ss>)
-->
   (<ss> ^attended-to-property <svs-prop>)
}