# Will find all the stale objects that intersect the new one, as possible stale objects to match

sp {attend-to-new-object*propose*intersect-command
   (state <s> ^name attend-to-new-object
              ^svs-object.id <id>
              -^intersect-command.result)
-->
   (<s> ^operator <o> +)
   (<o> ^name intersect-command
        ^category perception
        ^object-id <id>
        ^candidate-set <stale-objects>)
   (<stale-objects> ^type has-property
                    ^a.type all_nodes
                    ^property-name stale
                    ^property-value true)
}  