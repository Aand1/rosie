sp {attend-to-new-object*propose*complete-attend-to-new-object
   (state <s> ^name attend-to-new-object
              ^new-object <new-obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-attend-to-new-object
        ^category perception
        ^object <new-obj>)
}

sp {attend-to-new-object*apply*complete-attend-to-new-object
   (state <s> ^name attend-to-new-object
              ^operator <o>
              ^topstate <top>)
   (<o> ^name complete-attend-to-new-object
        ^object <new-obj>)
-->
   (<top> ^object <new-obj>)
}