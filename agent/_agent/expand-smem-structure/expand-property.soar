sp {expand-smem-structure*property*retrieve*lti
   (state <s> ^name expand-smem-structure
              ^smem-structure <struct>)
   (<struct> ^type property
             ^id <pred>)
   (<pred> -^name)
-->
   (<s> ^retrieve-lti <pred>)
}

sp {expand-smem-structure*property*report-failure*no*name
   (state <s> ^name expand-smem-structure
              ^smem-structure <struct>
              ^retrieved-lti <pred>)
   (<struct> ^type property
             ^id <pred>)
   (<pred> -^name)
-->
   (<s> ^operator <o> + !)
   (<o> ^name report-failure
        ^failure-type bad-smem-structure
        ^smem-structure <pred>
        ^structure-type property)
}