sp {expand-smem-structure*propose*complete-expand-smem-structure
   (state <s> ^name expand-smem-structure)
-->
   (<s> ^operator <o> + <)
   (<o> ^name complete-expand-smem-structure)
}

sp {expand-smem-structure*apply*complete-expand-smem-structure
   (state <s> ^name expand-smem-structure
              ^operator.name complete-expand-smem-structure
              ^superstate.operator.smem-structure <struct>)
-->
   (<struct> ^expanded true)
}