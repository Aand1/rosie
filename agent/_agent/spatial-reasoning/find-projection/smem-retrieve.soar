sp {find-projection*elaborate*retrieve-lti*predicate
   (state <s> ^name find-projection
              ^predicate <pred>
             -^retrieved-lti <pred>)
   (<pred> -^id)
-->
   (<s> ^retrieve-lti <pred>)
}

sp {find-projection*elaborate*retrieve-lti*predicate*relation
   (state <s> ^name find-projection
              ^predicate.relation <rel>
             -^retrieved-lti <rel>)
   (<rel> -^type)
-->
   (<s> ^retrieve-lti <rel>)
}