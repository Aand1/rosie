sp {execute-action*elaborate*smem-query*action-id
   (state <s> ^name execute-action
              ^action-context.action-handle <handle>)
-->
   (<s> ^smem-query <q>)
   (<q> ^cue <cue>
        ^depth 8)
   (<cue> ^handle <handle>)
}

sp {execute-action*copy*smem-query*result*action-id
   (state <s> ^name execute-action
              ^smem-query <q>
              ^action-context.action-handle <handle>)
   (<q> ^cue.handle <handle>
        ^result <res>)
-->
   (<s> ^action-id <res>)
}

sp {execute-action*elaborate*smem-retrieve*subaction
   :o-support
   (state <s> ^name execute-action
              ^action-id.procedural.subaction <sub>)
   (<sub> -^op_name)
-->
   (<s> ^smem-retrieve <ret>)
   (<ret> ^identifier <sub>
          ^depth 4)
}