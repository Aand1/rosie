sp {execute-primitive-action*elaborate*problem-space
   (state <s> ^superstate.name execute-action
              ^superstate.operator.action-type primitive)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name execute-primitive-action
         ^action-type primitive
         ^action <action>
         ^world <world>
         ^learning <learn>
         ^perception <perc>)
   (<action> ^type execute 
             ^propose-action-operators false)
   (<world> ^copy-type shallow)
   (<perc> ^attend-to-all-perception yes)
}
