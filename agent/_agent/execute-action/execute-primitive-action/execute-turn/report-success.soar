sp {execute-turn*propose*report-success
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle turn1
              ^current-action.turn-command.status success)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {execute-turn*apply*report-success*add*status
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle turn1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}

sp {execute-turn*apply*report-success*remove*command
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle turn1
              ^operator.name report-success
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^turn-command <cmd>)
   (<out> ^<cmd-name> <cmd>)
-->
   (<a> ^turn-command <cmd> -)
   (<out> ^<cmd-name> <cmd> -)
}
              