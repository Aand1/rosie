sp {execute-put-down1*prefer*attend-to-scene-change*over*others
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle put-down1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name << report-success report-failure do-pick-up >>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-put-down1*prefer*find-projection*over*send-put-down
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle put-down1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name find-projection)
   (<o2> ^name send-put-down-command)
-->
   (<s> ^operator <o1> > <o2>)
}
