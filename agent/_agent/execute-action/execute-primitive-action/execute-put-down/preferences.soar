sp {op_put-down1*prefer*attend-to-scene*over*others
   (state <s> ^name op_put-down1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name report-success)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_put-down1*prefer*drive*over*put-down
   (state <s> ^name op_put-down1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name send-drive-to-xy-command)
   (<o2> ^name send-put-down-command)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_put-down1*prefer*find-project*over*commands
   (state <s> ^name op_put-down1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name find-projection)
   (<o2> ^name << send-put-down-command send-drive-to-xy-command >>)
-->
   (<s> ^operator <o1> > <o2>)
}
