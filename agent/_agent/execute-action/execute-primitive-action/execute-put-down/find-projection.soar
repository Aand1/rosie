sp {execute-put-down1*propose*find-projection
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle put-down1
             -^current-action.projection-result
              ^held-object <held>
              ^destination <dest>
              ^predicate-handle <pred-handle>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name find-projection
        ^predicate-handle <pred-handle>
        ^arg-list <args>)
   (<args> ^1 <held> ^2 <dest>)
}

sp {execute-put-down1*apply*find-projection*copy*result
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle put-down1
              ^operator.name find-projection
              ^projection-result <res>
              ^current-action <a>)
-->
   (<a> ^projection-result <res>)
}
