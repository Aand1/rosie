sp {execute-put-down1*propose*report-success
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle put-down1
              ^current-action.put-down-command.status complete
              ^held-object.predicates.visibility visible1
              ^held-object <obj1>
              ^destination <obj2>
              ^predicate-handle <pred-handle>
              ^world.predicates.predicate <pred>)
   (<pred> ^handle <pred-handle>
           ^instance <i>)
   (<i> ^1 <obj1> ^2 <obj2>)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {execute-put-down1*apply*report-success
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle put-down1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}


sp {execute-put-down1*apply*report-success*remove*put-down-command
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle put-down1
              ^operator.name report-success
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^put-down-command <cmd>)
   (<out> ^put-down <cmd>)
-->
   (<a> ^put-down-command <cmd> -)
   (<out> ^put-down <cmd> -)
}
