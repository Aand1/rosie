sp {op_put-down1*propose*report-success
   (state <s> ^name op_put-down1
              ^current-action.put-down-command.status success)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {op_put-down1*apply*report-success*mark*finished
   (state <s> ^name op_put-down1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}

sp {op_put-down1*apply*report-success*remove*put-down-command
   (state <s> ^name op_put-down1
              ^operator.name report-success
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^{ <name> << drive-command put-down-command >> } <cmd>)
   (<out> ^do-control-law <cmd>)
-->
   (<a> ^<name> <cmd> -)
   (<out> ^do-control-law <cmd> -)
}

sp {op_put-down*apply*report-success*add*in*predicate
   (state <s> ^name op_put-down1
              ^operator.name report-success
              ^held-object <obj>
              ^top-state.world.predicates.predicate <in-pred>
              ^top-state.world.robot.current-location <loc>)
   (<in-pred> ^handle in1)
   -{(<in-pred> ^instance <cur-i>)
     (<cur-i> ^1 <obj> ^2 <loc>)}
-->
   (<in-pred> ^instance <i>)
   (<i> ^1 <obj> ^2 <loc>)
}


