sp {execute-pick-up1*propose*remove-moving-flag
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
              ^current-action.pick-up-command.status failure
              ^target-object.predicates.arm-status moving)
-->
   (<s> ^operator <o> + >)
   (<o> ^name remove-moving-flag)
}

sp {execute-pick-up1*apply*remove-moving-flag
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
              ^operator.name remove-moving-flag
              ^target-object.predicates <preds>)
   (<preds> ^arm-status moving)
-->
   (<preds> ^arm-status moving -)
}
        
