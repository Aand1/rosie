sp {execute-pick-up*elaborate*svs-filter*distance*to*object
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
               ^target-object.svs-obj.id <obj-id>
               ^top-state.svs.command <cmd>)
-->
    (<cmd> ^extract <dist>)
    (<dist> ^type distance
            ^distance_type centroid
            ^a <robot>
            ^b <obj>)
    (<robot> ^type node
             ^id robot_pos)
    (<obj> ^type node
           ^id <obj-id>)
    (<s> ^distance-filter <dist>)
}

sp {execute-pick-up*elaborate*distance-to-object*from*filter
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
               ^distance-filter.result.record.value <d>)
-->
    (<s> ^distance-to-object <d>)
}
    
                    