sp {execute-pick-up1*propose*send-drive-to-xy-command
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
               ^target-object <obj>
               ^object-position <pos>
               ^distance-to-object >= .1
#              ^world.robot.arm.action wait
              -^world.robot.arm.state internal
               -^executing-command true)
-->
   (<s> ^operator <o> + >)
   (<o> ^name send-drive-to-xy-command
        ^object <obj>)
}

sp {execute-pick-up1*apply*send-drive-to-xy-command*send
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle pick-up1
              ^operator <o>
              ^io.output-link <ol>
              ^current-action <a>
              ^object-position <pos>)
   (<o> ^name send-drive-to-xy-command
        ^object.handle <obj-handle>)
   (<pos> ^x <x> ^y <y>)
-->
    (<ol> ^do-control-law <law>)
    (<law> ^name drive-xy
           ^parameters <params>
           ^termination-condition.name stabilized)
    (<params> ^x <x> ^y <y>)
    (<a> ^drive-to-xy-command <law>)
}

sp {execute-pick-up1*propose*clear-drive-to-xy-command
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
               ^current-action.drive-to-xy-command.status success)
-->
    (<s> ^operator <o> + >)
    (<o> ^name clear-drive-to-xy-command)
}

sp {execute-pick-up1*apply*clear-drive-to-xy-command
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
               ^operator.name clear-drive-to-xy-command
               ^current-action <a>
               ^io.output-link <out>)
   (<a> ^drive-to-xy-command <cmd>)
   (<out> ^do-control-law <cmd>)
-->
   (<a> ^drive-to-xy-command <cmd> -)
   (<out> ^do-control-law <cmd> -)
}
