sp {execute-pick-up1*prefer*attend-to-scene*over*others
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name << report-success report-failure send-reset-command >>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-pick-up1*prefer*report-failure*over*send-reset-command
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name report-failure)
   (<o2> ^name send-reset-command)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-pick-up1*prefer*remove-moving-flag*over*others
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle pick-up1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name remove-moving-flag)
   (<o2> ^name << send-reset-command report-failure attend-to-scene-change >> )
-->
   (<s> ^operator <o1> > <o2>)
}

