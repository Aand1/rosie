sp {execute-pick-up1*prefer*attend-to-arm*over*report-success
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle pick-up1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name report-success)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-pick-up1*prefer*attend-to-scene*over*failure*operators
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle pick-up1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name << report-failure send-reset-command >>)
-->
   (<s> ^operator <o1> > <o2>)
}


