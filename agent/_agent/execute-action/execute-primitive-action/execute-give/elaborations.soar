sp {op_give1*elaborate*held-object*superoperator
   (state <s> ^name op_give1
              ^action-operator.arg1.id <obj>)
-->
   (<s> ^held-object <obj>)
}

sp {op_give1*elaborate*person*superoperator
   (state <s> ^name op_give1
              ^action-operator.arg2.{2} <person>)
-->
   (<s> ^person <person>)
}

sp {op_give*elaborate*person-info
   (state <s> ^problem-space.name execute-primitive-action
              ^person <person>
              ^top-state.perception-monitor.object-monitor.object-info <obj-info>)
   (<obj-info> ^wm-obj <person>)
-->
   (<s> ^person-info <obj-info>)
}

sp {op_give*elaborate*svs-filter*node_position*person
   (state <s> ^name op_give1
               ^person.svs-obj.id <person-id>
               ^top-state.svs.command <cmd>)
-->
    (<cmd> ^extract <pos>)
    (<pos> ^type node_position
            ^a <person>)
    (<person> ^type node
              ^id <person-id>)
    (<s> ^person-position-filter <pos>)
}

sp {op_give*elaborate*person-position*from*filter
   (state <s> ^name op_give1
               ^person-position-filter.result.record.value <p>)
-->
    (<s> ^person-position <p>)
}

          
