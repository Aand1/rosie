sp {execute-give1*propose*send-drive-to-xy-command
   (state <s> ^problem-space.name execute-primitive-action
               ^action-handle give1
               ^person-position <pos>
              -^current-action.drive-command)
   (<pos> ^x <x> ^y <y>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name send-drive-to-xy-command
        ^position <pos>)
}

sp {execute-give1*apply*send-drive-to-xy-command*send
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle give1
              ^operator <o>
              ^io.output-link <out>
              ^current-action <a>)
   (<o> ^name send-drive-to-xy-command
        ^position <pos>)
   (<pos> ^x <x> ^y <y>)
-->
    (<out> ^do-control-law <law>)
    (<law> ^name drive-xy
           ^parameters <params>
           ^termination-condition.name stabilized)
    (<params> ^x <x> ^y <y>)
    (<a> ^drive-command <law>)
}