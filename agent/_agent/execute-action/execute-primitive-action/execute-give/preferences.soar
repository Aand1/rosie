sp {op_give1*prefer*attend-to-scene*over*others
   (state <s> ^name op_give1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name report-success)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_give1*prefer*drive*over*give
   (state <s> ^name op_give1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name send-drive-to-xy-command)
   (<o2> ^name send-put-down-command)
-->
   (<s> ^operator <o1> > <o2>)
}

