}

sp {execute-orient1*elaborate*smem-query-cue*direction
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle orient1
			 ^direction-handle <dir-handle>)
-->
   (<s> ^smem-query-cue <cue>)
	(<cue> ^handle <dir-handle>)
}

sp {execute-orient1*elaborate*direction-id*from*retrieval
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle orient1
              ^direction-handle <dir-handle>
              ^smem-query-cue <cue>
              ^retrieved-lti-cue <ret>)
   (<ret> ^cue <cue>
          ^retrieved-lti <id>)
	(<cue> ^handle <dir-handle>)
-->
   (<s> ^direction-id <id>)
}
