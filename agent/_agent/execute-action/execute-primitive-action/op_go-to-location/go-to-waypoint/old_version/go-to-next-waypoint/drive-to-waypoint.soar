sp {go-to-next-waypoint*propose*drive-to-waypoint*bottom*level
   (state <s> ^name go-to-next-waypoint
              -^current-location.sub-map
              ^destination <dest>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name initiate-drive-to-waypoint
        ^waypoint <dest>)
}

sp {go-to-next-waypoint*elaborate*drive-to-waypoint*wall-side*from*edge
   (state <s> ^name go-to-next-waypoint
              ^operator <o> +
              ^current-location.edge <e>)
   (<o> ^name initiate-drive-to-waypoint
        ^waypoint <dest>)
   (<e> ^end <dest>
        ^wall-side <side>)
-->
   (<o> ^wall-side <side>)
}

sp {go-to-next-waypoint*elaborate*drive-to-waypoint*wall-side*default
   (state <s> ^name go-to-next-waypoint
              ^operator <o> +)
   (<o> ^name initiate-drive-to-waypoint
       -^wall-side <> 1)
-->
   (<o> ^wall-side 1)
}

sp {go-to-next-waypoint*elaborate*drive-to-waypoint*intermediate-turn*none
   (state <s> ^name go-to-next-waypoint
              ^operator <o> +
              ^current-location.edge <e>)
   (<o> ^name initiate-drive-to-waypoint
        ^waypoint <dest>)
   (<e> ^end <dest>
        ^intermediate-turn <yaw>)
-->
   (<o> ^intermediate-turn <yaw>)
}

sp {go-to-next-waypoint*elaborate*drive-to-waypoint*intermediate-turn*from*edge
   (state <s> ^name go-to-next-waypoint
              ^operator <o> +)
   (<o> ^name initiate-drive-to-waypoint
       -^intermediate-turn <> none)
-->
   (<o> ^intermediate-turn none)
}

sp {go-to-next-waypoint*apply*initiate-drive-to-waypoint
   (state <s> ^name go-to-next-waypoint
              ^operator <o>
              ^top-state.interaction.stack.segment <seg>)
   (<o> ^name initiate-drive-to-waypoint
        ^wall-side <side>
        ^waypoint <wp>
        ^intermediate-turn <yaw>)
-->
   (<seg> ^current-action <ca>)
   (<ca> ^name drive-to-waypoint1
         ^action-operator execute-drive-to-waypoint1
         ^arguments <args>)
   (<args> ^waypoint <wp>
           ^side <side>
           ^intermediate-turn <yaw>)
}


