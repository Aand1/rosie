sp {go-to-next-waypoint*prefer*advance-position*over*attend-to-scene-change
   (state <s> ^name go-to-next-waypoint
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name advance-position)
   (<o2> ^name attend-to-scene-change)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {go-to-next-waypoint*prefer*attend*over*report-success
   (state <s> ^name go-to-next-waypoint
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name report-success)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {go-to-next-waypoint*prefer*attend*over*face-waypoint
   (state <s> ^name go-to-next-waypoint
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name face-waypoint)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {go-to-next-waypoint*reject*attend-to-new-waypoint*if*driving
   (state <s> ^name go-to-next-waypoint
              ^operator <o> +
              ^current-action.drive-command <cmd>)
   (<o> ^name attend-to-scene-change
        ^change.type changed-waypoint)
   (<cmd> -^status)
-->
   (<s> ^operator <o> -)
}