sp {op_go-to-location*apply*report-success
   (state <s> ^name op_go-to-location1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}

sp {op_go*apply*report-success*remove*command
   (state <s> ^name op_go-to-location1
              ^operator.name report-success
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^go-command <cmd>)
   (<out> ^<cmd-name> <cmd>)
-->
   (<a> ^go-command <cmd> -)
   (<out> ^<cmd-name> <cmd> -)
}
              
