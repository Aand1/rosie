sp {op_go-to-location1*propose*report-failure*unknown-location
   (state <s> ^name op_go-to-location1
              ^desired-location <loc>
             -^desired-waypoint)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-failure
        ^failure-type unknown-location
        ^location <loc>)
}

sp {op_go-to-location1*apply*report-failure*unknown-location
   (state <s> ^name op_go-to-location1
              ^operator <o>
              ^top-state.interaction.status <status>)
   (<o> ^name report-failure
        ^failure-type unknown-location
        ^location <loc>)
-->
   (<status> ^failure <f>)
   (<f> ^type unknown-location
        ^location <loc>)
}
   
