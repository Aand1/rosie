sp {execute-go-to-location*elaborate*desired-location
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle go-to-location1
              ^execution-operator.arg2.{2} <loc>)
   (<loc> -^predicates.spatial-shape end1
          -^source)
-->
   (<s> ^desired-location <loc>)
}

sp {execute-go-to-location*elaborate*desired-location*source
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle go-to-location1
              ^execution-operator.arg2.{2}.source <loc>)
   (<loc> -^predicates.spatial-shape end1)
-->
   (<s> ^desired-location <loc>)
}

sp {execute-go-to-location*elaborate*desired-waypoint
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle go-to-location1
              ^desired-location.waypoint <wp-handle>
              ^top-state.maps.map.waypoint <wp>)
   (<wp> ^handle <wp-handle>)
-->
   (<s> ^desired-waypoint <wp>)
}

sp {execute-go-to-location*elaborate*until-clause*from*current-action
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle go-to-location1
              ^execution-operator.until-clause <until>)
-->
   (<s> ^until-clause <until>)
}
