sp {execute-remember*propose*connect-objects
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle remember1
              ^object1 <obj1>
              ^object2 <obj2>)
-->
   (<s> ^operator <o> + )
   (<o> ^name connect-objects
        ^object1 <obj1>
        ^object2 <obj2>)
}

sp {execute-remember*apply*connect-objects*mark*source
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle remember1
              ^operator <o>)
   (<o> ^name connect-objects
        ^object1 <obj1>
        ^object2 <obj2>)
-->
   (<obj2> ^source <obj1>)
}

sp {execute-remember*apply*connect-objects*mark*complete
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle remember1
              ^operator.name connect-objects
              ^current-action <a>)
-->
   (<a> ^status complete)
}

