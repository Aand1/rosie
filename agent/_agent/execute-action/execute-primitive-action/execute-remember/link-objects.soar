sp {execute-remember*propose*link-objects
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle remember1
              ^object1 <obj1>
              ^object2 <obj2>
              ^world.objects.object <obj2>)
-->
   (<s> ^operator <o> + )
   (<o> ^name link-objects
        ^object1 <obj1>
        ^object2 <obj2>)
}

sp {execute-remember*apply*link-objects*link
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle remember1
              ^operator <o>)
   (<o> ^name link-objects
        ^object1 <obj1>
        ^object2 <obj2>)
-->
   (<obj1> ^linked-to <obj2>)
}

sp {execute-remember*apply*link-objects*mark*complete
   (state <s> ^problem-space.name execute-primitive-action
              ^action-handle remember1
              ^operator.name link-objects
              ^current-action <a>)
-->
   (<a> ^status complete)
}