sp {execute-action*primitive*propose-action-operators
   (state <s> ^name execute-action
              ^current-action.execution-operator <ex-op>
             -^current-action.validated true
              ^problem-space.action <a>)
   (<ex-op> ^action-type primitive)
-->
   (<a> ^propose-action-operators true
        ^propose-driving-operators true)
   (<s> ^match-action.action-operator <ex-op>)
}

sp {execute-action*apply*primitive-action*mark*validated
   (state <s> ^name execute-action
              ^operator <o>
              ^current-action <a>)
   (<o> ^action-type primitive)
   (<a> -^validated true)
-->
   (<a> ^validated true)
}