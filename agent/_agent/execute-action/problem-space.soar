sp {execute-action*elaborate*problem-space
   (state <s> ^name execute-action)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action <action>
         ^learning <learn>
         ^world <world>)
   (<action> ^type execute)
   (<world> ^copy-type shallow)
}

sp {execute-action*elaborate*problem-space*action-type
   (state <s> ^name execute-action
              ^problem-space <ps>
              ^action-operator.action-type <type>)
-->
   (<ps> ^action-type <type>)
}

sp {execute-action*primitive*elaborate*problem-space*propose-actions*true
   (state <s> ^name execute-action
              ^problem-space <ps>
             -^validated-primitive-action true)
   (<ps> ^action-type primitive
         ^action <action>)
-->
   (<action> ^propose-actions true)
}