sp {generate-goal*apply*complete-generate-goal*single
   (state <s> ^operator <op>
   	      ^indexed-action <ia>)
   (<ia> -^desired)
   (<op> ^name elaborate-goal
   	 ^desired <d>
	 ^indexed-action <ia>)
-->
   (<ia> ^desired <d>)
   (<s> ^operator <op> -)
}

sp {generate-goal*delete*others
   (state <s> ^operator <op>
   	      ^operator {<op1> <> <op>} +)
   (<op> ^name elaborate-goal
   	 ^indexed-action <ia>)
   (<op1> ^name elaborate-goal	
   	  ^indexed-action <ia>)
-->
   (<s> ^operator <op1> -)
}


##############################################################################

sp {generate-goal*equal-preference*multiple*indexed-actions
   (state <s> ^operator <op1> +
   	      ^operator <op2> +)
   (<op1> ^name elaborate-goal
   	  ^indexed-action <ia1>)
   (<op2> ^name elaborate-goal
   	  ^indexed-action {<ia2> <> <ia1>})
-->
   (<s> ^operator <op1> = <op2>)
}


#################################################################################
sp {elaborate-goal*lookahead*required
   (state <s> ^look-ahead-operator <lop>
   	      ^operator <op> +)
   (<op> ^name elaborate-goal
   	 ^indexed-action <lop>)
-->
   (<s> ^operator <op> !)
}

sp {elaborate-goal*primitive*better
   (state <s> ^operator <op1> +
   	      ^operator <op2> +)
   (<op2> ^name elaborate-goal)
   (<op1> ^action-type << primitive composite >>)
-->
   (<s> ^operator <op2> > <op1>)
}

sp {elaborate-goal*smem-retrieve*better
   (state <s> ^operator <op1> +
   	      ^operator <op2> +)
   (<op1> ^name elaborate-goal)
   (<op2> ^name smem-retrieve-new)
-->
   (<s> ^operator <op2> > <op1>)
}
