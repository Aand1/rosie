# Prefer remove-start-of-execution-flag over anything else
sp {execute-action*prefer*remove-start-of-execution-flag*over*propose*action
   (state <s> ^name execute-action
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name remove-start-of-execution-flag)
   (<o2> -^name remove-start-of-execution-flag)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-action*prefer*report-failures*over*other
  (state <s> ^name execute-action
              ^operator <o1> +
              ^operator <o2> +)
  (<o1> ^name << report-failure-multiple-arguments >>)
  (<o2> ^name << remove-start-of-execution-flag validate-composite-action >> )
-->
  (<s> ^operator <o1> > <o2>)
}
