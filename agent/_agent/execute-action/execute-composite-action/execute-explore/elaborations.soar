sp {execute-explore*elaborate*action-handle*from*current-action
   (state <s> ^problem-space.name execute-composite-action
              ^action-operator.action-handle explore1)
-->
   (<s> ^action-handle explore1)
}

sp {execute-explore*elaborate*handle-manually*true
   (state <s> ^problem-space.name execute-composite-action
              ^action-handle explore1)
-->
   (<s> ^handle-manually true)
}

sp {execute-explore*elaborate*distance-map
   (state <s> ^problem-space.name execute-composite-action
              ^action-handle explore1)
-->
   (<s> ^distance-map <map>)
}

sp {execute-explore*elaborate*distance-map*distance*0
   (state <s> ^problem-space.name execute-composite-action
              ^action-handle explore1
              ^distance-map <map>
              ^world.robot.current-waypoint.handle <wp-handle>)
-->
   (<map> ^<wp-handle> 0)
}

sp {execute-explore*elaborate*distance-map*neighbors
   (state <s> ^problem-space.name execute-composite-action
              ^action-handle explore1
              ^distance-map <map>
              ^top-state.maps.map.waypoint <wp>)
   (<map> ^<wp-handle> <d>)
   (<wp> ^handle <wp-handle>
         ^next.handle <next-handle>)
   -{(<map> ^<next-handle> <= <d>)}
-->
   (<map> ^<next-handle> (+ <d> 1))
}


   
              