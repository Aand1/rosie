sp {execute-explore*propose*go-to-location
   (state <s> ^name op_explore1
              ^problem-space.action.type execute
              ^world.objects.object <loc>
              ^top-state.maps.map.waypoint <wp>
              ^distance-map.<wp-handle> { <dist> > 0 })
   (<loc> ^predicates.category location
          ^waypoint <wp-handle>
         -^visited true)
   (<wp> ^handle <wp-handle>)
-->
   (<s> ^operator <o> + = >)
   (<o> ^name go-to-location
        ^distance <dist>
        ^action-handle go-to-location1
        ^action-type primitive
        ^arg2 <arg2>)
   (<arg2> ^arg-type predicate
           ^arg-status desired
           ^handle to1
           ^2 <loc>)
}
