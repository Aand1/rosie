sp {execute-explore*propose*mark-visited
   (state <s> ^name op_explore1
              ^problem-space.action.type execute
              ^world.robot.current-location <loc>)
   (<loc> -^visited)
-->
   (<s> ^operator <o> +)
   (<o> ^name mark-visited
        ^location <loc>)
}

sp {execute-explore*apply*mark-visited
   (state <s> ^name op_explore1
              ^problem-space.action.type execute
              ^operator <o>)
   (<o> ^name mark-visited
        ^location <loc>)
-->
   (<loc> ^visited true)
}
              
              