sp {execute-explore*prefer*go-to-location*shorter*distance
   (state <s> ^problem-space.name execute-composite-action
              ^current-action.execution-operator.action-handle explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name go-to-location
         ^distance <d>)
   (<o2> ^name go-to-location
         ^distance > <d>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-explore*prefer*mark-visited*over*go-to-location
   (state <s> ^problem-space.name execute-composite-action
              ^current-action.execution-operator.action-handle explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name mark-visited)
   (<o2> ^name go-to-location)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-explore*prefer*ask-about-object*over*others
   (state <s> ^problem-space.name execute-composite-action
              ^current-action.execution-operator.action-handle explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name ask-about-object)
   (<o2> ^name << mark-visited go-to-location >> )
-->
   (<s> ^operator <o1> > <o2>)
}