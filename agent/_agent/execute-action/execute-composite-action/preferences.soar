sp {execute-composite-action*prefer*execute-action*over*all*others
   (state <s> ^problem-space.name execute-composite-action
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name execute-action)
   (<o2> ^name <> execute-action)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-composite-action*reject*execute-action*if*not*matched
   (state <s> ^problem-space.name execute-composite-action
              ^operator <o> +
              ^subaction-matched false)
   (<o> ^name execute-action
        ^action <a>)
   (<a> -^validated true)
-->
   (<s> ^operator <o> -)
}