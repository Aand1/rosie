sp {execute-composite-action*propose*execute-action*child-action
   (state <s> ^problem-space.name execute-composite-action
              ^child-action <action>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name execute-action
        ^action <action>)
}

sp {execute-composite-action*apply*execute-action*child-action*mark*validated
   (state <s> ^problem-space.name execute-composite-action
              ^operator <o>)
   (<o> ^name execute-action
        ^action <action>)
-->
   (<action> ^validated true)
}

sp {execute-composite-action*apply*execute-action*mark-performed*subaction
   (state <s> ^problem-space.name execute-composite-action
              ^operator <o>
              ^operator-matches.match-info <info>)
   (<o> ^name execute-action
        ^action.execution-operator <ex-op>)
   (<info> ^op-matched yes
           ^operator <sub-op>)
   (<sub-op> ^subaction-handle <sub-handle>
             ^perform once)
-->
   (<ex-op> ^subaction-handle <sub-handle>
            ^perform once)
}
   