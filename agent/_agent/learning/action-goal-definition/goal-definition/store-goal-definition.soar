sp {store*general*goal*definition*objects
   (state <s> ^name goal-definition
   	      ^goal-id <gid>
	      ^goal <goal>
	      ^smem.command <scomm>
	      ^generalizable <gen>
	      ^pairing <p>)
   (<p> ^smem <grel>
   	^wmem <relp>)
   (<gen> ^wme-id <obj>
   	  ^lt-id <sobj>)
   (<goal> ^<pred-link> <relp>)
   (<relp> ^<relarg> <obj>)
   (<gid> ^<pred-link> <grel>)
-->
   (<grel> ^<relarg> <sobj>)
   (<scomm> ^store <sobj> <gid> <grel>)
}

sp {store*general*goal*definition*relation
   (state <s> ^name goal-definition
   	      ^goal-id <gid>
	      ^goal <goal>
	      ^smem.command <scomm>
	      ^generalizable <gen>
	       ^pairing <p>)
   (<p> ^smem <grel>
   	^wmem <relp>)
   (<gen> ^wme-id <obj>
   	  ^lt-id <sobj>)
   (<goal> ^<pred-link> <relp>)
   (<relp> ^<relarg>.smem-rep <obj>)
   (<gid> ^<pred-link> <grel>)
-->
   (<grel> ^<relarg> <sobj>)
   (<scomm> ^store <sobj> <gid> <grel>)
}



## non-generalizable structure are integral to verb definition
sp {store*general*goal*definition*1*no-generalization*object
   (state <s> ^name goal-definition
   	      ^goal-id <gid>
	      ^goal <goal>
	      ^smem.command <scomm>
	        ^pairing <p>)
   (<p> ^smem <grel>
   	^wmem <relp>)
  -{(<s> ^generalizable <gen>)
    (<gen> ^wme-id <obj>)}
   (<goal> ^<pred-link> <relp>)
   (<relp> ^<relarg> <obj>)
   (<obj> ^id <sobj>)
   (<gid> ^<pred-link> <grel>)
-->
   (<grel> ^<relarg> <nobj>)
   (<nobj> ^default <sobj>)
   (<scomm> ^store <grel> <nobj>)
}

sp {store*general*goal*definition*no-generalization*relation
   (state <s> ^name goal-definition
   	      ^goal-id <gid>
	      ^goal <goal>
	      ^smem.command <scomm>
	      ^pairing <p>)
   (<p> ^smem <grel>
   	^wmem <relp>)
 -{(<s> ^generalizable <gen>)
    (<gen> ^wme-id <obj>)}
   (<goal> ^<pred-link> <relp>)
   (<relp> ^<relarg> <q>)
   (<q> -^id
   	^smem-rep <obj>)
   (<gid> ^<pred-link> <grel>)
-->
   (<grel> ^<relarg> <nobj>)
   (<nobj> ^default <obj>)
   (<scomm> ^store <nobj> <grel> <obj>)
}