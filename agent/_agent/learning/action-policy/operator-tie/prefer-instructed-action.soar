sp {elaborate*attribute*match*epmem*name
   (state <s> ^name selection
   	      ^indexed-action <map-op>
   	      ^operator <o> +)
   (<o> ^superoperator <sop>)
   (<sop> ^name <n>)
   (<map-op> ^name <n>) 
-->
   (<o> ^matched name)
}

sp {selection*mark*operators*with*same*arguments*relation
   (state <s> ^name selection
   	      ^operator <op> +
	      ^indexed-action <ia>)
   (<op> ^superoperator <sop>)
   (<ia> ^<arg> <val>)
   (<val> ^<arg2> <obj>)
   (<sop> ^<arg> <nval>)
   (<nval> ^<arg2> <nobj>)
   (<obj> ^id <id>) 
   (<nobj> ^id <id>)
-->
   (<op> ^matched <arg>)
}

sp {selection*mark*operators*with*same*argument
   (state <s> ^name selection
   	      ^operator <op> +
	      ^indexed-action <ia>)
   (<op> ^superoperator <sop>)
   (<ia> ^<arg> <val>)
   (<val> -^relation <any>)
   (<sop> ^<arg> <nval>)
   (<val> ^id <id>) 
   (<nval> ^id <id>)
-->
   (<op> ^matched <arg>)
}

sp {selection*elaborate*operator*match
   (state <s> ^name selection
   	      ^operator <op> + 
   	      ^indexed-action <map-op>
	      -^ignore-set.attribute <attr>)
   (<op> ^superoperator <sop>)
   (<map-op> ^<attr> <val>)
   (<op> -^matched <attr>)
-->
   (<op> ^op-matched no
   	 ^not-matched <attr>)
}

sp {selection*prefer*match*epmem
   (state <s> ^name selection
              ^indexed-action <map-op>
   	      ^operator <o> +)
   (<o> -^op-matched no)
   -->
   (<s> ^operator <o> >)
}

sp {prefer*match*epmem*elaborate*spatial-rel
   (state <s> ^name selection
	      ^superstate.desired <des>
   	      ^operator <o>)
   (<o> -^not-matched <any>
   	^superoperator <sop>)
   (<des> ^track-relation <tr>)
   (<tr> ^relation <prep>
   	 ^<trarg> <trval>)
   (<sop> ^<arg2> <sop-arg>)
   (<sop-arg> ^<arg> <val2>)
   (<val2> ^focus <fval2>)
   (<trval> ^focus <fval2>)
   -->
   (<sop-arg> ^relation <prep>)
}
