### elaborate state 
sp {elaborate*state*name*action*operator-tie
   (state <s> ^superstate <ss>
   	      ^attribute operator
	      ^impasse tie)
   (<ss> ^name action
   	 ^problemspace.action.type execute)
-->
   (<s> ^name multiple-argument-instantiations)
}

sp {multiple-argument-instantiations*elaborate*indexed-action
   (state <s> ^name multiple-argument-instantiations
   	      ^superstate.superstate-indexed-action <ia>)
-->
   (<s> ^indexed-action <ia>)
}

sp {multiple-argument-instantiations*elaborate*arguments
   (state <s> ^name multiple-argument-instantiations
   	      ^indexed-action <ia>)
   (<ia> ^{<arg> <> name} <val1>
   	 ^<arg> {<val2> <> <val1>})
-->
   (<s> ^multiple-instantiations-of <arg>)
}


