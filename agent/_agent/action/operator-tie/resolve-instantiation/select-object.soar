sp {resolve*propose*select*object
   (state <s> ^name resolve-instantiation
   	      ^object-link <link>)
   (<link> ^result <res>)
-->
   (<s> ^operator <op> +)
   (<op> ^name select-object
   	 ^object <res>)
}

sp {resolve*select*object*preference*DEF
   (state <s> ^name resolve-instantiation
   	      ^operator <op1> +
	      ^operator <op2> +
	      ^object-link <link>)
   (<link> ^result-info <info>
   	   ^lexical.specifier << it this that DEF >>)
   (<info> ^score <score1>
	   ^score <score2>)
   (<score1> ^result <obj1>
   	    ^value <val1>)
   (<score2> ^result <obj2>
   	    ^value {<val2> > <val1>})
   (<op1> ^object <obj1>
   	  ^name select-object)
   (<op2> ^object <obj2>
   	  ^name select-object)
-->
   (<s> ^operator <op1> > <op2>)
}

sp {resolve*select*object*preference*INDEF
   (state <s> ^name resolve-instantiation
   	      ^operator <op1> +
	      ^operator <op2> +
	      ^object-link <link>)
   (<link> ^result-info <info>
   	   ^lexical.specifier << INDEF >>)
   (<op1> ^object <obj1>
   	  ^name select-object)
   (<op2> ^object {<obj2> <> <obj1>}
   	  ^name select-object)
-->
   (<s> ^operator <op1> = <op2>)
}

sp {resolve*instantiation*apply*select*object*
   (state <s> ^name resolve-instantiation
   	      ^operator <op1>)
   (<op1> ^object <obj1>
   	  ^name select-object)
-->
   (<s> ^winner <obj1>)
}