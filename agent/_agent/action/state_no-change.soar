sp {action*state_no-change*propose*report-failure
   (state <s> ^superstate.problemspace.action.type execute
              ^impasse no-change
              ^attribute state)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-failure)
}

sp {action*state_no-change*apply*report-failure
   (state <s> ^superstate.problemspace.action.type execute
              ^impasse no-change
              ^attribute state
              ^operator.name report-failure
              ^top-state.interaction.status <status>)
-->
   (<status> ^failure.type no-proposed-action)
}
   
              