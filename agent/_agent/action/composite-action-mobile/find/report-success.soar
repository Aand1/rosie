sp {initiate-find1*propose*report-success
   (state <s> ^name initiate-find1
              ^object-to-find.predicates <preds>
              ^current-waypoint <wp>
              ^starting-waypoint <wp>)
   (<preds> -^arm-status grabbed
             ^visible true)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {initiate-find1*apply*report-success
   (state <s> ^name initiate-find1
              ^operator.name report-success
              ^top-state.interaction.status <status>)
-->
   (<status> ^performed.action-event.type find1)
}