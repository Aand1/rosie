sp {learn-desired-elaboration-rule*propose*report-unknown-goal-failure
   (state <s> ^name learn-desired-elaboration-rule
              ^action-operator.action-id <id>)
   (<id> -^goal <goal>)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-unknown-goal-failure)
}

sp {learn-desired-elaboration-rule*apply*report-unknown-goal-failure
   (state <s> ^name learn-desired-elaboration-rule
              ^operator.name report-unknown-goal-failure
              ^top-state.interaction.status <status>)
-->
   (<status> ^failure.type unknown-goal)
}

