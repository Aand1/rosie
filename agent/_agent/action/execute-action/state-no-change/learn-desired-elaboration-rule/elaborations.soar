sp {learn-desired-elaboration-rule*elaborate*current-action*from*superstate
   (state <s> ^name learn-desired-elaboration-rule
              ^superstate.superstate.operator <action>)
-->
   (<s> ^current-action <action>)
}

sp {learn-desired-elaboration-rule*elaborate*action-id*from*current-action
   (state <s> ^name learn-desired-elaboration-rule
              ^superstate.action-id <id>)
-->
   (<s> ^action-id <id>)
}

sp {learn-desired-elaboration-rule*elaborate*predicate
   (state <s> ^name learn-desired-elaboration-rule
              ^action-id.goal.predicate <pred>)
-->
   (<s> ^predicate <gp>)
   (<gp> ^smem-rep <pred>)
}

sp {learn-desired-elaboration-rule*elaborate*desired
   (state <s> ^name learn-desired-elaboration-rule)
-->
   (<s> ^desired <desired>)
}

sp {learn-desired-elaboration-rule*elaborate*world
   (state <s> ^name learn-desired-elaboration-rule
              ^superstate.world <world>)
-->
   (<s> ^world <world>)
}
