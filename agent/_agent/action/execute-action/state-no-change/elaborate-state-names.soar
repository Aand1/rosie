sp {execute*action*state*no-change*elaborate*learn-desired-elaboration-rule
   (state <s> ^superstate.problem-space.name execute-composite-action
              ^impasse no-change
              ^attribute state
              -^superstate.desired)
-->
   (<s> ^name learn-desired-elaboration-rule)
}

sp {execute*action*state*no-change*elaborate*no-proposed-action
   (state <s> ^superstate.problem-space.action.type execute
             -^name << learn-desired-elaboration-rule >>
              ^impasse no-change
              ^attribute state)
-->
   (<s> ^name no-proposed-action)
}

   
              