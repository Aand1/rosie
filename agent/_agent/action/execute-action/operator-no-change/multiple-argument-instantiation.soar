
sp {multiple-argument-instantiations*elaborate*problem-space
   (state <s> ^name multiple-argument-instantiations)
-->
   (<s> ^problem-space.action.type none)
}

sp {multiple-argument-instantiations*elaborate*current-action
   (state <s> ^name multiple-argument-instantiations
              ^superstate.current-action <cmd>)
-->
   (<s> ^current-action <cmd>)
}

sp {multiple-argument-instantiations*elaborate*arguments
   (state <s> ^name multiple-argument-instantiations
   	      ^current-action <cmd>)
   (<cmd> ^action-id.procedural.arg-name <arg-name>
          ^<arg-name> <arg1> { <arg2> <> <arg1> })
-->
   (<s> ^multiple-instantiations-of <arg-name>)
}

sp {multiple-argument-instantiations*propose*report-multiple-arguments
   (state <s> ^name multiple-argument-instantiations)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-multiple-arguments)
}

sp {multiple-argument-instantiations*apply*report-multiple-arguments
   (state <s> ^name multiple-argument-instantiations
              ^operator.name report-multiple-arguments
              ^top-state.interaction.status <status>)
-->
   (<status> ^failure.type multiple-arguments)
}