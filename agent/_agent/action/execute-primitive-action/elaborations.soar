sp {execute-primitive-action*elaborate*problem-space
   (state <s> ^superstate.operator.problem-space execute-primitive-action)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name execute-primitive-action
         ^action <action>
         ^state <state>)
   (<action> ^type execute 
             ^category primitive
             ^propose-actions false)
   (<state> ^type current
            ^world-copy shallow)
}

sp {execute-primitive-action*elaborate*state*current-action
   (state <s> ^problem-space.name execute-primitive-action
              ^superstate.current-action <ca>)
-->
   (<s> ^current-action <ca>)
}

sp {execute-primitive-action*elaborate*state*attend-to-all-scene-changes*waiting
   (state <s> ^superstate.problem-space.name execute-primitive-action
              ^superstate.operator.command-operator true)
-->
   (<s> ^attend-to-all-scene-changes true
        ^allow-wait true)
}