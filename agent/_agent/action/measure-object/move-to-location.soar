sp {measure-object*propose*move-to-location
   (state <s> ^name measure-object
              ^object-on-location false
             -^measure-subgoal.made-measurement true
              ^location-to-measure-at <loc>
              ^object-to-measure <obj>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name op_1
        ^_metadata <move>
        ^action-type composite
        ^argument3 <obj>
        ^argument4.argument <loc>
        ^action-info measure-move-to-location)
}

sp {measure-object*move-to-location*propose*copy-success-to-superstate
   (state <s> ^name op_1
              ^success <success>
              ^superstate.operator <o>)
   (<o> ^name op_1
        ^action-info measure-move-to-location)
-->
   (<o> ^succeeded true)
}

sp {measure-object*copy*moved-to-location
   (state <s> ^name measure-object
              ^operator <o>)
   (<o> ^name op_1
        ^succeeded true
        ^action-info measure-move-to-location)
-->
   (<s> ^moved-to-location true)
}

sp {measure-object*prefer*skip*over*move
   (state <s> ^name measure-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name skip-move-to-location)
   (<o2> ^name op_1)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {measure-object*propose*skip-move-to-location
   (state <s> ^name measure-object
              ^move-metadata
             -^moved-to-location
              ^location-to-measure-at.id <loc>
              ^object-to-measure.id <obj>
              ^topstate.prepositions.prep <prep>)
   (<prep> ^name on
           ^pair <p>)
   (<p> ^a <obj>
        ^b <loc>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name skip-move-to-location)
}

sp {measure-object*apply*skip-move-to-location
   (state <s> ^name measure-object
              ^operator.name skip-move-to-location)
-->
   (<s> ^moved-to-location true)
}

