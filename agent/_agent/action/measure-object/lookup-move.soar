
sp {measure-object*propose*comprehend-action-command
   (state <s> ^name measure-object
             -^move-metadata)
-->
   (<s> ^operator <o> + >)
   (<o> ^name comprehend-action-command
        ^parameter <move>)
   (<move> ^type action-command
           ^lexical.verb <verb>)
   (<verb> ^direct-object <do> ^preposition <on> ^word move)
}

sp {measure-object*apply*comprehend-action-command*copy*metadata
   (state <s> ^name measure-object
              ^operator <o>)
   (<o> ^name comprehend-action-command
        ^parameter.result._metadata <meta>)
-->
   (<s> ^move-metadata <meta>)
}

sp {measure-object*comprehend-action-command*remove*action*problemspace
   (state <s> ^name measure-object 
              ^operator <o> +)
   (<o> ^name comprehend-action-command)
-->
   (<s> ^problemspace action -)
}