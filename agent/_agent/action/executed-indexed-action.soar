	    
sp {propose*write*action*status*topstate
   (state <s> ^superstate <ss>
   	      ^impasse no-change
	      ^attribute state)
   (<ss> ^indexed-action <act>
   	 ^name action
	 ^superstate.operator.name action
         ^topstate.interaction.status <stat>
         ^lexical-information <lex>)
   (<act> ^applied yes)
-->
   (<stat> ^performed <performed>)
   (<performed> ^action-event <ae>)
   (<ae> ^type <lex>)
}

sp {propose*write*action*status*topstate*composite-action
   (state <s> ^indexed-action <act>
   	      ^name action
	      ^topstate-indexed-action <tact>
	      ^superstate.operator.name action
	      ^topstate.interaction.status <stat>
              ^lexical-information <lex>)
   (<tact> ^applied yes)
   (<act> ^action-type composite)
-->
   (<stat> ^performed <performed>)
   (<performed> ^action-event <ae>)
   (<ae> ^type <lex>)
}