sp {topstate*apply*execute-open*send*open*command
   (state <s> ^topstate <top>
              ^operator <o>
              ^io.output-link <ol>)
   (<o> ^name execute-open
        ^arg1.id <id>)
-->
   (<ol> ^set-state <set>)
   (<set> ^id <id>
          ^name door
          ^value open)
}

sp {topstate*apply*execute-open*mark*current-action*complete
   (state <s> ^topstate <s>
              ^operator <o>
              ^current-action <a>)
   (<o> ^name execute-open
        ^arg1.predicates.door open)
-->
   (<a> ^status complete)
}
