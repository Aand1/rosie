### action model

sp {apply*put-down*action-model
   (state <s> ^operator <op>
	      -^io
	      ^grabbed <obj>)
   (<op> ^name op_put-down-object-location
   	 ^argument1 <obj>
	 ^argument2 <nval>)
   (<nval> ^argument <loc>
	   ^relation <rel>)
 -->
   (<s> ^grabbed <obj> -
   	^track-relation <trel>)
   (<trel> ^object1 <obj>
   	   ^object2 <loc>
	   ^complete success
	   ^vpredicate <rel>)
}

#### elaborate not-tracking
sp {apply*put-down*action-model*delete*not-tracking
   (state <s> ^operator <op>
	      -^io)
   (<op> ^name op_put-down-object-location
   	 ^argument1 <obj>
	 ^argument2 <nval>)
   (<nval> ^argument <loc>
	   ^relation <rel>)
   (<s> ^not-tracking <tr>)
   (<tr> ^p1 <obj1>
   	 ^p2 <obj2>
	 ^relation <rel>)
   (<obj1> ^focus <f1>)
   (<obj2> ^focus <f2>)
   (<obj> ^focus <f1>)
   (<loc> ^focus <f2>)
-->
   (<s> ^not-tracking <tr> -)
}

