sp {go-to-waypoint*propose*go-to-next-waypoint*all*adjacent*same*map
   (state <s> ^name go-to-waypoint
              ^same-map true
              ^current-location <cur>)
   (<cur> ^map.id <map-id>
          ^next <next>)
   (<next> ^map.id <map-id>)
-->
   (<s> ^operator <o> +)
   (<o> ^name go-to-next-waypoint
        ^current-location <cur>
        ^waypoint <next>)
}

sp {go-to-waypoint*prefer*go-to-next-waypoint*desired*waypoint
   (state <s> ^name go-to-waypoint
              ^operator <o> +
              ^desired.waypoint.id <id>)
   (<o> ^name go-to-next-waypoint
         ^waypoint.id <id>)
-->
   (<s> ^operator <o> >)
}
