sp {go-to-waypoint*propose*go-to-next-waypoint*all*adjacent
   (state <s> ^type go-to-waypoint
              ^top-state.current-location.neighbor-area <next>)
-->
   (<s> ^operator <o> +)
   (<o> ^name go-to-next-waypoint
        ^area <next>)
}

sp {go-to-waypoint*prefer*go-to-next-waypoint*desired*area
   (state <s> ^type go-to-waypoint
              ^operator <o> +
              ^desired.location.id <id>)
   (<o> ^name go-to-next-waypoint
         ^area.id <id>)
-->
   (<s> ^operator <o> >)
}

sp {go-to-waypoint*propose*go-to-next-waypoint*destination-area
   (state <s> ^type go-to-waypoint
              ^name evaluate-operator
              ^destination-area.id <dest-id>
              ^current-location <cur-loc>)
   (<cur-loc> ^neighbor-area <neighbor>)
   (<neighbor> ^id <dest-id>)
-->
   (<s> ^operator <o> + = >)
   (<o> ^name go-to-next-waypoint
        ^area <neighbor>)
}
