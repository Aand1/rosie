
sp {action*elaborate*initiate-put-down1*arg2*from*action-command
   (state <s> ^problemspace.action <act>
              ^operator <o> +
              ^action-command <cmd>)
   (<o> ^name initiate-put-down1)
   (<cmd> ^arg2.argument <arg2>)
   (<arg2> ^id <rel>
           ^2 <obj>)
-->
   (<o> ^arg2 <new-arg2>)
   (<new-arg2> ^2 <obj>
               ^id <rel>)
}

sp {action*elaborate*until-clause*on*action-operator*when*selected
   (state <s> ^problemspace.action <act>
              ^operator <o>
              ^action-command <cmd>)
   (<cmd> ^_metadata.initiation-operator <op-name>
          ^until-clause <until>)
   (<o> ^name <op-name>)
-->
   (<o> ^until-clause <until>)
}
              