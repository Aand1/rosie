

sp {action*propose*execute-move1*action-command
   (state <s> ^name action
              ^action-command <cmd>)
   (<cmd> ^id.name move1
          ^arg1 <obj>
          ^arg2.{2} <dest>
          ^arg2.id <rel>)
-->
   (<s> ^operator <o> +)
   (<o> ^name execute-move1
        ^arg1 <obj>
        ^arg2 <new-arg2>
        ^action-type composite)
   (<new-arg2> ^id <rel>
               ^2 <dest>)
}

