sp {action*propose*execute-move1*action-command
   (state <s> ^name action
              ^action-command <cmd>)
   (<cmd> ^id.name move1
          ^arg1 <obj>
          ^arg2.arg2 <dest>
          ^arg2.id <rel>)
-->
   (<s> ^operator <o> +)
   (<o> ^name execute-move1
        ^arg1 <obj>
        ^arg2 <new-arg2>
        ^action-type composite)
   (<new-arg2> ^id <rel>
               ^arg2 <dest>)
}

sp {action*propose*go-to-waypoint*action-command
   (state <s> ^name action
              ^action-command <cmd>
              ^topstate.map.waypoints.waypoint <wp>)
   (<cmd> ^id.name go-to-waypoint1
          ^waypoint-id <wp-id>)
   (<wp> ^id <wp-id>)
-->
   (<s> ^operator <o> +)
   (<o> ^name go-to-waypoint
        ^waypoint <wp>
        ^waypoint-id <wp-id>
        ^action-type composite)
}