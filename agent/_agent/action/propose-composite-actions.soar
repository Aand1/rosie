sp {action*propose*execute-move1*action-command
   (state <s> ^name action
              ^action-command <cmd>)
   (<cmd> ^id.name move1
          ^arg1 <obj>
          ^arg2.arg2 <dest>
          ^arg2.id <rel>)
-->
   (<s> ^operator <o> +)
   (<o> ^name execute-move1
        ^arg1 <obj>
        ^arg2 <new-arg2>
        ^action-type composite)
   (<new-arg2> ^id <rel>
               ^arg2 <dest>)
}

sp {action*propose*go-to-waypoint*action-command
   (state <s> ^name action
              ^action-command <cmd>
              ^world.robot.current-waypoint <cur>)
   (<cmd> ^id.name go-to-waypoint1
          ^waypoint <wp>)
-->
   (<s> ^operator <o> +)
   (<o> ^name go-to-waypoint
        ^current-location <cur>
        ^waypoint <wp>
        ^action-type composite)
}