
####################### propose initiate-go1 ##################################
sp {action*propose*initiate-go1
   (state <s> ^problem-space.action.propose-actions true)
-->
   (<s> ^operator <o> +)
   (<o> ^name initiate-go1
        ^action-name go1
         ^action-type composite)
}

sp {follow-command*propose*execute-move1*action-command
   (state <s> ^name follow-command
              ^action-command <cmd>)
   (<cmd> ^id.name move1
          ^arg1 <obj>
          ^arg2.{2} <dest>
          ^arg2.id <rel>)
-->
   (<s> ^operator <o> +)
   (<o> ^name execute-move1
        ^arg1 <obj>
        ^arg2 <new-arg2>
        ^action-type composite)
   (<new-arg2> ^id <rel>
               ^2 <dest>)
}

sp {follow-command*propose*initiate-deliver1*action-command
   (state <s> ^name follow-command
              ^action-command <cmd>)
   (<cmd> ^id.name deliver1
          ^arg1 <obj>
          ^arg2.{2} <dest>
          ^arg2.id <rel>)
-->
   (<s> ^operator <o> +)
   (<o> ^name initiate-deliver1
        ^arg1 <obj>
        ^arg2 <new-arg2>
        ^action-type composite)
   (<new-arg2> ^id <rel>
               ^2 <dest>)
}