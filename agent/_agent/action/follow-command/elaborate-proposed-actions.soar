sp {follow-command*elaborate*initiate-put-down1*arg2*from*action-command
   (state <s> ^name follow-command
              ^operator <o> +
              ^action-command <cmd>)
   (<o> ^name initiate-put-down1)
   (<cmd> ^arg2 <arg2>)
   (<arg2> ^arg-type predicate
           ^id <rel>
           ^2 <obj>)
-->
   (<o> ^arg2 <new-arg2>)
   (<new-arg2> ^arg-type predicate
               ^2 <obj>
               ^id <rel>)
}

sp {follow-command*elaborate*until-clause*on*action-operator
   (state <s> ^name follow-command 
              ^operator <o> +
              ^action-command <cmd>)
   (<cmd> ^_metadata.op_name <op-name>
          ^until-clause <until>)
   (<o> ^name <op-name>)
-->
   (<o> ^until-clause <until>)
}

