sp {follow-command*elaborate*retrieve-lti*action-command*action*id
   (state <s> ^name follow-command
              ^action-command.id <id>
             -^retrieved-lti <id>)
   (<id> -^name)
-->
   (<s> ^retrieve-lti <id>)
}

sp {follow-command*elaborate*retrieve-lti*action-command*procedural
   (state <s> ^name follow-command
              ^action-command.id.procedural <proc>
             -^retrieved-lti <proc>)
   (<proc> -^op_name)
-->
   (<s> ^retrieve-lti <proc>)
}

sp {follow-command*elaborate*retrieve-lti*action-command*procedural*argument
   (state <s> ^name follow-command
              ^action-command.id.procedural <proc>
             -^retrieved-lti <arg>)
   (<proc> ^{<arg-name> <> op_name <> execution-operator} <arg>)
   (<arg> -^arg-type)
-->
   (<s> ^retrieve-lti <arg>)
}
