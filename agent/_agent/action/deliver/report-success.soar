sp {initiate-deliver1*propose*report-success
   (state <s> ^name initiate-deliver1
              ^object-to-deliver.predicates <preds>
              ^current-waypoint <wp>
              ^delivery-waypoint <wp>)
   (<preds> -^arm-status grabbed)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {initiate-deliver1*apply*report-success
   (state <s> ^name initiate-deliver1
              ^operator.name report-success
              ^top-state.interaction.status <status>)
-->
   (<status> ^performed.action-event.type deliver1)
}