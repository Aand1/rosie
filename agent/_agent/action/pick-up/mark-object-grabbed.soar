sp {op_pick-up*elaborate*topstate*grabbed
   (state <s> ^name op_pick-up-object
              ^operator <o>
              ^topstate <top>
              ^io.input-link.self <self>)
   (<self> ^grabbed-object {<id> <> -1}
           ^action wait
           ^prev-action grab)
   (<top> ^object <obj>)
   (<obj> ^id <id>
          ^category block)
-->
   (<top> ^grabbed <obj>)
   (write |Marked object as grabbed| (crlf))
}

