sp {execute-pick-up*propose*complete-pick-up
   (state <s> ^name execute-pick-up
              ^current-action.command-status complete)
-->
   (<s> ^operator <o> + >)
   (<o> ^name complete-pick-up)
}

sp {execute-pick-up*apply*complete-pick-up*remove*moving
   (state <s> ^name execute-pick-up
              ^operator.name complete-pick-up
              ^target-object.predicates <preds>)
   (<preds> ^arm-status moving)
-->
   (<preds> ^arm-status moving -)
}

sp {execute-pick-up*apply*complete-pick-up*add*grabbed
   (state <s> ^name execute-pick-up
              ^operator.name complete-pick-up
              ^target-object.predicates <preds>)
-->
   (<preds> ^arm-status grabbed)
}

