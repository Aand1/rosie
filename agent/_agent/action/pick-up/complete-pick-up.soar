sp {execute-pick-up*apply*complete-pick-up*mark*finished
   (state <s> ^name execute-pick-up
              ^operator.name complete-pick-up
              ^target-object.predicates.arm-status grabbed
              ^superstate.operator.action <a>)
-->
   (<a> ^status complete)
}

sp {execute-pick-up*apply*complete-pick-up*move*belief*to*arm
   (state <s> ^name execute-pick-up
              ^operator.name complete-pick-up
              ^target-object.svs-obj.id <id>
              ^io.input-link.self.pose <pose>
              ^topstate.svs.command <cmd>)
   (<pose> ^x <x> ^y <y> ^z <z>)
-->
   (<cmd> ^set_transform <xform>)
   (<xform> ^id <id>
            ^position <pos>)
   (<pos> ^x <x> ^y <y> ^z <z>)
}