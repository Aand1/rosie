sp {move-object*propose*select-subaction-perform-move
   (state <s> ^name move-object
              ^move-metadata <move>
              ^object-to-move <obj>
              ^destination <dest>
              ^current-action.cleared-object true)
-->
   (<s> ^operator <o> +)
   (<o> ^name select-subaction-perform-move
        ^action-type custom
        ^object <obj>
        ^destination <dest>
        ^metadata <move>)
}

sp {move-object*apply*select-subaction-perform-move
   (state <s> ^name move-object
              ^operator <o>
              ^current-action <a>)
   (<o> ^name select-subaction-perform-move
        ^object <obj>
        ^destination <dest>
        ^metadata <move>)
-->
   (<a> ^subaction <sub>)
   (<sub> ^name op_1
          ^parameters <p>)
   (<p> ^_metadata <move>
        ^action-type composite
        ^argument3 <obj>
        ^argument4.argument <dest>)
}

sp {move-object*move-to-location*propose*copy-success-to-superstate
   (state <s> ^name op_1
              ^success <success>
              ^superstate.operator <o>)
   (<o> ^name op_1)
-->
   (<o> ^succeeded true)
}

sp {move-object*apply*perform-move*mark*complete
   (state <s> ^name move-object
              ^operator <o>
              ^current-action <a>)
   (<o> ^name op_1
        ^succeeded true)
-->
   (<a> ^complete true)
}
