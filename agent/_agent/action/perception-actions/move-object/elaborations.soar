sp {move-object*elaborate*move-metadata
   (state <s> ^name move-object 
              ^action-stack.global-parameters.move-metadata <move>)
-->
   (<s> ^move-metadata <move>)
}

sp {move-object*elaborate*object-to-move
   (state <s> ^name move-object
              ^current-action.object <obj>)
-->
   (<s> ^object-to-move <obj>)
}

sp {move-object*elaborate*destination
   (state <s> ^name move-object
              ^current-action.destination <dest>)
-->
   (<s> ^destination <dest>)
}

sp {move-object*elaborate*above-prep
   (state <s> ^name move-object
              ^topstate.prepositions.prep <above>)
   (<above> ^name above)
-->
   (<s> ^above-prep <above>)
}

sp {move-object*elaborate*object-is-clear*false
   (state <s> ^name move-object
              ^object-to-move.id <obj>
              ^above-prep.pair.b <obj>)
-->
   (<s> ^object-is-clear false)
}

sp {move-object*elaborate*object-is-clear*true
   (state <s> ^name move-object
             -^object-is-clear false)
-->
   (<s> ^object-is-clear true)
}


sp {find-object*apply*subaction*complete*no*object*to*grab
   (state <s> ^name move-object
              ^operator.name op_1
              ^object-to-move <obj>
              ^robot-state.arm <arm>
              ^current-action <a>)
   (<obj> -^perception-obj
          -^moving-object true)
   (<arm> -^grabbed <obj>
           ^action wait)
-->
   (<a> ^complete true)
}
           