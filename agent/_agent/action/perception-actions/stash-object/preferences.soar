sp {stash-object*prefer*count-objects*over*move-object
   (state <s> ^name stash-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name count-objects)
   (<o2> ^name select-subaction-move-object)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {stash-object*reject*move*exclude-bin
   (state <s> ^name stash-object
              ^operator <o> +
              ^exclude-bin <bin>)
   (<o> ^name select-subaction-move-object
        ^bin-count.bin <bin>)
-->
   (<s> ^operator <o> -)
}

sp {stash-object*prefer*move*lower*bin*count
   (state <s> ^name stash-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name select-subaction-move-object
         ^bin-count.count <c>)
   (<o2> ^name select-subaction-move-object
         ^bin-count.count > <c>)
-->
   (<s> ^operator <o1> > <o2>)
}