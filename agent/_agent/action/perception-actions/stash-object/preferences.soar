sp {stash-object*prefer*my*ops*over*primitive*when*grabbed
   (state <s> ^name stash-object
              ^operator <o1> +
              ^operator <o2> +
              ^object-to-stash <obj>
              ^robot-state.arm.grabbed <obj>)
   (<o1> ^name << count-objects select-stash-bin open-bin attend-to-scene >>)
   (<o2> ^action-type primitive)
-->
   (<s> ^operator <o1> > <o2>)
}

