# Subaction Ordering

sp {find-object*prefer*stash-object-on-goal*over*others
   (state <s> ^name find-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << select-subaction-stash-object-on-goal >>)
   (<o2> ^name << select-subaction-lookup-word select-subaction-move-object-to-goal select-subaction-stash-object-on-location select-subaction-stash-homeless-object select-subaction-measure-object select-subaction-clear-object-for-shape report-failure >>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {find-object*prefer*lookup-word*over*others
   (state <s> ^name find-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << select-subaction-lookup-word >>)
   (<o2> ^name << select-subaction-move-object-to-goal select-subaction-stash-object-on-location select-subaction-stash-homeless-object select-subaction-measure-object select-subaction-clear-object-for-shape report-failure >>)
-->
