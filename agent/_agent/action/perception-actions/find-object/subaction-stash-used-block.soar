# Stash a block that has been 'used' (placed on the goal or measuring device)
#   No longer need it

sp {find-object*propose*select-subaction-stash-used-block
   (state <s> ^name find-object
              ^used-block <obj>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name select-subaction-stash-used-block
        ^action-type custom
        ^used-block <obj>)
}

sp {find-object*apply*select-subaction-stash-used-block
   (state <s> ^name find-object
              ^operator <o>
              ^current-action <a>)
   (<o> ^name select-subaction-stash-used-block
        ^used-block <obj>)
-->
   (<a> ^subaction <sub>)
   (<sub> ^name stash-object
          ^object-to-stash <obj>)
}

