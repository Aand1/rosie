# Stash a block that is homeless
# (isn't on a location)
sp {find-object*propose*select-subaction-stash-homeless-block
   (state <s> ^name find-object
              ^homeless-block <obj>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name select-subaction-stash-homeless-block
        ^action-type custom
        ^homeless-block <obj>)
}

sp {find-object*apply*select-subaction-stash-homeless-block
   (state <s> ^name find-object
              ^operator <o>
              ^current-action <a>)
   (<o> ^name select-subaction-stash-homeless-block
        ^homeless-block <obj>)
-->
   (<a> ^subaction <sub>)
   (<sub> ^name stash-object
          ^object-to-stash <obj>)
}

