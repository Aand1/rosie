sp {find-object*propose*report-failure
   (state <s> ^name find-object
             -^matching-info.candidate.attribute-match perfect)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-failure
        ^action-type custom)
}

sp {find-object*apply*report-failure
   (state <s> ^name find-object
              ^operator.name report-failure
              ^current-action <a>)
-->
   (<a> ^complete true)
}

sp {find-object*apply*subaction*complete*missing-object
   (state <s> ^name find-object
              ^operator <o>
              ^topstate <top>)
   (<o> ^name << stash-object move-object measure-object clear-object >>
        ^subaction <sub>)
   (<sub> ^object <obj>)
   (<top> -^object <obj>)
-->
   (<sub> ^complete true)
}

sp {find-object*apply*subaction*complete*missing-destination
   (state <s> ^name find-object
              ^operator <o>
              ^topstate <top>)
   (<o> ^name << move-object >>
        ^subaction <sub>)
   (<sub> ^destination <dest>)
   (<top> -^object <dest>)
-->
   (<sub> ^complete true)
}
