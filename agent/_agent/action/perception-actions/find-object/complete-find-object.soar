sp {find-object*propose*complete-find-object
   (state <s> ^name find-object
              ^desired-object.id <did>
              ^goal-location.id <gid>
              ^above-prep.pair <p>)
   (<p> ^a <did> ^b <gid>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name complete-find-object
        ^action-type custom)
}

sp {find-object*apply*complete-find-object
   (state <s> ^name find-object
              ^operator.name complete-find-object
              ^current-action <a>)
-->
   (<a> ^complete true)
}