sp {action*elaborate*prefer*table-to-stove
   (state <s> ^problemspace action
   	      ^operator <op1> +
	      ^operator <op2> +
	      ^cooked <obj>)
   (<op1> ^name op_put-down-object-location
   	  ^argument2.argument.id 2
	  ^argument1 <obj>)
   (<op2> ^name op_put-down-object-location
   	  ^argument2.argument.id 4
	  ^argument1 <obj>)
-->
   (<s> ^operator <op1> > <op2>)
}

sp {action*copy*cooked*superstate
   (state <s> ^action-type composite
   	      ^action-marker composite
   	      -^io
	      ^superstate <ss>
	      ^cooked <obj>)
-->
    (<ss> ^cooked <obj>)
}


