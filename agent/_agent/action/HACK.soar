sp {action*elaborate*prefer*table-to-stove
   (state <s> ^problemspace action
   	      ^operator <op1> +
	      ^operator <op2> +
	      ^cooked <obj>)
   (<op1> ^name op_put-down-object-location
   	  ^argument2.argument.id 2
	  ^argument1 <obj>)
   (<op2> ^name op_put-down-object-location
   	  ^argument2.argument.id 4
	  ^argument1 <obj>)
-->
   (<s> ^operator <op1> > <op2>)
}

sp {action*copy*cooked*superstate
   (state <s> ^action-type composite
   	      -^io
	      ^superstate <ss>
	      ^cooked <obj>)
-->
   (<ss> ^cooked <obj>)
}

sp {action*copy*off*superstate*not-evaluate
   (state <s> ^action-type composite
      	      ^superstate <ss>
   	      -^io
      	      ^operator <op>)
   (<op> ^name op_stop
   	 ^argument1 <obj>)
   (<ss> ^name evaluate-operator
   	 ^action-type composite)
-->
   (<ss> ^off <obj>)
}


sp {action*copy*on*superstate*not-evaluate
   (state <s> ^action-type composite
   	      -^io
	      ^operator <op>
	      ^superstate <ss>)
   (<op> ^name op_activate
   	 ^argument1 <obj>)
   (<ss> ^name evaluate-operator
   	 ^action-type composite)
-->
   (<ss> ^on <obj>)
}



