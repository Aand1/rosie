sp {execute-put-down*propose*complete-put-down
   (state <s> ^name execute-put-down
              ^current-action.command-status <status>)
-->
   (<s> ^operator <o> + <)
   (<o> ^name complete-put-down)
}

sp {execute-put-down*apply*complete-put-down
   (state <s> ^name execute-put-down
              ^operator.name complete-put-down
              ^current-action <a>)
-->
   (<a> ^status complete)
}