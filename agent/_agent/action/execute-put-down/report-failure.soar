# Report failure if there are no more candidates for the missing object
sp {execute-put-down*propose*report-failure
   (state <s> ^name execute-put-down
              ^current-action.command-status <status>
             -^topstate.scene-monitor.new-object.status
              ^held-object.predicates.status stale)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-failure)
}

# Report a failure, may lead to more interactions
sp {execute-put-down*apply*report-failure*report*failure
   (state <s> ^name execute-put-down
              ^operator.name report-failure
              ^held-object <obj>
              ^topstate.interaction.status <status>)
-->
   (<status> ^failure <f>)
   (<f> ^type missing-object
        ^missing-object <obj>)
}

# Remove the command-status so that if we come back and try it again later, 
#   we can start fresh
sp {execute-put-down*apply*report-failure*remove*status
   (state <s> ^name execute-put-down
              ^current-action <a>
              ^operator.name report-failure)
   (<a> ^command-status <status>)
-->
   (<a> ^command-status <status> -)
}
