sp {execute-drive-to-waypoint1*propose*face-waypoint
   (state <s> ^name execute-drive-to-waypoint1
              ^current-action <a>
              ^waypoint <wp>)
   (<a> -^face-command <cmd>
        -^faced-waypoint)
-->
   (<s> ^operator <o> + >)
   (<o> ^name face-waypoint
        ^waypoint <wp>)
}

sp {execute-drive-to-waypoint1*apply*face-waypoint
   (state <s> ^name execute-drive-to-waypoint1
              ^operator <o>
              ^current-action <a>
              ^top-state.io.input-link <in>
              ^top-state.io.output-link <out>)
   (<o> ^name face-waypoint
        ^waypoint <wp>)
   (<wp> ^id <id> ^x <x> ^y <y>)
   (<in> ^x <cx> ^y <cy>)
-->
   (<out> ^face-point <fp>)
   (<fp> ^cur-x <cx> ^cur-y <cy> ^x <x> ^y <y>)
   (<a> ^face-command <fp>)
}

sp {top-state*execute-drive-to-waypoint1*apply*face-waypoint*cleanup
   (state <s> ^superstate nil
              ^operator.name execute-drive-to-waypoint1
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^face-command <cmd>)
   (<cmd> ^status success)
-->
   (<a> ^face-command <cmd> -
        ^faced-waypoint true)
   (<out> ^face-point <cmd> -)
}
              


   