sp {execute-drive-to-waypoint1*propose*complete-drive-to-waypoint
   (state <s> ^name execute-drive-to-waypoint1
              ^current-action.at-waypoint true)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-drive-to-waypoint)
}

sp {execute-drive-to-waypoint1*prefer*attend
   (state <s> ^name execute-drive-to-waypoint1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name complete-drive-to-waypoint)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-drive-to-waypoint1*apply*complete-drive-to-waypoint
   (state <s> ^name execute-drive-to-waypoint1
              ^operator.name complete-drive-to-waypoint
              ^current-action <a>)
-->
   (<a> ^status complete)
}
   