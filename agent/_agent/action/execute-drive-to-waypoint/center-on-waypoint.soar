sp {execute-drive-to-waypoint1*propose*center-on-waypoint
   (state <s> ^name execute-drive-to-waypoint1
              ^current-action <a>
              ^waypoint <wp>)
   (<a> -^center-command <cmd>
         ^near-waypoint true
        -^at-waypoint)
-->
   (<s> ^operator <o> + >)
   (<o> ^name center-on-waypoint
        ^waypoint <wp>)
}

sp {execute-drive-to-waypoint1*apply*center-on-waypoint
   (state <s> ^name execute-drive-to-waypoint1
              ^operator <o>
              ^current-action <a>
              ^top-state.io.input-link <in>
              ^top-state.io.output-link <out>)
   (<o> ^name center-on-waypoint
        ^waypoint <wp>)
   (<wp> ^id <id> ^x <x> ^y <y>)
   (<in> ^x <cx> ^y <cy>)
-->
   (<out> ^do-control-law <law>)
   (<law> ^name drive-to-tag
          ^parameters.class <id>
          ^termination-condition.name stabilized)
   (<a> ^center-command <law>)
}

sp {top-state*execute-drive-to-waypoint1*apply*center-on-waypoint*cleanup
   (state <s> ^superstate nil
              ^operator.name execute-drive-to-waypoint1
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^center-command <cmd>)
   (<cmd> ^status success)
-->
   (<a> ^center-command <cmd> -
        ^at-waypoint true)
   (<out> ^center-point <cmd> -)
}
              


   