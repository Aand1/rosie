sp {execute-pick-up*propose*remove-command-status
   (state <s> ^name execute-pick-up
              ^target-object.predicates.status live
              ^current-action.command-status failure)
-->
   (<s> ^operator <o> + )
   (<o> ^name remove-command-status)
}

sp {execute-pick-up*apply*remove-command-status
   (state <s> ^name execute-pick-up
              ^operator.name remove-command-status
              ^current-action <a>)
   (<a> ^command-status <status>)
-->
   (<a> ^command-status <status> -)
}