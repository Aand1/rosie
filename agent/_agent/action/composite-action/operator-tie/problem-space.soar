sp {action*operator-tie*elaborate*selection*problem-space
   (state <s> ^superstate.problem-space.name action
              ^superstate.problem-space.action-type composite
              ^impasse tie
              ^attribute operator)
-->
   (<s> ^name selection
        ^problem-space <ps>)
   (<ps> ^name action
         ^action-type composite
         ^default-state-copy no
         ^action <action>
         ^state <state>
         ^learning <learn>)
   (<action> ^type simulate)
   (<state> ^world-copy deep)
   (<learn> ^use chunking)
}

sp {action*operator-tie*elaborate*learning*type*exploration
   (state <s> ^superstate.action-context.action-type composite
              ^problem-space.learning <learn>)
-->
   (<learn> ^type exploration)
}

sp {action*operator-tie*elaborate*learning*type*retrospective
   (state <s> ^superstate.problem-space.learning.type retrospective
              ^problem-space.learning <learn>)
-->
   (<learn> ^type retrospective)
}


