sp {action*operator-tie*elaborate*state*name*no-action-selection-knowledge
   (state <s> ^superstate.problem-space.name action
              ^superstate.action-context.action-type composite
              ^attribute operator
              ^impasse tie)
-->
   (<s> ^name selection)
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^default-state-copy no
         ^action <action>
         ^state <state>
         ^learning <learn>)
   (<action> ^type simulate)
   (<state> ^world-copy deep)
   (<learn> ^type exploration
            ^use chunking)
}


sp {retrospective-learning*elaborate*evaluate-operator*default-desired-copy
   (state <s> ^problem-space.name action
              ^operator <o> +)
   (<o> ^name evaluate-operator)
-->
   (<o> ^default-desired-copy yes)
   (<o> ^default-operator-copy yes)
}

sp {action*apply*init-current-evaluation-depth*max
   (state <s> ^top-selection true
              ^operator <op>)
   (<op> ^name init-current-evaluation-depth)
-->
   (<s> ^max-evaluation-depth 2)
}
