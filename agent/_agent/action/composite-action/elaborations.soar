sp {composite-operator*elaborate*map-id
   (state <s> ^problemspace action
   	      ^action-type composite
	      ^superstate.operator <sop>)
   (<sop> ^_metadata.map-id <map-id>)
-->
   (<s> ^map-id <map-id>)
}

sp {composite-operator*elaborate*other-ids
   (state <s> ^problemspace action
   	      ^action-type composite
	      ^map-id <map-id>)
   (<map-id> ^procedural <proc-id>
   	     ^action-space <as-id>
	     ^goal <gid>)
-->
   (<s> ^procedural-id <proc-id>
   	^action-space-id <as-id>
	^goal-id <gid>)
}

sp {composite-operator*elaborate*not-tracking
   (state <s> ^problemspace action
   	      ^action-type composite
	      ^desired <d>)
   (<d> ^track-relation <dtr>)
   (<dtr> ^p1 <val1>
   	  ^p2 <val2>
	  ^relation <prep>)
  -{(<s> ^track-relation <tr>)
    (<tr> ^object1 <val3>
	 ^object2 <val4>
	 ^vpredicate <prep>
	 ^complete success)
   (<val1> ^focus <focus1>)
   (<val2> ^focus <focus2>)
   (<val3> ^focus <focus1>)
   (<val4> ^focus <focus2>)}
-->
   (<s> ^not-tracking <dtr>)
}

