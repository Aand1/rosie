sp {generate-grounded-predicate*elaborate*objects*level1
   (state <s> ^name generate-grounded-predicate
   	      ^grounded-predicate <gp>
	      ^indexed-action <iop>
	      ^procedural-id.<varg> <sobj>
	      ^predicate-definition <p1>)
   (<p1> ^<parg> <sobj>)
   (<iop> ^<varg> <obj>)
-->
   (<gp> ^<parg> <obj>)
   (<s> ^connected <sobj>)
}

sp {generate-grounded-predicate*elaborate*objects*relation*level2
   (state <s> ^name generate-grounded-predicate
   	      ^grounded-predicate <tr>
	      ^indexed-action <iop>
	      ^procedural-id.<narg>.<varg> <sobj>
	      ^predicate-definition <p1>)
   (<tr> ^_instantiation-of <p1>)
   (<p1> ^<parg> <sobj>)
   (<iop> ^<narg>.<varg> <obj>)
-->
   (<tr> ^<parg> <obj>)
   (<s> ^connected <sobj>)
}

## default values for not connected objects
sp {generate-grounded-predicate*elaborate*objects*relation*default
   (state <s> ^name generate-grounded-predicate
   	      ^grounded-predicate <tr>
	      -^connected <sobj>
	      ^predicate-definition <p1>)
   (<tr> ^_instantiation-of <p1>)
   (<p1> ^<parg> <sobj>)
   (<sobj> ^default <defval>)
-->
   (<tr> ^<parg> <ndef>)
   (<ndef> ^smem-rep <defval>)
}