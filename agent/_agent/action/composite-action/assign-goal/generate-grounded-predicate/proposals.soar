## propose retrieve predicate if substructure is absent
sp {generate-grounded-predicate*retrieve
   (state <s> ^name generate-grounded-predicate
   	      ^predicate-definition <pred>
	      -^retrieved <rel>)
   (<pred> ^relation <rel>)
   (<rel> -^<any>)
-->
   (<s> ^operator <op> +)
   (<op> ^name retrieve-concept
   	 ^ltid <rel>)
}

sp {generate-grounded-predicate*retrieve*default
   (state <s> ^name generate-grounded-predicate
   	      ^relevant-relation-definition <rel>)
   (<rel> -^<any>)
-->
   (<s> ^operator <op> +)
   (<op> ^name retrieve-concept
   	 ^ltid <rel>)
}


sp {generate-grounded-predicate*retrieve*substructure*relation
   (state <s> ^name generate-grounded-predicate
   	      ^relevant-relation-definition <rel>
	      -^found-prep.predicate <rel>)
   (<rel> ^name <rel-name>)
-->
   (<s> ^operator <op> + >)
   (<op> ^name lookup-preposition
   	 ^type <rel-name>)
}

sp {generate-grounded-predicates*propose*complete-generate-predicate
   (state <s> ^name generate-grounded-predicate
   	      ^grounded-predicate <pred>)
-->
   (<s> ^operator <op> + <)
   (<op> ^name complete-generate-grounded-predicate
   	 ^grounded-predicate <pred>)
}