## propose retrieve predicate if substructure is absent
sp {generate-grounded-predicate*retrieve
   (state <s> ^name generate-grounded-predicate
   	      ^predicate-definition <pred>
	      -^retrieved <rel>)
   (<pred> ^relation <rel>)
   (<rel> -^<any>)
-->
   (<s> ^operator <op> +)
   (<op> ^name retrieve-concept
   	 ^ltid <rel>)
}

sp {generate-grounded-predicate*retrieve*default
   (state <s> ^name generate-grounded-predicate
   	      ^relevant-relation-definition <rel>)
   (<rel> -^<any>)
-->
   (<s> ^operator <op> +)
   (<op> ^name retrieve-concept
   	 ^ltid <rel>)
}

sp {generate-grounded-predicate*retrieve*substructure*relation
   (state <s> ^name generate-grounded-predicate
   	      ^relevant-relation-definition <rel>
	      -^found-prep.predicate <rel>)
   (<rel> ^name <rel-name>)
-->
   (<s> ^operator <op> + >)
   (<op> ^name lookup-preposition
   	 ^type <rel-name>)
}


### If an argument in action-command cannot be accounted for in the goal, create that link

sp {generate-grounded-predicate*augment*goal-mapping
   (state <s> ^name generate-grounded-predicate
   	      ^indexed-action <sop>
	      -^relevant-relation-definition <rel-def>
	      ^predicate-definition <pred>)
   (<sop> ^<argument> <arg-val>
   	  ^_metadata.map-id.procedural <proc-id>)
   (<arg-val> ^argument <any-obj>
   	      ^relation <rel>)
   (<rel> ^smem-rep <rel-def>)
   (<proc-id> ^<argument> <sarg-val>)
   (<sarg-val> ^argument <sany-obj>
   	       ^relation <srel>)
-->
   (<s> ^operator <op> + >)
   (<op> ^name augment-goal-definition
   	 ^predicate-definition <pred>
	 ^relation-node <srel>)
}

sp {generate-grounded-predicates*propose*complete-generate-predicate
   (state <s> ^name generate-grounded-predicate
   	      ^grounded-predicate <pred>)
-->
   (<s> ^operator <op> + <)
   (<op> ^name complete-generate-grounded-predicate
   	 ^grounded-predicate <pred>)
}