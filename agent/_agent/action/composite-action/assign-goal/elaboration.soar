sp {assign-goal*elaborate*state
   (state <s> ^name assign-goal)
-->
   (<s> ^desired <d>
   	^bit flip)
}

sp {assign-goal*elaborate*state*smem-ids
   (state <s> ^name assign-goal
   	      ^superstate.operator <sop>)
   (<sop> ^name assign-goal
   	  ^goal-id <glid>
	  ^procedural-id <pid>)
-->
   (<s> ^goal-id <glid>
   	^procedural-id <pid>)
}