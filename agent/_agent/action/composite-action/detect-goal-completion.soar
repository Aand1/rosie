   (state <s> ^problemspace action
   	      ^desired <d>
   	      ^track-relation <tr>)
   (<d> ^track-relation <dtr>
   	-^achieve-state)
   (<dtr> ^p1 <val1>
   	  ^p2 <val2>
	  ^relation <prep>)
   (<tr> ^object1 <val3>
	 ^object2 <val4>
	 ^vpredicate <prep>
	 ^complete success)
   (<val1> ^focus <fval1>)
   (<val2> ^focus <fval2>)
   (<val3> ^focus <fval1>)
   (<val4> ^focus <fval2>)
-->
   (<s> ^success <d>)
}

