sp {elaborate*state*name*action*operator-tie*no-action-selection-knowledge
   (state <s> ^superstate.problem-space.action.type execute
              ^attribute operator
              ^impasse tie
             -^item.op-matched yes)
-->
   (<s> ^name no-action-selection-knowledge)
}

sp {no-action-selection-knowledge*propose*report-no-action-selection-knowledge
   (state <s> ^name no-action-selection-knowledge)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-no-action-selection-knowledge)
}

sp {no-action-selection-knowlege*apply*report-no-action-selection-knowledge
   (state <s> ^name no-action-selection-knowledge
              ^operator.name report-no-action-selection-knowledge
              ^top-state.interaction.status <status>)
-->
   (<status> ^failure.type no-proposed-action)
}
