sp {create-procedural-structure*propose*complete-structure
   (state <s> ^name create-procedural-structure
              ^procedural <proc>)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-structure)
}

sp {create-procedural-structure*apply*complete-structure*modify*action
   (state <s> ^name create-procedural-structure
              ^operator.name complete-structure
              ^action-id <id>
              ^procedural <proc>)
-->
   (<id> ^type action ^action-type composite ^procedural <proc>)
}

sp {create-procedural-structure*apply*complete-structure*store
   (state <s> ^name create-procedural-structure
              ^operator.name complete-structure
              ^top-state.smem.command <cmd>
              ^action-id <id>
              ^procedural <proc>)
-->
   (<cmd> ^store <id>)# <proc>)
}
