sp {execute*action*state*no-change*no-proposed-action
   (state <s> ^superstate.problem-space.action.type execute
              ^impasse no-change
              ^attribute state)
-->
   (<s> ^name no-proposed-action)
}

sp {no-proposed*action*propose*create-procedural-structure
   (state <s> ^name no-proposed-action
              ^superstate.action-command <cmd>)
   (<cmd> ^id <id>)
   (<id> -^procedural)
-->
   (<s> ^operator <o> + >)
   (<o> ^name create-procedural-structure
        ^action-command <cmd>)
}

sp {no-proposed-action*propose*report-failure
   (state <s> ^name no-proposed-action)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-failure)
}

sp {no-proposed-action*apply*report-failure
   (state <s> ^name no-proposed-action
              ^operator.name report-failure
              ^top-state.interaction.status <status>)
-->
   (<status> ^failure.type no-proposed-action)
}

   
              
