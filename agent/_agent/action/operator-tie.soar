sp {action*operator-tie*propose*report-failure
   (state <s> ^superstate.problemspace.action.type execute
              ^attribute operator
              ^choices multiple)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-failure)
}

sp {action*operator-tie*apply*report-failure*action-tie
   (state <s> ^superstate.problemspace.action.type execute
              ^superstate.action-command
              ^attribute operator
              ^choices multiple
              ^operator.name report-failure
              ^topstate.interaction.status <status>)
-->
   (<status> ^failure.type action-tie)
}
   
sp {action*operator-tie*apply*report-failure*no-proposed-action
   (state <s> ^superstate.problemspace.action.type execute
              -^superstate.action-command
              ^attribute operator
              ^choices multiple
              ^operator.name report-failure
              ^topstate.interaction.status <status>)
-->
   (<status> ^failure.type no-proposed-action)
}
   
              
              