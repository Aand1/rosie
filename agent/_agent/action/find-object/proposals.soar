sp {find-object*propose*locate-object
   (state <s> ^name op_find-object
              ^object-desc <desc>
             -^found-object <obj>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name locate-object
        ^object-desc <desc>)
}

sp {find-object*propose*clear-object
   (state <s> ^name op_find-object
              ^found-object <obj>
             -^object-is-clear)
-->
   (<s> ^operator <o> + >)
   (<o> ^name clear-object
        ^object <obj>)
}

