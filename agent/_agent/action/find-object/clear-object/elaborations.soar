sp {clear-object*elaborate*found-object
   (state <s> ^name clear-object
              ^superstate.found-object <obj>)
-->
   (<s> ^found-object <obj>)
}

sp {clear-object*elaborate*move-metadata
   (state <s> ^name clear-object
              ^superstate.move-metadata <move>)
-->
   (<s> ^move-metadata <move>)
} 

sp {clear-object*elaborate*current-bin
   (state <s> ^name clear-object
              ^found-object.id <id>
              ^topstate.object <bin>
              ^topstate.prepositions.prep <on>)
   (<bin> ^id <bin-id>
          ^is-bin true)
   (<on> ^name on
         ^pair <p>)
   (<p>  ^a <id>
         ^b <bin-id>)
-->
   (<s> ^current-bin <bin>)
}

sp {clear-object*elaborate*blocking-object
   (state <s> ^name clear-object
              ^found-object.id <id>
              ^topstate.object <blocker>
              ^topstate.prepositions.prep <on>)
   (<blocker> ^id <blocker-id>)
   (<on> ^name on
         ^pair <p>)
   (<p>  ^a <blocker-id>
         ^b <id>)
-->
   (<s> ^blocking-object <blocker>)
}
   