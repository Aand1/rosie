sp {find-object*propose*move-object-to-goal
   (state <s> ^name op_find-object
              ^found-object <obj>
              ^goal-location <goal>
              ^move-metadata <move>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name perform-move-object
        ^moving-object <obj>
        ^destination <goal>
        ^_metadata <move>
        ^parameters.action-desc moving-to-goal)
}
        
   