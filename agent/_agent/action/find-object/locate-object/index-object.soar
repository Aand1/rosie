sp {locate-object*propose*index-parameter 
   (state <s> ^name locate-object
              ^indexable-object <i>)
   (<i> -^result)
-->
   (<s> ^operator <o> + >)
   (<o> ^name index-parameter
        ^parameter <i>)
}

sp {locate-object*index-parameter*remove*action*problemspace
   (state <s> ^name locate-object
              ^operator <o> +)
   (<o> ^name index-parameter)
-->
   (<s> ^problemspace action -)
}