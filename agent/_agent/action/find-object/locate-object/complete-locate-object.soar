sp {locate-object*propose*complete-locate-object
   (state <s> ^name locate-object
              ^indexable-object.result <obj>)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name complete-locate-object
        ^object <obj>)
}

sp {locate-object*apply*complete-locate-object*copy-result
   (state <s> ^name locate-object
              ^operator <o>
              ^superstate <ss>)
   (<o> ^name complete-locate-object
        ^object <obj>)
-->
   (<ss> ^found-object <obj>)
}

sp {locate-object*apply*complete-locate-object*clear*results
   (state <s> ^name locate-object
              ^operator.name complete-locate-object
              ^indexable-object <i>)
   (<i> ^result <res>)
-->
   (<i> ^result <res> -)
}