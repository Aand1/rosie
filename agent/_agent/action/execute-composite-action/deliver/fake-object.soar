sp {top-state*elaborate*world*soda
   (state <s> ^superstate nil
              ^world.objects <objs>)
-->
   (<objs> ^object <soda>)
   (<soda> ^id soda5
           ^predicates <preds>
           ^visible true
           ^real true)
   (<preds> ^status live
            ^color red1
            ^shape soda1)
}

sp {action*interrupt
   (state <s> ^superstate nil
              ^operator.name action)
-->
   (interrupt)
}