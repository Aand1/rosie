sp {top-state*elaborate*world*soda
   (state <s> ^superstate nil
              ^world.objects <objs>)
-->
   (<objs> ^object <soda>)
   (<soda> ^handle small-soda1 ^item-type object
           ^predicates <preds>
           ^visible true
           ^real true)
   (<preds> ^category block1 ^color red1 ^object-id small-soda1 ^shape soda1 ^size small1
            ^status live ^movable true)
}

sp {action*interrupt
   (state <s> ^superstate nil
              ^operator.name action)
-->
   (interrupt)
}