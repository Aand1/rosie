sp {action*elaborate*initiage-go1*location
   (state <s> ^problem-space.action <act>
              ^operator <o> +
              ^action-command <cmd>)
   (<o> ^name initiate-go1)
   (<cmd> ^arg2.{2} <loc>)
-->
   (<o> ^location <loc>)
}

sp {action*elaborate*initiate-go1*arg2*from*action-command
   (state <s> ^problem-space.action <act>
              ^operator <o> +
              ^top-state.maps.map.waypoint <wp>)
   (<o> ^name initiate-go1
        ^location.waypoint <wp-id>)
   (<wp> ^id <wp-id>)
-->
   (<o> ^arg2 <arg2>)
   (<arg2> ^arg-type waypoint
           ^id <wp>)
}
   
sp {action*elaborate*initiate-go1*arg2*unknown*waypoint
   (state <s> ^problem-space.action <act>
              ^operator <o> +)
   (<o> ^name initiate-go1
        ^location <loc>)
   (<loc> -^waypoint)
-->
   (<o> ^arg2 <arg2>)
   (<arg2> ^arg-type waypoint)
}

sp {action*elaborate*initiate-go1*arg2*unknown*waypoint*not*on*map
   (state <s> ^problem-space.action <act>
              ^operator <o> +
             -^top-state.maps.map.waypoint.id <wp-id>)
   (<o> ^name initiate-go1
        ^location.waypoint <wp-id>)
-->
   (<o> ^arg2 <arg2>)
   (<arg2> ^arg-type waypoint)
}
