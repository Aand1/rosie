sp {execute-put-down1*propose*report-success
   (state <s> ^name execute-put-down1
              ^current-action.command-status complete
              ^held-object.predicates.status live)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {execute-put-down1*apply*report-success
   (state <s> ^name execute-put-down1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}
