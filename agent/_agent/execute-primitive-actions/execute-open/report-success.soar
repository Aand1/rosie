sp {execute-open*propose*report-success
   (state <s> ^name execute-open1
              ^target-object.predicates.door1 open1)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {execute-open*apply*report-success*add*status
   (state <s> ^name execute-open1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}

sp {execute-open*apply*report-success*remove*command
   (state <s> ^name execute-open1
              ^operator.name report-success
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^open-command <cmd>)
   (<out> ^<cmd-name> <cmd>)
-->
   (<a> ^open-command <cmd> -)
   (<out> ^<cmd-name> <cmd> -)
}
              