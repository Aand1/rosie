sp {top-state*apply*execute-open1*send*open*command
   (state <s> ^top-state <top>
              ^operator <o>
              ^io.output-link <ol>)
   (<o> ^name execute-open1
        ^arg1.id.id <id>)
-->
   (<ol> ^set-state <set>)
   (<set> ^id <id>
          ^name door
          ^value open)
}

sp {top-state*apply*execute-open1*mark*current-action*complete
   (state <s> ^top-state <s>
              ^operator <o>
              ^current-action <a>)
   (<o> ^name execute-open1
        ^arg1.id.predicates.door open)
-->
   (<a> ^status complete)
}
