sp {execute-close*propose*report-success
   (state <s> ^name execute-close1
              ^target-object.predicates.door1 closed2)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {execute-close*apply*report-success*add*status
   (state <s> ^name execute-close1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}

sp {execute-close*apply*report-success*remove*command
   (state <s> ^name execute-close1
              ^operator.name report-success
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^close-command <cmd>)
   (<out> ^set-state <cmd>)
-->
   (<a> ^close-command <cmd> -)
   (<out> ^set-state <cmd> -)
}
              
