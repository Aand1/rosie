sp {go-to-next-waypoint*propose*face-waypoint
   (state <s> ^name go-to-next-waypoint
             -^status complete
             -^face-command <cmd>
              ^waypoint <wp>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name face-waypoint
        ^waypoint <wp>)
}

# For now don't use pose
sp {go-to-next-waypoint*apply*face-waypoint
   (state <s> ^name go-to-next-waypoint
              ^operator <o>
              ^top-state.world.robot.current-waypoint <cur-wp>
              ^top-state.io.output-link <out>)
   (<o> ^name face-waypoint
        ^waypoint <wp>)
   (<wp> ^x <x> ^y <y>)
   (<cur-wp> ^x <cx> ^y <cy>)
-->
   (<out> ^face-point <fp>)
   (<fp> ^cur-x <cx> ^cur-y <cy> ^x <x> ^y <y>)
   (<s> ^face-command <fp>)
}


   