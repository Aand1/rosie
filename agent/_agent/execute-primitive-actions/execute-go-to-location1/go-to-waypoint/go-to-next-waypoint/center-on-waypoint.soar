sp {go-to-next-waypoint*propose*center-on-waypoint*turn
   (state <s> ^name go-to-next-waypoint
              ^intermediate-turn <> none
              ^waypoint <wp>
              ^turn-command.status success
              ^drive-command.status success
             -^center-command)
-->
   (<s> ^operator <o> + >)
   (<o> ^name center-on-waypoint
        ^waypoint <wp>)
}

sp {go-to-next-waypoint*propose*center-on-waypoint*no*turn
   (state <s> ^name go-to-next-waypoint
              ^intermediate-turn none
              ^waypoint <wp>
              ^drive-command.status success
             -^center-command)
-->
   (<s> ^operator <o> + >)
   (<o> ^name center-on-waypoint
        ^waypoint <wp>)
}

sp {go-to-next-waypoint*apply*center-on-waypoint
   (state <s> ^name go-to-next-waypoint
              ^operator <o>
              ^io.input-link.self <self>
              ^io.output-link <out>)
   (<o> ^name center-on-waypoint
        ^waypoint.handle <handle>)
-->
   (<out> ^do-control-law <law>)
   (<law> ^name drive-to-tag
          ^parameters.class <handle>
          ^termination-condition.name stabilized)
   (<s> ^center-command <law>)
}
