sp {go-to-waypoint*prefer*go-to-next-waypoint*desired*waypoint
   (state <s> ^name go-to-waypoint
              ^operator <o> +
              ^desired.waypoint <wp>)
   (<o> ^name go-to-next-waypoint
         ^next-waypoint <wp>)
-->
   (<s> ^operator <o> >)
}

sp {go-to-waypoint*reject*go-to-next-waypoint*visited
   (state <s> ^name go-to-waypoint
              ^operator <o> +
              ^visited <wp>)
   (<o> ^name go-to-next-waypoint
        ^next-waypoint <wp>)
-->
   (<s> ^operator <o> -)
}

   