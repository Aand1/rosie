sp {execute-drive-to-waypoint1*propose*report-success
   (state <s> ^name execute-drive-to-waypoint1
              ^current-action.center-command.status success)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {execute-drive-to-waypoint1*propose*report-success
   (state <s> ^name execute-drive-to-waypoint1
              ^current-action.drive-command <cmd>)
   (<cmd> ^name drive-xy
          ^status success)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {execute-drive-to-waypoint1*prefer*attend
   (state <s> ^name execute-drive-to-waypoint1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change)
   (<o2> ^name report-success)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {execute-drive-to-waypoint1*apply*report-success
   (state <s> ^name execute-drive-to-waypoint1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}

sp {execute-drive-to-waypoint1*apply*report-success*cleanup*command
   (state <s> ^name execute-drive-to-waypoint1
              ^operator.name report-success
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^{ <cmd-type> << face-command drive-command turn-command center-command >> } <cmd>)
   (<out> ^<cmd-name> <cmd>)
-->
   (<a> ^<cmd-type> <cmd> -)
   (<out> ^<cmd-name> <cmd> -)
}
   