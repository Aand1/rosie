sp {execute-drive-to-waypoint1*propose*center-on-waypoint*turn
   (state <s> ^name execute-drive-to-waypoint1
              ^intermediate-turn <> none
              ^current-action <a>
              ^waypoint <wp>)
   (<a> ^turn-command.status success
        ^drive-command.status success
       -^center-command)
-->
   (<s> ^operator <o> + >)
   (<o> ^name center-on-waypoint
        ^waypoint <wp>)
}

sp {execute-drive-to-waypoint1*propose*center-on-waypoint*no*turn
   (state <s> ^name execute-drive-to-waypoint1
              ^intermediate-turn none
              ^current-action <a>
              ^waypoint <wp>)
   (<a> ^drive-command.status success
       -^center-command)
-->
   (<s> ^operator <o> + >)
   (<o> ^name center-on-waypoint
        ^waypoint <wp>)
}

sp {execute-drive-to-waypoint1*apply*center-on-waypoint
   (state <s> ^name execute-drive-to-waypoint1
              ^operator <o>
              ^current-action <a>
              ^io.input-link.self <self>
              ^io.output-link <out>)
   (<o> ^name center-on-waypoint
        ^waypoint.handle <handle>)
-->
   (<out> ^do-control-law <law>)
   (<law> ^name drive-to-tag
          ^parameters.class <handle>
          ^termination-condition.name stabilized)
   (<a> ^center-command <law>)
}
