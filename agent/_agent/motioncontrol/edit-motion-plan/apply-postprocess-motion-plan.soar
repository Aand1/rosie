sp {apply*postprocess-motion-plan
	(state <s> ^superstate nil 
			   ^name motion-control
               ^operator.name postprocess-motion-plan
               ^io <io>)
  	(<io> ^output-link <ol>)
    (<ol> ^plan <cmd>)
-->
	(<ol> ^plan <cmd> - 
		  ^postprocess <ex>)
 	(<ex> ^precision 0.5)
}

sp {apply*postprocess-motion-plan-if-continued
	(state <s> ^superstate nil 
			   ^name motion-control
               ^operator.name postprocess-motion-plan
               ^io <io>)
  	(<io> ^output-link <ol>)
    (<ol> ^continue <cmd>)
-->
	(<ol> ^continue <cmd> -)
}