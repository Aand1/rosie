sp {propose*postprocess-motion-plan*finished
	(state <s> ^name
               ^request
               ^topstate.io.output-link <ol>)
  	(<ol> ^plan <srch>)
    (<srch> ^status finished
            ^success true)
-->
	(<s> ^operator <op> + =)
 	(<op> ^name postprocess-motion-plan)
}

sp {propose*postprocess-motion-plan*stopped
	(state <s> ^name
               ^request
               ^topstate.io.output-link <ol>)
  	(<ol> ^plan <srch>)
    (<srch> ^status stopped
            ^success true)
-->
	(<s> ^operator <op> + =)
 	(<op> ^name postprocess-motion-plan)
}
 
sp {propose*halt-on-failure
	(state <s> ^name
               ^request
               ^topstate.io.output-link <ol>)
  	(<ol> ^plan <srch>)
    (<srch> ^success false)
-->
	(<s> ^operator <op> + =)
 	(<op> ^name halt-on-failure)
}

