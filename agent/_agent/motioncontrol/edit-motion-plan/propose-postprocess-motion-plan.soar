sp {propose*postprocess-motion-plan*finished
	(state <s> ^superstate nil
               ^name motion-control
               ^io.output-link <ol>)
  	(<ol> ^plan <srch>)
    (<srch> ^status finished
            ^success true)
-->
	(<s> ^operator <op> + =)
 	(<op> ^name postprocess-motion-plan)
}

sp {propose*postprocess-motion-plan*stopped
	(state <s> ^superstate nil
               ^name motion-control
               ^io.output-link <ol>)
  	(<ol> ^plan <srch>)
    (<srch> ^status stopped
            ^success true)
-->
	(<s> ^operator <op> + =)
 	(<op> ^name postprocess-motion-plan)
}
