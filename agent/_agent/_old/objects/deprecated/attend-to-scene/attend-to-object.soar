sp {attend-to-scene*propose*attend-to-object*specific
   (state <s> ^name attend-to-scene
              ^parameters.use-all-objects false
              ^parameters.use-object <obj>
             -^attended-to-object <obj>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name attend-to-object
        ^category perception
        ^object <obj>)
}

sp {attend-to-scene*propose*attend-to-object*all
   (state <s> ^name attend-to-scene
              ^parameters.use-all-objects true
              ^topstate.object <obj>
             -^attended-to-object <obj>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name attend-to-object
        ^category perception
        ^object <obj>)
}