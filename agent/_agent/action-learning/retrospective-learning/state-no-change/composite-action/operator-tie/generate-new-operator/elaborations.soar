sp {generate-new-operator*elaborate*object*candidates
   (state <s> ^name generate-new-operator
   	      ^superstate.superstate <ss>)
   (<ss> ^object <obj>)
   (<obj> ^focus <f>)
-->
   (<s> ^candidate <obj>)
}

sp {generate-new-operator*elaborate*relation*candidates
   (state <s> ^name generate-new-operator
   	      ^superstate.superstate <ss>)
   (<ss> ^desired <des>)
   (<des> ^track-relation <tr>)
   (<tr> ^relation <rel>)
-->
   (<s> ^candidate-relation <rel>)
}

sp {generate-new-operator*elaborate*action-space-id
   (state <s> ^name generate-new-operator
   	      ^superstate <ss>)
   (<ss> ^retrieved-action <ra>)
-->
   (<s> ^pairs <p>
	^retrieved-action <ra>)
}

sp {generate-new-operator*query*map-id
   (state <s> ^name generate-new-operator
   	      ^operator-definition <pid>)
-->
   (<s> ^smem-query-cue <cue>)
   (<cue> ^procedural <pid>)
}

sp {generate-new-operator*copy*map-id
   (state <s> ^name generate-new-operator
	      ^retrieved-lti-cue <rlc>
	      ^topstate.LTIstore <store>)
   (<rlc> ^cue <cue>
   	  ^retrieved-lti <ret>)
-->
   (<s> ^map-id <ret>)
   (<store> ^map-id <ret>)
}