sp {apply*generate-predicate*relation
   (state <s> ^name generate-desired
   	      ^operator <op>
	      ^desired <des>)
   (<op> ^name generate-predicates
   	 ^iterator <next>
	 ^predicate <pred>
	 ^attribute relation-predicate)
-->
   (<des> ^track-relation <pred>)
   (<pred> ^next <next>)
   (<s> ^iterator <next> -
   	^iterator <pred>)
}

sp {apply*generate-predicate*state
   (state <s> ^name generate-desired
   	      ^operator <op>
	      ^desired <des>)
   (<op> ^name generate-predicates
   	 ^iterator <next>
	 ^predicate <pred>
	 ^attribute state-predicate)
-->
   (<des> ^achieve-state <pred>)
   (<pred> ^next <next>)
   (<s> ^iterator <next> -
   	^iterator <pred>)
}
   	