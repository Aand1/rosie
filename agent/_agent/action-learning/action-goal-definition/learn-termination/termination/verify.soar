sp {verify*track-relation
   (state <s> ^name termination
   	      ^operator <op>
	      ^iterator <dtr>
	      ^superstate <ss>)
   (<op> ^name verify
   	 ^predicate <dtr>)
   (<ss> ^desired <des>
   	 ^track-relation <tr>)
   (<des> ^track-relation <dtr>)
   (<dtr> ^p1 <val1>
   	  ^p2 <val2>
	  ^relation <prep>)
   (<tr> ^object1 <val3>
   	 ^object2 <val4>
	 ^vpredicate <prep>
	 ^complete success)
   (<val1> ^focus <fval1>)
   (<val2> ^focus <fval2>)
   (<val3> ^focus <fval1>)
   (<val4> ^focus <fval2>)
   (<dtr> ^next <next>)
-->
   (<s> ^success <dtr>
   	^iterator <dtr> -
	^iterator <next>)
}

sp {verify*achieve-state
   (state <s> ^name termination
   	      ^operator <op>
	      ^iterator <dtr>
	      ^superstate <ss>)
   (<op> ^name verify
   	 ^predicate <dtr>)
   (<ss> ^desired <des>
   	 ^<state> <sobj>)
   (<des> ^achieve-state <dtr>)
   (<dtr> ^p1 <obj>
	  ^state <state>)
   (<sobj> ^focus <fval1>)
   (<obj> ^focus <fval1>)
   (<dtr> ^next <next>)
-->
   (<s> ^success <dtr>
   	^iterator <dtr> -
	^iterator <next>)
}