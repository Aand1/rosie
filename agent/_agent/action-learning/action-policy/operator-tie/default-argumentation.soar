sp {prefer*match*epmem*elaborate*spatial-rel*not-in-desired*store*action-space
   (state <s> ^name selection
   	      ^operator <o>
	      ^action-space-id.defaults <def>
	      ^indexed-action <ia>
	      -^copied-relation-from-desired <sop-arg>
	      ^smem.command <scomm>)
   (<o> -^not-matched <any>
   	^superoperator <sop>)
   (<ia> ^<arg2> <ia-val>
   	 ^name <ia-name>)
   (<ia-val> ^relation.smem-rep <rel>)
   (<sop> ^<arg2> <sop-arg>
   	  ^name <ia-name>)
   (<sop-arg> ^<arg>.focus <any-focus>)
-->
   (<def> ^name <ia-name>
   	  ^<arg2> <ia-arg>)
   (<ia-arg> ^relation <nrel>)
   (<nrel> ^smem-rep <rel>)
   (<scomm> ^store <ia-arg> <nrel> <def>)
}

sp {prefer*match*epmem*elaborate*spatial-rel*not-in-desired
   (state <s> ^name selection
   	      ^operator <o>
	      ^action-space-id.defaults <def>
	      -^copied-relation-from-desired <sop-arg>)
   (<o> -^not-matched <any>
   	^superoperator <sop>)
   (<sop> ^<arg2> <sop-arg>
   	  ^name <ia-name>)
   (<sop-arg> ^<arg>.focus <any-focus>)
   (<def> ^name <ia-name>
   	  ^<arg2> <ia-arg>)
   (<ia-arg> ^relation <nrel>)
   (<nrel> ^smem-rep <rel>)
-->
   (<sop-arg> ^relation <new-rel>)
   (<new-rel> ^smem-rep <rel>)
}