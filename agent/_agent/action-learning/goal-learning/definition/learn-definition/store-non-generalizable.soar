sp {store*goal-definition*p1*p2*attributes
   (state <s> ^name learn-definition
   	      ^goal-id.relation-predicate <grel>
	      ^goal.relation-predicate <relp>
	      ^smem.command <scomm>
	      ^pairing <pair>)
   (<pair> ^smem <grel>
   	   ^wmem <relp>)
   (<grel> ^{<arg> << p1 p2 >>}.default.value <val>)
   (<relp> ^<arg>.property <prop>)
   (<prop> ^label <label>
   	   ^name <name>)
-->
   (<val> ^<name> <label>)
   (<scomm> ^store <val>)
}

