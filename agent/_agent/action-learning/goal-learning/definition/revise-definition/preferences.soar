sp {prefer*generate-revisions
   (state <s> ^name revise-definition
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name generate-revisions)
   (<op2> ^name revise-predicate-definition)
-->
   (<s> ^operator <op1> > <op2>)
}

sp {worse*add-new-predicate
   (state <s> ^name revise-definition
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name revise-predicate-definition
   	  -^revision-details.type add-new-predicate)
   (<op2> ^name revise-predicate-definition
   	  ^revision-details.type add-new-predicate)
-->
   (<s> ^operator <op1> > <op2>)
}

sp {equal*revise-predicate-definition
   (state <s> ^name revise-definition
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name revise-predicate-definition
   	  -^revision-details.type add-new-predicate)
   (<op2> ^name revise-predicate-definition
   	  -^revision-details.type add-new-predicate)
-->
   (<s> ^operator <op1> = <op2>)
}

sp {equal*revise-predicate-definition*both*add-new-predicate
   (state <s> ^name revise-definition
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name revise-predicate-definition
   	  ^revision-details.type add-new-predicate)
   (<op2> ^name revise-predicate-definition
   	  ^revision-details.type add-new-predicate)
-->
   (<s> ^operator <op1> = <op2>)
}

sp {equal*generate-revisions
   (state <s> ^name revise-definition
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name generate-revisions)
   (<op2> ^name generate-revisions)
-->
   (<s> ^operator <op1> = <op2>)
}

sp {prefer*revised-predicate-definition
   (state <s> ^name revise-definition
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name revise-predicate-definition)
   (<op2> ^name complete-revise-definition)
-->
   (<s> ^operator <op1> > <op2>)
}

