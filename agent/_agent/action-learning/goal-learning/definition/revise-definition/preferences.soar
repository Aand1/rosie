sp {prefer*generate-revisions
   (state <s> ^name revised-definition
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name generate-revisions)
   (<op2> ^name revise-predicate-definition)
-->
   (<s> ^operator <op1> > <op2>)
}

sp {prefer*revised-predicate-definition
   (state <s> ^name revise-definition
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name revise-predicate-definition)
   (<op2> ^name complete-revise-definition)
-->
   (<s> ^operator <op1> > <op2>)
}

