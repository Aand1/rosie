sp {revise-definition*propose*relax-filters
   (state <s> ^name revise-predicate-definition
   	      ^change-default <cd>
	      ^predicate-definition <def>
	      ^grounded-predicate <gp>
	      ^partial-grounds <pg>
	      -^relaxed-filters <sval>)
   (<cd> ^argument <arg>
   	 ^features <ft>)
   (<def> ^<arg> <sval>)
   (<gp> ^<arg> <val>)
   (<pg> ^<arg> <val>)
-->
   (<s> ^operator <op> +, =)
   (<op> ^name relax-filters
   	 ^smem-object <sval>)
}

sp {revise-definition*propose*add-new-default
   (state <s> ^name revise-predicate-definition
   	      ^change-default <cd>
	      ^predicate-definition <def>
	      ^grounded-predicate <gp>
	      ^partial-grounds <pg>
	      -^added-new-default <sval>)
   (<cd> ^argument <arg>
   	 ^features <ft>)
   (<def> ^<arg> <sval>)
   (<gp> ^<arg> <val>)
   (<pg> ^<arg> {<oval> <> <val>})
-->
   (<s> ^operator <op> +, =)
   (<op> ^name add-new-default
   	 ^smem-object <sval>
	 ^default-value <ft>)
}

sp {revise-definition*propose*complete-revise-predicate-pefinition
   (state <s> ^name revise-predicate-definition)
-->
   (<s> ^operator <op> + <)
   (<op> ^name complete-revise-predicate-definition)
}

