sp {add-new-default*apply*add-new-default
   (state <s> ^name add-new-default
   	      ^superstate.operator <op>
	      ^smem.command <scomm>)
   (<op> ^name add-new-default
   	 ^default-value <val>
	 ^smem-object <sobj>)
-->
   (<sobj> ^default <def>)
   (<def> ^value <val>)
   (<s> ^default <def>)
   (<scomm> ^store <sobj> <def> <val>)
}

sp {add-new-default*apply*add-new-default*generalizable
   (state <s> ^name add-new-default
   	      ^default <def>
	      ^superstate.generalizable <gen>
	      ^smem.command <scomm>)
   (<gen> ^goal-argument <arg>
   	  ^features <f>)
-->
   (<def> ^<arg> <f>)
   (<scomm> ^store <def> <f>)
}