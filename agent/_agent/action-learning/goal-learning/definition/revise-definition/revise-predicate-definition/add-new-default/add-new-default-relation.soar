sp {add-new-default*relation*new*default-node*non-existant
   (state <s> ^name add-new-default
   	      ^goal-argument relation
	      ^default-value-node <node>
	      ^value non-existant
	      ^smem.command <scomm>)
-->
   (<node> ^default <def>)
   (<def> ^value non-existant
   	  ^p1 <nnode1>
	  ^p2 <nnode2>)
   (<scomm> ^store <node> <def> <nnode1> <nnode2>) 
   (<s> ^new-default-node <def>)
}


sp {add-new-default*relation*new*default-node*non-existant*p1*p2
   (state <s> ^name add-new-default
   	      ^goal-argument relation
	      ^new-default-node <def>
	      ^value non-existant
	      ^superstate <ss>
	      ^smem.command <scomm>)
   (<ss> ^predicate-definition <pdef>
   	 ^superstate <sss>)
   (<sss> ^revise-predicate-definition <rpd>)
   (<rpd> ^wmem-smem-pairs.<obj> <sobj>)
   (<pdef> ^{<arg> << p1 p2 >>} <sobj>)
   (<def> ^<arg> <nnode>)
-->
   (<s> ^<nnode> <obj>)
   (<scomm> ^store <node> <def> <nnode>) 
}

sp {add-new-default*relation*new*default-node
   (state <s> ^name add-new-default
   	      ^goal-argument relation
	      ^value.predicate <val>
	      ^default-value-node <dvn>
	      ^smem.command <scomm>
	      ^grounded-predicate <gp>)
   (<gp> ^p1 <val1>
   	 ^p2 <val2>)
-->
   (<dvn> ^default <def>)
   (<def> ^value <val>
   	  ^p1 <new-p1>
	  ^p2 <new-p2>)
   (<scomm> ^store <dvn> <def> <new-p1> <new-p2>)
   (<s> ^<new-p1> <val1>
   	^<new-p2> <val2>
	^new-default-node <def>)
}

sp {add-new-default*relation*argument*features
   (state <s> ^name add-new-default
   	      ^goal-argument relation
	      ^new-default-node <def>
	      ^<node> <val>
	      ^smem.command <scomm>)
   (<def> ^<< p1 p2 >> <node>)
   (<val> ^property <prop>)
   (<prop> ^label <label>
   	   ^name {<name> <> heat <> door})
-->
   (<node> ^<name> <label>)
   (<scomm> ^store <node>)
}