sp {add-new-default*relation*new*default-node
   (state <s> ^name add-new-default
   	      ^goal-argument relation
	      ^value.predicate <val>
	      ^default-value-node <dvn>
	      ^smem.command <scomm>
	      ^grounded-predicate <gp>)
   (<gp> ^p1 <val1>
   	 ^p2 <val2>)
-->
   (<dvn> ^default <def>)
   (<def> ^value <val>
   	  ^p1 <new-p1>
	  ^p2 <new-p2>)
   (<scomm> ^store <dvn> <def> <new-p1> <new-p2>)
   (<s> ^<new-p1> <val1>
   	^<new-p2> <val2>
	^new-default-node <def>)
}

sp {add-new-default*relation*argument*features
   (state <s> ^name add-new-default
   	      ^goal-argument relation
	      ^new-default-node <def>
	      ^<node> <val>
	      ^smem.command <scomm>)
   (<def> ^<< p1 p2 >> <node>)
   (<val> ^property <prop>)
   (<prop> ^label <label>
   	   ^name <name>)
-->
   (<node> ^<name> <label>)
   (<scomm> ^store <node>)
}