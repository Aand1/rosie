
#### add non existant default value
sp {add-new-default*store*non-existant*default
   (state <s> ^name add-new-default
   	      ^goal-argument {<garg> << p1 p2 >>}
	      ^default-value-node <node>
	      ^value non-existant
	      ^grounded-action.{<arg> <> name <> _metadata} <val>
	      -^ignore-set.attribute <arg>
	      ^smem.command <scomm>)
-->
   (<node> ^default <def>)
   (<def> ^value non-existant
   	  ^<arg> <nnode>)
   (<s> ^<nnode> <val>)
   (<scomm> ^store <node> <def>)
}

sp {add-new-default*store*object*default
   (state <s> ^name add-new-default
   	      ^goal-argument {<garg> << p1 p2 >>}
	      ^default-value-node <node>
	      ^value {<gval> <> non-existant}
	      ^grounded-action.{<arg> <> name <> _metadata} <val>
	      -^ignore-set.attribute <arg>
	      ^smem.command <scomm>)
-->
   (<node> ^default <def>)
   (<def> ^value <sval>
   	  ^<arg> <nnode>)
   (<s> ^<nnode> <val>
   	^<sval> <gval>)
   (<scomm> ^store <node> <def>)
}

sp {add-new-default*store*non-existant*default*features
   (state <s> ^name add-new-default
   	      ^<node> <val>
	       ^goal-argument {<garg> << p1 p2 >>}
	      ^default-value-node.default.<darg> <node>
	      ^smem.command <scomm>)
   (<val> ^property <prop>)
   (<prop> ^label <label>
   	   ^name {<name> <> door <> heat})
-->
   (<node> ^<name> <label>)
   (<scomm> ^store <node>)
}
