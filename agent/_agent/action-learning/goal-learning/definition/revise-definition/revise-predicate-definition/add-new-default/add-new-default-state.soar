sp {add-new-default*store*non-existant*default
   (state <s> ^name add-new-default
   	      ^goal-argument state
	      ^default-value-node <node>
	      ^value non-existant
	      ^superstate <ss>
	      ^smem.command <scomm>)
   (<ss> ^predicate-definition <pdef>
   	 ^superstate <sss>)
   (<sss> ^revise-predicate-definition <rpd>)
   (<rpd> ^wmem-smem-pairs.<obj> <sobj>)
   (<pdef> ^p1 <sobj>)
-->
   (<node> ^default <def>)
   (<def> ^value non-existant
   	  ^p1 <nnode>)
   (<s> ^<nnode> <wobj>)
   (<scomm> ^store <node> <def>)
}

sp {add-new-default*store*non-existant*default*properties
   (state <s> ^name add-new-default
   	      ^goal-argument state
	      ^default-value-node.default.p1 <nnode>
	      ^<nnode> <obj>
	      ^smem.command <scomm>)
   (<obj> ^property <prop>)
   (<prop> ^label <label>
   	   ^name {<name> <> door <> heat})
-->
   (<nnode> ^<name> <label>)
   (<scomm> ^store <nnode>)
}
