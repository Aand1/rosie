sp {add-new-default*store*non-existant*default
   (state <s> ^name add-new-default
   	      ^goal-argument state
	      ^default-value-node <node>
	      ^value non-existant
	      ^superstate <ss>
	      ^smem.command <scomm>)
   (<ss> ^predicate-definition <pdef>
   	 ^superstate <sss>)
   (<sss> ^goal-definition <gid>
   	  ^revise-predicate-definition <rpd>)
   (<pdef> ^p1 <p1node>)
   (<gid> ^<predicate> {<pdef2> <> <pdef>})
   (<rpd> ^failure-details.predicate-definition <pdef2>
   	  ^grounded-predicate <gp>)
   (<pdef2> ^<arg> <p1node>)
   (<gp> ^<arg> <wobj>)
-->
   (<node> ^default <def>)
   (<def> ^value non-existant
   	  ^p1 <nnode>)
   (<s> ^<nnode> <wobj>)
   (<scomm> ^store <node> <def>)
}

sp {add-new-default*store*non-existant*default*properties
   (state <s> ^name add-new-default
   	      ^goal-argument state
	      ^default-value-node.default.p1 <nnode>
	      ^<nnode> <obj>
	      ^smem.command <scomm>)
   (<obj> ^property <prop>)
   (<prop> ^label <label>
   	   ^name {<name> <> door <> heat})
-->
   (<nnode> ^<name> <label>)
   (<scomm> ^store <nnode>)
}
