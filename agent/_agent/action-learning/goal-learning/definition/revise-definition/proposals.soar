sp {revise-definition*propose*generate*revisions    
   (state <s> ^name revise-definition
	      ^grounded-goal.<type> <rel>
	      ^grounded-action <ga>
	      ^goal-definition <gdef>)
  -{(<s> ^revise-predicate-definition <rpd>)
    (<rpd> ^grounded-predicate <rel>)}
-->
   (<s> ^operator <op> +)
   (<op> ^name generate-revisions
   	 ^grounded-predicate <rel>
	 ^goal-definition <gdef>
	 ^grounded-action <ga>)
}

sp {revise-definition*propose*revise-predicate-definition
   (state <s> ^name revise-definition
   	      ^revise-predicate-definition <pred>
	      -^revised-predicate-definition <pred>
	      ^grounded-action <ga>
	      ^topstate.agent-params.use-default-values true)
-->
   (<s> ^operator <op> +)
   (<op> ^name revise-predicate-definition
   	 ^revision-details <pred>
	 ^grounded-action <ga>)
}

sp {revise-definiton*propose*revise-predicate-definition*non-existant
   (state <s> ^name revise-definition
   	      ^revise-predicate-definition-non-existant <pred>
	      -^revised-predicate-definition <pred>
	      ^grounded-action <ga>
	      ^topstate.agent-params.use-default-values true)
-->
   (<s> ^operator <op> + <)
   (<op> ^name revise-predicate-definition
   	 ^revision-details <pred>
	 ^grounded-action <ga>)
}

sp {revise-definition*propose*complete-revise-definition
   (state <s> ^name revise-definition)
-->
   (<s> ^operator <op> + <)
   (<op> ^name complete-revise-definition)
}
