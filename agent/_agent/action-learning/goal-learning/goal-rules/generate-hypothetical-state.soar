### copy objects
sp {copy*objects*goal-rules
   (state <s> ^name goal-rules
   	      ^operator.name generate-hypothetical-state
   	      ^superstate.object <obj>
	      ^duplicates <dup>)
-->
   (<s> ^object <nobj>)
   (<dup> ^<obj> <nobj>)
}


## hypothesize the desired state
sp {copy*relation-predicate
   (state <s> ^name goal-rules
   	      ^topstate-indexed-action.desired <des>
   	      ^operator.name generate-hypothetical-state
	      ^environment <env>
	      ^object <nobj1>
	      ^object <nobj2>
	      ^duplicates <dup>
	      ^topstate.found-prep <rel>)
   (<des> ^relation-predicate <pred>)
   (<pred> ^relation <rel>
   	   ^p1 <obj1>
	   ^p2 <obj2>)
   (<dup> ^<obj1> <nobj1>
   	  ^<obj2> <nobj2>)
-->
   (<env> ^predicate <nprep>)
   (<nprep> ^a <nobj1>
   	    ^b <nobj2>
	    ^relation <rel>)
}

### copy state predicate
sp {copy*state-predicate
   (state <s> ^name goal-rules
   	      ^topstate-indexed-action.desired <des>
   	      ^operator.name generate-hypothetical-state
	      ^environment <env>
	      ^duplicates <dup>)
   (<dup> ^<obj> <nobj>)
   (<des> ^state-predicate <dtr>)
   (<dtr> ^p1 <obj>
   	  ^state <state>)
-->
   (<env> ^<state> <nobj>)
}


