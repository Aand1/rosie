#### generate goal elaborate state ##############

sp {generate-goal*elaborate*ltids
   (state <s> ^name generate-goal
   	      ^superstate.operator <sop>)
   (<sop> ^name generate-goal
   	  ^goal-id <glid>
	  ^procedural-id <pid>)
-->
   (<s> ^goal-id <glid>
   	^procedural-id <pid>)
}

sp {generate-goal*elaborate*grounded-action
   (state <s> ^name generate-goal   
   	      ^superstate.action <ia>)
-->
   (<s> ^grounded-action <ia>)
}



sp {generate-goal*elaborate*failure
   (state <s> ^name generate-goal
   	      ^goal-id <gid>)
-->
   (<s> ^failure <f>)
   (<f> ^goal-definition <gid>)
}
   	     

#### retrieve the goal definiton
sp {generate-goal*retrieve*goal-definition
   (state <s> ^name generate-goal
   	      ^goal-id <gid>
	      -^retrieved-lti <gid>)
   (<gid> -^<attr>)
-->
   (<s> ^nretrieve-lti <gid>)
}

#### if the goal ltid does not have any substructure, Rosie does not know the goal definition
sp {generate-goal*elaborate*failure*unknown-goal
   (state <s> ^name generate-goal
   	      ^goal-id <gid>
	      ^retrieved-lti <gid>
	      ^failure <f>)
   (<gid> -^<attr>)
-->
   (<f> ^type unknown-goal)
}


