sp {generate-goal*propose*complete-generate-goal*elaborate-goal
   (state <s> ^name generate-goal
   	      ^desired <d>
	      ^superstate <ss> 
	      ^operator.name complete-generate-goal)
   (<ss> ^action <ia>
   	 ^superstate <sss>)
   (<ia> ^name <name>
   	 ^_metadata.argument-count <arg-count>)
-->
    (<ss> ^desired-state <d>)
}

sp {generate-goal*propose*elaborate-goal
   (state <s> ^desired-state <d>
   	      ^superstate <ss>)
   (<ss> ^indexed-action <ia>)
-->
   (<ss> ^operator <op> + >)
   (<op> ^name elaborate-goal
   	 ^desired <d>
	 ^indexed-action <ia>)
}

sp {generate-goal*apply*complete-generate-goal*single
   (state <s> ^operator <op>
   	      ^indexed-action <ia>)
   (<op> ^name elaborate-goal
   	 ^desired <d>)
-->
   (<ia> ^desired <d>)
   (<s> ^operator <op> -)
}

sp {generate-goal*apply*complete-generate-goal*multiple
   (state <s> ^operator <op>
   	      ^indexed-action <ia>
	      ^operator {<op2> <> <op>} +)
   (<op> ^name elaborate-goal
   	 ^desired <d>)
   (<op2> ^name elaborate-goal)
-->
   (<ia> ^desired <d>)
   (<s> ^operator <op> -
   	^operator <op2> -)
}

sp {generate-goal*prefer*elaborate-goal*equal
   (state <s> ^operator <op1> +
   	      ^operator {<op2> > <op1>} +)
   (<op1> ^name elaborate-goal
   	  ^desired <d1>)
   (<op2> ^name elaborate-goal
   	  ^desired <d2>)
-->
   (<s> ^operator <op1> > <op2>)
}

sp {elaborate-goal*lookahead*required
   (state <s> ^look-ahead-operator <lop>
   	      ^operator <op> +)
   (<op> ^name elaborate-goal
   	 ^indexed-action <lop>)
-->
   (<s> ^operator <op> !)
}

sp {elaborate-goal*primitive*better
   (state <s> ^operator <op1> +
   	      ^operator <op2> +)
   (<op2> ^name elaborate-goal)
   (<op1> ^action-type << primitive composite >>)
-->
   (<s> ^operator <op2> > <op1>)
}
   	  