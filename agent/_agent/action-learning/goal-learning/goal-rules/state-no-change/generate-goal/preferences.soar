sp {generate-goal*prefer*generate-grounded-predicate*smaller-id
   (state <s> ^name generate-goal
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name generate-grounded-predicate
   	  ^definition <id1>)
   (<op2> ^name generate-grounded-predicate
   	  ^definition {<id2> > <id1>})
-->
   (<s> ^operator <op1> > <op2>)
}

sp {generate-goal*prefer*process-failure    
   (state <s> ^name generate-goal
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name complete-generate-goal)
   (<op2> ^name process-failure)
-->
   (<s> ^operator <op1> < <op2>)
}
   