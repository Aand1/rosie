### if failure occured because Rosie does not know the goal definition, report status to topstate

sp {generate-goal*report-status*unknown-goal
   (state <s> ^name generate-goal
   	      ^operator <op>
	      ^failure <det>
	      ^goal-id <gid>
	      ^topstate.interaction.status <stat>)
   (<op> ^name process-failure)
 -->
   (<stat> ^failure <f>)
   (<f> ^type failure
   	^information unknown-goal
	^details <det>)
}


sp {generate-goal*report-status*overspecific-argument
   (state <s> ^name generate-goal
   	      ^operator <op>
	      ^failure <f>
	      ^goal-id <gid>
	      ^topstate.interaction.status <stat>)
   (<op> ^name process-failure)
   (<f> ^details <det>)
   (<det> ^type overspecific-definition)
-->
   (<stat> ^failure <f>)
   (<f> ^type failure	
   	^information unknown-goal
	^details <det>
	^goal-id <gid>)
}

  