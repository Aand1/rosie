### failure because previous definition is too specific
sp {generate-grounded-predicate*process*failure*definition*too*specific
   (state <s> ^name generate-grounded-predicate
	      ^definition <def>
	      ^predicate-type <type>
	      ^operator.name process-failure)
-->
   (<s> ^predicate-grounding-failure <pgf>)
   (<pgf> ^type overspecific-definition 
          ^predicate-definition <def>
	  ^predicate-type <type>)
}

sp {generate-grounded-predicate*process*failure*partial-grounds
   (state <s> ^name generate-grounded-predicate
   	      ^assignment.evaluation.grounded-predicate <gp>
	      ^operator.name process-failure
	      ^predicate-grounding-failure <pgf>)
-->
   (<pgf> ^partial-grounds <gp>)
}

sp {generate-grounded-predicate*process*failure*partial-grounds*superstate
   (state <s> ^name generate-grounded-predicate
   	      ^superstate.failure <f>
	      ^predicate-grounding-failure <pgf>)
   (<pgf> ^partial-grounds)
-->
   (<f> ^predicate-grounding-failure <pgf>)
}