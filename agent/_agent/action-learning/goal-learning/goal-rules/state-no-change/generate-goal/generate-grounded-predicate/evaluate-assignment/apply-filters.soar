####### filters for objects 

sp {evaluate-assignment*apply*filter*0*attribute
   (state <s> ^name evaluate-assignment
   	      ^operator.name apply-filters
	      ^pair <pair>
	      ^<pair> <f>)  
   (<pair> ^wmem-object <wobj>)
   (<f> -^<any> <label>)
-->
   (<s> ^matched <pair>)
}

sp {evaluate-assignment*apply*filter*1*attribute
   (state <s> ^name evaluate-assignment
   	      ^operator.name apply-filters
	      ^pair <pair>
	      ^<pair> <f>)  
   (<pair> ^wmem-object <wobj>)
   (<f> ^<attr1> <label1>
   	-^<attr2> {<label2> <> <label1>})
   (<wobj> ^property <prop>)
   (<prop> ^label <label1>
   	   ^name <attr1>)
-->
   (<s> ^matched <pair>)
}

sp {evaluate-assignment*apply*filter*2*attribute
   (state <s> ^name evaluate-assignment
   	      ^operator.name apply-filters
	      ^pair <pair>
	      ^<pair> <f>)  
   (<pair> ^wmem-object <wobj>)
   (<f> ^<attribute1> <label1>
   	^<attribute2> {<label2> <> <label1>}
	-^<attribute> {<label3> <> <label1> <> <label2>})
   (<wobj> ^property <prop1>
   	   ^property <prop2>)
   (<prop1> ^label <label1>
   	    ^name <attribute1>)
   (<prop2> ^label <label2>
   	    ^name <attribute2>)
-->
   (<s> ^matched <pair>)
}

sp {evaluate-assignment*apply*filter*3*attribute
   (state <s> ^name evaluate-assignment
   	      ^operator.name apply-filters
	      ^pair <pair>
	      ^<pair> <f>)  
   (<pair> ^wmem-object <wobj>)
   (<f> ^<attribute1> <label1>
   	^<attribute2> {<label2> <> <label1>}
	^<attribute3> {<label3> <> <label1> <> <label2>}
	-^<attribute> {<label4> <> <label3> <> <label1> <> <label2>})
   (<wobj> ^property <prop1>
   	   ^property <prop2>
	   ^property <prop3>)
   (<prop1> ^label <label1>
   	    ^name <attribute1>)
   (<prop2> ^label <label2>
   	    ^name <attribute2>)
   (<prop3> ^label <label3>
   	    ^name <attribute3>)
-->
   (<s> ^matched <pair>)
}

sp {evaluate-assignment*apply*filter*4*attribute
   (state <s> ^name evaluate-assignment
   	      ^operator.name apply-filters
	      ^pair <pair>
	      ^<pair> <f>)  
   (<pair> ^wmem-object <wobj>)
   (<f> ^<attribute1> <label1>
   	^<attribute2> {<label2> <> <label1>}
	^<attribute3> {<label3> <> <label1> <> <label2>}
	^<attribute4> {<label4> <> <label3> <> <label1> <> <label2>}
	-^<attribute5> {<label5> <> <label4> <> <label3> <> <label1> <> <label2>})
   (<wobj> ^property <prop1>
   	   ^property <prop2>
	   ^property <prop3>
	   ^property <prop4>)
   (<prop1> ^label <label1>
   	    ^name <attribute1>)
   (<prop2> ^label <label2>
   	    ^name <attribute2>)
   (<prop3> ^label <label3>
   	    ^name <attribute3>)
   (<prop4> ^label <label4>
   	    ^name <attribute4>)
-->
   (<s> ^matched <pair>)
}

