sp {generate-grounded-predicate*propose*evaluate*defaults
   (state <s> ^name generate-grounded-predicate
   	      ^assignment <dva>)
   (<dva> -^evaluation)
-->
   (<s> ^operator <op> + =)
   (<op> ^name evaluate-assignment
   	 ^assignment <dva>)
}
 
sp {generate-grounded-predicate*propose*process-failure
   (state <s> ^name generate-grounded-predicate
   	      ^grounding failure)
-->
   (<s> ^operator <op> + <)
   (<op> ^name process-failure)
}

### complete-generate-grounded-predicate
sp {generate-grounded-predicate*propose*complete-generate-grounded-predicate
   (state <s> ^name generate-grounded-predicate
   	      ^assignment <da>)
   (<da> ^evaluation.evaluation successful)
-->
   (<s> ^operator <op> + <)
   (<op> ^name complete-generate-grounded-predicate
   	 ^assignment <da>)
}