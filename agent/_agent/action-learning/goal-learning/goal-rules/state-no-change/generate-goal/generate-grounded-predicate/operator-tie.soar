sp {operator-tie*generate-grounded-predicate
   (state <s> ^choices multiple
   	      ^attribute operator
	      ^superstate <ss>)
   (<ss> ^operator <op1> +
	 ^operator {<op2> <> <op1>} +
	 ^definition <def>)
   (<op1> ^assignment <asg1>)
   (<op2> ^assignment <asg2>)
   (<asg1> -^{<any> <> evaluation})
-->
   (<ss> ^operator <op1> >)
}

sp {operator-tie*generate-grounded-predicate*not-required
   (state <s> ^choices multiple
   	      ^attribute operator
	      ^superstate <ss>)
   (<ss> ^operator <op1> +
	 ^operator {<op2> <> <op1>} +)
   (<op1> ^assignment.evaluation.grounded-predicate not-required)
 -->
   (<ss> ^operator <op1> >)
}

sp {operator-tie*generate-grounded-predicate*not-required*equal
   (state <s> ^choices multiple
   	      ^attribute operator
	      ^superstate <ss>)
   (<ss> ^operator <op1> +
	 ^operator {<op2> <> <op1>} +)
   (<op1> ^assignment.evaluation.grounded-predicate not-required)
   (<op2> ^assignment.evaluation.grounded-predicate not-required)
 -->
   (<ss> ^operator <op1> = <op2>)
}