#####################################################
sp {verify*track-relation*in-focus
   (state <s> ^name learn-termination-rule
   	      ^operator <op>
	      ^iterator <dtr>
	      ^superstate <ss>)
   (<op> ^name verify
   	 ^predicate <dtr>)
   (<ss> ^desired <des>
   	 ^environment.predicate <tr>
	 ^in-focus <if>)
   (<des> ^track-relation <dtr>
   	  ^in-focus <dif>)
   (<dtr> ^p1 <val1>
   	  ^p2 <val2>
	  ^relation <prep>)
   (<tr> ^a <val3>
   	 ^b <val4>
	 ^relation <prep>)
   (<dif> ^<fval1> <val1>)
   (<dif> ^<fval2> <val2>)
   (<if> ^<fval1> <val3>)
   (<if> ^<fval2> <val4>)
   (<dtr> ^next <next>)
-->
   (<s> ^success <dtr>
   	^iterator <dtr> -
	^iterator <next>)
}

sp {verify*achieve-state*in-focus
   (state <s> ^name learn-termination-rule
   	      ^operator <op>
	      ^iterator <dtr>
	      ^superstate <ss>)
   (<op> ^name verify
   	 ^predicate <dtr>)
   (<ss> ^desired <des>
   	 ^environment.<state> <sobj>
	 ^in-focus <if>)
   (<des> ^achieve-state <dtr>
   	  ^in-focus <dif>)
   (<dtr> ^p1 <obj>
	  ^state <state>)
   (<dif> ^<fval1> <obj>)
   (<if> ^<fval2> <sobj>)
   (<dtr> ^next <next>)
-->
   (<s> ^success <dtr>
   	^iterator <dtr> -
	^iterator <next>)
}