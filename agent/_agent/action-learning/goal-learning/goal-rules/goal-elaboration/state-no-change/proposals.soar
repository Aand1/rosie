sp {learn-desired-elaboration-rule*propose*assign-goal
   (state <s> ^name learn-desired-elaboration-rule
   	      ^indexed-action <ia>)
   (<ia> ^_metadata.map-id <m>
   	 -^desired)
   (<m> ^goal <gid>
   	^procedural <pid>)
-->
   (<s> ^operator <op> +)
   (<op> ^name assign-goal
   	 ^goal-id <gid>
	 ^procedural-id <pid>)
}
