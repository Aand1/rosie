sp {state-no-change*indexed-action*not-proposed
   (state <s> ^attribute state
   	      ^impasse no-change
	      ^superstate.indexed-action <ia>
	      ^problemspace <ps>)
-->
   (<s> ^name learn-desired-elaboration-rule
   	^indexed-action <ia>)
   (<ps> ^name learning)
}


#### write result
sp {assign-goal*indexed-action*write-result
   (state <s> ^name assign-goal
   	      ^superstate.indexed-action <ia>
	      ^desired <d>
	      ^operator.name complete-assign-goal)
-->
   (<ia> ^desired <d>)
}

sp {assign-goal*indexed-action*retract*goal-elaboration
   (state <s> ^name assign-goal
   	      ^superstate.indexed-action <ia>
	      ^desired <d>
	      ^operator.name complete-assign-goal
	      ^superstate.superstate.superstate <sss>)
-->
   (<sss> ^learned desired-elaboration)
}


 