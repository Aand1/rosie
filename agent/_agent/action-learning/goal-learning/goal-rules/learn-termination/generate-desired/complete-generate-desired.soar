sp {apply*complete-generate-desired*list
   (state <s> ^name generate-desired
   	      ^operator <op>
	      ^iterator <next>)
   (<op> ^name complete-generate-desired
   	 ^desired <des>)
-->
   (<des> ^list <list>)
   (<list> ^next <next>)
}

sp {apply*complete-generate-desired*superstate
   (state <s> ^name generate-desired
   	      ^operator <op>
	      ^superstate <ss>)
   (<op> ^name complete-generate-desired
   	 ^desired <des>)
  (<des> ^list <list>)
-->
   (<ss> ^desired <des>)
}