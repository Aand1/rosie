### propose initialize-iterator
sp {generate-desired*propose*initialize*list*iterator
   (state <s> ^name generate-desired
   	      -^iterator)
-->
   (<s> ^operator <op> + >)
   (<op> ^name initialize-iterator)
}


### propose generate goal-predicates
sp {propose*generate-predicates*relation
   (state <s> ^name generate-desired
   	      ^segment-desired <seg-des>
	      ^iterator <next>)
   (<seg-des> ^{<< relation-predicate state-predicate >> <attr>} <pred>)
   (<pred> ^_instantiation-of <rep>
   	   -^next)
-->
   (<s> ^operator <op> +)
   (<op> ^name generate-predicates
   	 ^iterator <next>
	 ^predicate-definition <rep>
	 ^attribute <attr>
	 ^predicate <pred>)
}
   

### propose complete generate-desired
sp {propose*complete-generate-desired
   (state <s> ^name generate-desired
   	      ^desired <des>)
-->
   (<s> ^operator <op> < +)
   (<op> ^name complete-generate-desired
   	 ^desired <des>)
}
