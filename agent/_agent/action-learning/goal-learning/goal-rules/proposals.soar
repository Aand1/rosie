### propose generate-hypothetical-state
sp {rules*propose*generate-hypothetical-state
   (state <s> ^name goal-rules
   	      -^object)
-->
   (<s> ^operator <op> + >)
   (<op> ^name generate-hypothetical-state)
}

### propose goal-elaboration
sp {rules*propose*goal-elaboration
   (state <s> ^name goal-rules
   	      -^learned desired-elaboration)
-->
   (<s> ^operator <op> +)
   (<op> ^name goal-elaboration)
}

### propose termination
sp {rules*propose*termination
   (state <s> ^name goal-rules
	      -^success <d>
	      ^learned desired-elaboration)
-->
   (<s> ^operator <op> +)
   (<op> ^name termination)
}
