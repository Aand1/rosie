sp {topstate*elaborate*robot-state
   (state <s> ^topstate <s>)
-->
   (<s> ^robot-state <rs>)
   (<rs> ^arm <arm>)
}

sp {anystate*elaborate*robot-state
   (state <s> ^io <io>
              ^superstate.robot-state <rs>)
-->
   (<s> ^robot-state <rs>)
}

sp {anystate*elaborate*grabbed
   (state <s> ^robot-state.arm <arm>)
   (<arm> ^grabbed <obj>)
-->
   (<s> ^grabbed <obj>)
}