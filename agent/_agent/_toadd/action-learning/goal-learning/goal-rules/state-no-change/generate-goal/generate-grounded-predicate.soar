sp {generate-goal*apply*generate-grounded-predicate
   (state <s> ^name generate-goal   
   	      ^operator <op>
	      ^desired <d>)
   (<op> ^name generate-grounded-predicate
   	 ^definition <val>)
   (<d> ^<< track-relation achieve-state >> <pred>)
   (<pred> ^_instantiation-of <val>)
   (<d> ^list <list>)
   (<list> ^next <next>)
-->
   (<list> ^next <next> -
   	   ^next <pred>)
   (<pred> ^next <next>)
}
