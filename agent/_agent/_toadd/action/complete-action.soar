sp {action*application*propose*complete-action
   (state <s> ^problemspace.action <act>
              ^superstate <ss>
              ^action-status << complete failure >>)
   (<ss> ^operator <sop>
         ^selected-action <sop>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name complete-action)
}

sp {action*application*apply*complete-action*remove*selected-action
   (state <s> ^problemspace.action <act>
              ^operator.name complete-action
              ^superstate <ss>)
   (<ss> ^operator <sop>
         ^selected-action <sop>)
-->
   (<ss> ^selected-action <sop> -)
}
   