sp {board-between*propose*complete
   (state <s> ^name board-between
              ^instance <instance>)
              #^superstate.operator.name board-between
              #^result.set <res>)
#   -{(<s> ^instance <s1>)
#     (<s1> ^c.object <p>)
#     (<res> -^instance <s1>)}
-->
   (<s> ^operator <o> + =)
   (<o> ^name complete
        ^instance <instance>)
}

#search control hack
sp {board-between*perference*complete*samex
   (state <s> ^name board-between
              ^operator <o1> +)
   (<o1> ^name complete ^instance <instance>)
   (<instance> ^2 <obja> ^3 <objb>)
   (<obja> ^y <x> ^x <y>)
   (<objb> ^y <x> -^x <y>)
-->
   (<s> ^operator <o1> >)
}

