sp {learn-predicate*elaborate*diff-table
   (state <s> ^name learn-predicate)
-->
    (<s> ^diff-table <dtt>)
    (<dtt> ^diff <d1> <d2> <d3> <d4> <d5> <d6> <d7> <d8> <d9> <d10>)
    (<d1> ^num 2 1 ^result 1)
    (<d2> ^num 3 1 ^result 2)
    (<d3> ^num 4 1 ^result 3)
    (<d4> ^num 5 1 ^result 4)
    (<d5> ^num 3 2 ^result 1)
    (<d6> ^num 4 2 ^result 2)
    (<d7> ^num 5 2 ^result 3)
    (<d8> ^num 4 3 ^result 1)
    (<d9> ^num 5 3 ^result 2)
    (<d10> ^num 5 4 ^result 1)
}

