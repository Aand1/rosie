###DEPRICATED
sp {DEPRICATEDtransfer-predicate-structure*apply*complete*next-stored-sentence
:interrupt
   (state <s> ^name transfer-predicate-structure
              ^operator.name complete
              ^superstate.superstate.game <game>
              ^top-state.dialog-context <dc>
              ^nlp-relationships <gr>
              ^nlp-functions <nf>
              ^smem.result.retrieved <pred> 
              ^top-state <ts>)
   (<ts> ^stored-sentence <sent> ^io.input-link.language.sentence <se>)
   (<pred> ^predicate-structure <ps> ^handle <name> )
   (<se> ^next <n1> ^complete-sentence <cs>)
   (<dc> ^learn-link <ll> ^input-object-1 <io> -^input-object-2)
   #(<ll> ^handle <name>)
-->
    (<dc> ^learn-link <ll> -)
    (<dc> ^input-object-1 <io> -)
    #try attaching to <dc>?
    (<ts> ^next-stored-sentence <st>)
    (<st> ^complete-sentence <cs> ^next (deep-copy <n1>) ^spelling *)#^sentence-number (+ 3 <sn>) 
}
