sp {test-goal-demonstration*propose*state-diff-towers
   (state <s> ^superstate.name wait-for-responseo)
-->
# (interrupt)
   (<s> ^operator <op> + >)
   (<op> ^name state-diff
        ^initial <istate>
        ^ignore <att>
        ^attend-only <ao>
        ^final <fstate>)
   (<ao> ^objects.object <o1> <o3> <o2> <o4>)#^relationships.relation below 
   (<att> ^object <o1> ^relation on)
   (<istate> ^relationships <rels> ^objects <os> ^object-desc-set <ods>)
   (<fstate> ^relationships <rels2> ^objects <os2> ^object-desc-set <ods2>)
   
  (<os> ^object <o1> <o2> <o3>)
  (<o1> ^id 1)
  (<o2> ^id 2)
  (<ods> ^obj-desc <od1> <od2>)
  (<od1> ^id 1)
  (<od2> ^id 2)
  (<rels> ^relations <r1> <r2>)
  (<r1> ^name on ^instance <p1> <p1a>)
  (<p1> ^1 1 ^2 2)
  (<p1a> ^1 2 ^2 4)
  (<r2> ^name above ^instance <p2>)
  (<p2> ^1 2 ^2 1)
  
  (<os2> ^object <o1> <o2> <o3> <o4>)
  (<o3> ^id 3)
  (<o4> ^id 4)
  (<ods2> ^obj-desc <od22> <od12> <od32> <od42>)
  (<od12> ^id 1)
  (<od22> ^id 2)
  (<od32> ^id 3)
  (<od42> ^id 4 ^word grid-blue)
  (<rels2> ^relations <r3> <r3a>)
  (<r3> ^name on ^instance <p5> <p3> <p4>)
  (<p5> ^1 3 ^2 2)
  (<p3> ^1 2 ^2 1)
  (<p4> ^1 1 ^2 4)
  
  (<r3a> ^name below ^instance <p5a> <p3a> <p4a>)
  (<p5a> ^2 3 ^1 2)
  (<p3a> ^2 2 ^1 1)
  (<p4a> ^2 1 ^1 4)
 
 
}

