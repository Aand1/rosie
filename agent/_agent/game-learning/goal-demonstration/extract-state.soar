sp {extract-state*propose*copy-predicates
   (state <s> ^name extract-state
             -^predicate-set.done)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-predicates)
}

sp {extract-state*apply*copy-predicates*mark*done
   (state <s> ^name extract-state
              ^operator.name copy-predicates
              ^predicate-set <preds>)
-->
   (<preds> ^done true)
}

