sp {chunk-game-impasse*apply*complete
   (state <s> ^name chunk-game-impasse
              ^operator.name complete
             -^opponent <r>
              ^superstate <ss>
              ^retrieve-handle <r>)
  (<ss> ^opponent false)
-->
    (<ss> ^retrieve-handle <r>)
}

sp {chunk-game-impasse*apply*complete*opponent
   (state <s> ^name chunk-game-impasse
              ^operator.name complete
              ^opponent <r>
              ^superstate <ss>
              ^retrieve-handle <r>)
   (<ss> ^opponent true)
-->
    (<ss> ^retrieve-handle <r>)
}

sp {chunk-game-impasse*propose*complete
   (state <s> ^name chunk-game-impasse)
-->
    (<s> ^operator <o> + <)
    (<o> ^name complete)
}

