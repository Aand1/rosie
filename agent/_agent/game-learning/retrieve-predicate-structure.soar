
sp {retrieve-predicate-structure*elaborate*smem-query
   (state <s> ^name retrieve-predicate-structure
              ^predicate-handle <handle>)
-->
   (<s> ^smem-query <query>)
   (<query> ^cue <cue>
            ^depth 8)
   (<cue> ^handle <handle>)
}

sp {retrieve-predicate-structure*elaborate*smem-retrieve
