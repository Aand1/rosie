sp {dialog-event*propose*failed-comprehension
   (state <s> ^name dialog-event
              ^purpose.type failed-comprehension)
-->
   (<s> ^operator <o> +)
   (<o> ^name failed-comprehension)
}
  
sp {dialog-event*apply*failed-comprehension
   (state <s> ^name dialog-event
              ^operator <o>
              ^top-state <ts>)
   (<ts> ^io.output-link <out>
         ^interaction.status <status>)
   (<o> ^name failed-comprehension)
-->
   (<out> ^send-message <msg>)
   (<msg> ^type single-word-response
          ^fields.word |Comprehension failed. Please rephrase your entry.|)
   (<status> ^performed.dialog-event <de>)
   (<de> ^type single-word-response
         ^originator agent)
   }