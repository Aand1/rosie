### success on searching the semantic memory with the cue provided
sp {get-linguistic-id*propose*write*to*superstate*no-failure
   (state <s> ^name get-linguistic-id)
-->
   (<s> ^operator <op> + <)
   (<op> ^name complete-get-linguistic-id
   	 ^linguistic <lid>)
}

sp {get-linguistic-id*apply*write-to-superstate
   (state <s> ^name get-linguistic-id
   	      ^superstate <ss>
	      ^linguistic {<lid> <> failure}
	      ^operator <op>)   
   (<op> ^name complete-get-linguistic-id)
-->
   (<ss> ^linguistic <lid>)
}


## Hack for the smem bug: put the pp-object on the top-state$
sp {get-linguistic-id*LTIstore*pp-object
   (state <s> ^name get-linguistic-id
   	      ^top-state.LTIstore <ls>
	      ^linguistic.pp-object <pobj>)
   (<op> ^name complete-get-linguistic-id)
-->
   (<ls> ^pp-object <pobj>)
}





