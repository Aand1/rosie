sp {train-predicate*elaborate*retrieve*predicate
   (state <s> ^name train-predicate
              ^predicate <pred>)
   (<pred> -^handle)
-->
   (<s> ^retrieve-lti <pred>)
}

sp {train-predicate*elaborate*retrieve*predicate*property
   (state <s> ^name train-predicate
              ^predicate.property <prop>)
   (<prop> -^handle)
-->
   (<s> ^retrieve-lti <prop>)
}

