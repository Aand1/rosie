### extract relevant attributes from retrieved episode
sp {retrospective-learning*copy*retrieved*environment*attributes
   (state <s> ^name retrospective-learning
   	      ^environment <senv>
	      ^epmem.result.retrieved.environment <env>)
   (<env> ^{<> clear <> intermediate <> not-predicate <> not-state <attr> <> predicate} <val>)
-->
   (<senv> ^<attr> <val>)
}

sp {retrospective-learning*elaborate*object-map
   (state <s> ^name retrospective-learning)
-->
   (<s> ^object-map <map>)
}

sp {retrospective-learning*elaborate*world
   (state <s> ^name retrospective-learning
              ^retrieved-episode <ret>)
-->
   (<s> ^world <world>)
   (<world> ^objects <objs>
            ^predicates <preds>
            ^robot <robot>)
}

sp {retrospective-learning*elaborate*world*object
   (state <s> ^name retrospective-learning
              ^retrieved-episode <ret>
              ^world.objects <objs>
              ^object-map <map>)
   (<ret> ^world.objects.object <obj>)
-->
   (<map> ^mapping <e>)
   (<e> ^epmem <obj>
        ^wmem <new-obj>)
}