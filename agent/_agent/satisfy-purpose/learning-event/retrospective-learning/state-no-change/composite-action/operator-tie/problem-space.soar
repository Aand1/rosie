sp {retrospective-learning*composite-action*operator-tie*elaborate*problem-space
   (state <s> ^superstate.problem-space.name composite-action
              ^impasse tie
              ^attribute operator)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name composite-action
         ^action <action>
         ^state <state>
         ^learning <learn>
         ^default-state-copy no)
   (<action> ^type simulate)
   (<state> ^type imagined)
}

sp {retrospective-learning*composite-action*operator-tie*elaborate*name*selection
   (state <s> ^superstate.problem-space <sps>
              ^impasse tie
              ^attribute operator)
   (<sps> ^name composite-action)
-->
   (<s> ^name selection)
}

sp {retrospective-learning*composite-action*elaborate*problem-space*learning*retrospective
  (state <s> ^name selection
              ^problem-space <ps>
              ^current-subaction <sub>)
  (<ps> ^name composite-action
         ^learning <learn>)
-->
  (<learn> ^type retrospective
           ^use chunking)
}

