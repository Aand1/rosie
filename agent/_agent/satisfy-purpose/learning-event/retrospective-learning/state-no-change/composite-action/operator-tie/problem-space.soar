sp {retrospective-learning*composite-action*operator-tie*elaborate*problem-space
   (state <s> ^superstate.problem-space <sps>
              ^impasse tie
              ^attribute operator)
   (<sps> ^name composite-action
          ^learning.type retrospective)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name composite-action
         ^action <action>
         ^state <state>
         ^learning <learn>
         ^default-state-copy no)
   (<action> ^type simulate)
   (<state> ^type imagined)
   (<learn> ^type retrospective)
}

sp {retrospective-learning*composite-action*operator-tie*elaborate*name*selection
   (state <s> ^superstate.problem-space <sps>
              ^impasse tie
              ^attribute operator)
   (<sps> ^name composite-action
          ^learning.type retrospective)
-->
   (<s> ^name selection)
}
