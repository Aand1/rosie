sp {store*general*goal*definition*objects
   (state <s> ^name learn-definition
   	      ^goal-id <gid>
	      ^goal <goal>
	      ^smem.command <scomm>
	      ^generalizable <gen>
	      ^pairing <p>)
   (<p> ^smem <grel>
   	^wmem <relp>)
   (<gen> ^wme-id <obj>
   	  ^lt-id <sobj>)
   (<goal> ^<pred-link> <relp>)
   (<relp> ^<relarg> <obj>)
   (<gid> ^<pred-link> <grel>)
-->
   (<grel> ^<relarg> <sobj>)
   (<scomm> ^store <sobj> <gid> <grel>)
}




sp {store*general*goal*definition*relation
   (state <s> ^name learn-definition
   	      ^goal-id <gid>
	      ^goal <goal>
	      ^smem.command <scomm>
	      ^generalizable <gen>
	       ^pairing <p>)
   (<p> ^smem <grel>
   	^wmem <relp>)
   (<gen> ^wme-id <obj>
   	  ^lt-id <sobj>)
   (<goal> ^<pred-link> <relp>)
   (<relp> ^relation.smem-rep <obj>)
   (<gid> ^<pred-link> <grel>)
-->
   (<grel> ^relation <sobj>)
   (<scomm> ^store <sobj> <gid> <grel>)
}
