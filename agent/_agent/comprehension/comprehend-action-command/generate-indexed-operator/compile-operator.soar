sp {generate*operator*name
   (state <s> ^name generate-indexed-operator
   	      ^procedural-id <pid>)
   (<pid> ^op_name <op-name>)
-->
   (<s> ^indexed-operator <op>)
   (<op> ^name <op-name>)
 }

sp {generate*operator*direct-object*arguments
   (state <s> ^name generate-indexed-operator
   	      ^procedural-id <pid>
	      ^linguistic-id <lid>
	      ^indexable-parameter <ip>
	      ^indexed-operator <op>)
   (<ip> ^direct-object <sub-ip>)
   (<sub-ip> ^result <obj>)
   (<lid> ^direct-object <lobj>)
   (<pid> ^<arg> <lobj>)
-->
   (<op> ^<arg> <obj>)
}

sp {generate*operator*pp-object*arguments
   (state <s> ^name generate-indexed-operator
   	      ^procedural-id <pid>
	      ^linguistic-id <lid>
	      ^indexable-parameter <ip>
	      ^indexed-operator <op>)
   (<ip> ^pp-object <sub-ip>)
   (<sub-ip> ^result <res>)
   (<res> ^relation <rel>
   	  ^object <obj>)
   (<lid> ^pp-object <ppobj>)
   (<ppobj> ^preposition <prep-id>
   	    ^object <objid>)
   (<pid> ^<arg> <arg-obj>)
   (<arg-obj> ^relation <prep-id>
   	      ^argument <objid>)
-->
   (<op> ^<arg> <new>)
   (<new> ^relation <rel>
   	  ^argument <obj>)
}

sp {generate*operator*pp-object*arguments*no-relation
   (state <s> ^name generate-indexed-operator
   	      ^procedural-id <pid>
	      ^linguistic-id <lid>
	      ^indexable-parameter <ip>
	      ^indexed-operator <op>)
   (<ip> ^pp-object <sub-ip>)
   (<sub-ip> ^result <res>)
   (<res> -^relation <rel>
   	  ^object <obj>)
   (<lid> ^pp-object <ppobj>)
   (<ppobj> -^preposition <prep-id>
   	    ^object <objid>)
   (<pid> ^<arg> <arg-obj>)
   (<arg-obj> -^relation <prep-id>
   	      ^argument <objid>)
-->
   (<op> ^<arg> <new>)
   (<new> ^argument <obj>)
}