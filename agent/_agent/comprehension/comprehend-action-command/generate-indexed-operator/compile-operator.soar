sp {generate*operator*name
   (state <s> ^name generate-indexed-operator
   	      ^procedural-id <pid>)
   (<pid> ^op_name <op-name>)
-->
   (<s> ^indexed-operator <op>)
   (<op> ^name <op-name>)
 }

sp {generate*operator*direct-object*arguments*level1
   (state <s> ^name generate-indexed-operator
   	      ^procedural-id <pid>
	      ^linguistic-id <lid>
	      ^indexable-parameter <ip>
	      ^indexed-operator <op>)
   (<ip> ^direct-object <sub-ip>)
   (<sub-ip> ^result <obj>)
   (<lid> ^direct-object <lobj>)
   (<pid> ^<arg> <lobj>)
-->
   (<op> ^<arg> <obj>)
}

sp {generate*operator*pp-object*arguents*level1
   (state <s> ^name generate-indexed-operator
   	      ^procedural-id <pid>
	      ^linguistic-id <lid>
	      ^indexable-parameter <ip>
	      ^indexed-operator <op>)
   (<ip> ^pp-object <sub-ip>)
   (<pid> ^<arg> <val>)
   (<val> ^argument <objid>)
   (<lid> ^pp-object.object <objid>)
-->
   (<op> ^<arg> <new>)
   (<s> ^pairing <pair>)
   (<pair> ^pp-object <sub-ip>
   	   ^procedural-id-node <val>)
}

sp {generate*operator*pp-object*argument
   (state <s> ^name generate-indexed-operator
   	      ^procedural-id <pid>
	      ^linguistic-id <lid>
	      ^indexed-operator.<arg> <new>
	      ^pairing <pair>)
   (<pair> ^pp-object <sub-ip>
   	   ^procedural-id-node <val>)
   (<sub-ip> ^result <res>)
   (<res> ^object <obj>)
   (<lid> ^pp-object <ppobj>)
   (<ppobj> ^object <objid>)
   (<pid> ^<arg> <val>)
   (<val> ^argument <objid>)
-->
   (<new> ^argument <obj>)
}


sp {generate*operator*pp-object*relation
   (state <s> ^name generate-indexed-operator
