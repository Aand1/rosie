### propose retrieve-concept to retrieve pp-object in the action-concept-network
sp {propose*pp-object*get-linguistic-id
   (state <s> ^name get-linguistic-id
   	      ^linguistic <ling>
	      ^bit flip
	      -^retrieved <ppobj>)
   (<ling> ^pp-object <ppobj>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name retrieve-concept
   	 ^ltid <ppobj>
	 ^recursive true)
}

### propose retrieve-concept to retrieve objects 
sp {propose*retrieve-argument*get-linguistic-id
   (state <s> ^name get-linguistic-id
   	      ^linguistic <ling>
	      ^bit flip
	      -^retrieved <dobj>)
   (<ling> ^direct-object <dobj>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name retrieve-concept
   	 ^ltid <dobj>)
}


### propose create-new-mapping cue fails
sp {get-linguistic-id*failure
   (state <s> ^name get-linguistic-id
   	      ^failure.type no-map
	      -^new-mapping
	      ^cue <cue>)
-->
   (<s> ^operator <op> +)
   (<op> ^name create-new-mapping
	 ^cue <cue>
	 ^type no-map)
}

sp {get-linguistic-id*propose*failure*missing-argument
   (state <s> ^name get-linguistic-id
   	      ^failure <f>
	      ^cue <cue>)
   (<f> ^type missing-argument
   	^argument <arg>
	^smem-pointer <pointer>)
   (<pointer> -^<arg>)
-->
   (<s> ^operator <op> +)
   (<op> ^name create-new-mapping
	 ^argument <arg>
	 ^smem-pointer <pointer>	   
	 ^type missing-argument)
}

sp {get-linguistic-id*propose*failure*missing-argument-substructure
   (state <s> ^name get-linguistic-id
   	      ^failure <f>
	      ^cue <cue>)
   (<f> ^type missing-argument-substructure
   	^argument <arg>
	^smem-pointer <pointer>)
   (<pointer> -^<arg>)
-->
   (<s> ^operator <op> +)
   (<op> ^name create-new-mapping
	 ^argument <arg>
	 ^smem-pointer <pointer>	   
	 ^type missing-argument-substructure)
}