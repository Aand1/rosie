sp {retrieve*ltid*semantic-memory
   (state <s> ^name retrieve-concept
	      ^smem.command <scomm>)
   (<ss> ^operator.ltid <ltid>)
-->
   (<scomm> ^retrieve <ltid>)
}

sp {retrieve*ltid*semantic-memory*report
   (state <s> ^name retrieve-concept
   	      ^smem.result <res>
	      ^smem.command <scomm>
	      ^superstate <ss>)
   (<res> ^<any> <result>)
   (<scomm> ^retrieve <ltid>)
 -->
   (<ss> ^retrieved <ltid>)
}
