### prefer get-linguistic-id over create-new-mapping
sp {prefer*get-linguistic-id*create-new-mapping
   (state <s> ^name get-linguistic-id
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name get-linguistic-id)
   (<op2> ^name create-new-mapping)
-->
   (<s> ^operator  <op1> > <op2>)
}

sp {prefer*retrieve-concept*create-new-mapping
   (state <s> ^name get-linguistic-id
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name retrieve-concept)
   (<op2> ^name create-new-mapping)
-->
   (<s> ^operator  <op1> > <op2>)
}

sp {equal*preferences*create-new-mapping
   (state <s> ^name get-linguistic-id
   	      ^operator <op1> +
	      ^operator <op2> +)
   (<op1> ^name create-new-mapping
   	  ^argument <arg>
	  ^smem-pointer <pointer>)
   (<op2> ^name create-new-mapping
   	  ^argument <arg>
	  ^smem-pointer <pointer>)
-->
   (<s> ^operator  <op1> = <op2>)
}