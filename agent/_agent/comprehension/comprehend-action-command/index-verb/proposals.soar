sp {propose*get-linguistic-id
   (state <s> ^name index-verb
   	      ^cue <cue>
	      -^linguistic <any>)
-->
   (<s> ^operator <op> +)
   (<op> ^name get-linguistic-id
   	 ^cue <cue>
	 ^category indexing)
}

sp {propose*get-parent-map
   (state <s> ^name index-verb
   	      ^linguistic <ling>
	      -^map-id <map>)
-->
   (<s> ^operator <op> +)
   (<op> ^name get-map-id
   	 ^linguistic <ling>
	 ^category indexing)
}

sp {propose*get-procedural-id
   (state <s> ^name index-verb
   	      ^map-id <map>
	      -^procedural <proc>)
  -->
  (<s> ^operator <op> + =)
  (<op> ^name get-procedural-id
  	^map-id <map>
	^category indexing)
}

