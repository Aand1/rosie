sp {index-parameter*propose*expand-object
   (state <s> ^name index-parameter
              ^parameter <p>)
   (<p> ^type object
        ^lexical.word {<word> <> UNKNOWN <> unknown}
       -^indexable.lexical <word>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name expand-object
        ^word <word>)
}

sp {index-parameter*apply*expand-object
   (state <s> ^name index-parameter
              ^operator <o>
              ^parameter <p>)
   (<o> ^name expand-object
        ^word <word>)
-->
   (<p> ^indexable <i>)
   (<i> ^type word
        ^lexical <word>
        ^index-info.use-interaction true)
}