###########################################################
## ON
sp {spatial-reasoning*elaborate*topstate*prepositions*on
   (state <s> ^superstate nil
              ^prepositions <p>)
-->
   (<p> ^prep.name on)
}
smem --add {
	(<P2> ^prep-name on ^name on ^relation <R21> <R22> <R23>)   
   (<R21> ^avg 0 ^axis x ^bottom 0 ^count 0 ^squaredsum 0 ^sum 0 ^top 0 ^type aligned )
   (<R22> ^avg 0.005 ^axis z ^bottom 0.00001 ^count 3 ^squaredsum 0 ^sum 0 ^top 0.1 ^type greater )
   (<R23> ^avg 0 ^axis y ^bottom 0 ^count 0 ^squaredsum 0 ^sum 0 ^top 0 ^type aligned )
}

