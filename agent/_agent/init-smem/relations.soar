(@R1000 ^name on1 ^type relation ^relation <R1000x> <R1000y> <R1000z> )
   (<R1000x> ^avg 0.0 ^axis x ^bottom 0.0 ^count 0 ^squaredsum 0.0 ^sum 0.0 ^top 0.0 ^type aligned )
   (<R1000y> ^avg 0.0 ^axis y ^bottom 0.0 ^count 0 ^squaredsum 0.0 ^sum 0.0 ^top 0.0 ^type aligned )
   (<R1000z> ^avg 0.005 ^axis z ^bottom 0.00001 ^count 3 ^squaredsum 0.0 ^sum 0.0 ^top 0.1 ^type greater )
(@R1001 ^name in1)
(@R1002 ^name right-of1)
(@R1003 ^name left-of1)
(@R1004 ^name above1)
(@R1005 ^name below1)
(@R1006 ^name smaller-than1)
(@R1007 ^name linear-with1)
(@R1008 ^name next-to1)
(@R1009 ^name aligned-with1)
(@R1010 ^name under1)
(@R1011 ^name near1)
(@R1012 ^name diagonal-with1)
(@R1013 ^name diagonal-from1)
