
sp {spatial-reasoning*elaborate*topstate*prepostions
   (state <s> ^superstate nil)
-->
   (<s> ^prepositions <p>)
   #(<p> ^prep.name on)
}

sp {spatial-reasoning*elaborate*topstate*cheating-info
   (state <s> ^superstate nil
              ^smem.command <cmd>)
-->
   (<s> ^ltis <ltis>)
   (<ltis> ^shape <shape>
           ^arch <arch>
           ^on <on>)
   (<shape> ^name shape
            ^type visual)
   (<arch> ^name shape-arch
          ^property <shape>)
   (<on> ^prep-name on ^name on ^relation <R21> <R22> <R23>)   
   (<R21> ^avg 0.0 ^axis x ^bottom 0.0 ^count 0 ^squaredsum 0.0 ^sum 0.0 ^top 0.0 ^type aligned )
   (<R22> ^avg 0.005 ^axis z ^bottom 0.00001 ^count 3 ^squaredsum 0.0 ^sum 0.0 ^top 0.1 ^type greater )
   (<R23> ^avg 0.0 ^axis y ^bottom 0.0 ^count 0 ^squaredsum 0.0 ^sum 0.0 ^top 0.0 ^type aligned )
   (<cmd> ^store <shape> <arch> <on> <R21> <R22> <R23>)
}
