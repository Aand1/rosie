sp {init-agent*propose*complete-init-agent
   (state <s> ^name init-agent)
-->
   (<s> ^operator <o> + <)
   (<o> ^name complete-init-agent)
}

sp {init-agent*apply*complete-init-agent
   (state <s> ^name init-agent
              ^operator.name complete-init-agent
              ^superstate <ss>)
-->
   (<ss> ^name rosie)
}