
sp {interpret-failure*propose*interpret-action-tie
   (state <s> ^name interpret-failure
              ^failure.type action-tie
             -^handled <failure>)
-->
   (<s> ^operator <o> +)
   (<o> ^name interpret-action-tie)
}

sp {interpret-failure*apply*interpret-action-tie*mark*handled
   (state <s> ^name interpret-failure
              ^operator.name interpret-action-tie
              ^failure <failure>)
-->
   (<s> ^handled <failure>)
}

sp {interpret-failure*apply*interpret-action-tie*create*message
   (state <s> ^name interpret-failure
              ^operator.name interpret-action-tie)
-->
   (<s> ^outgoing-message <msg>)
   (<msg> ^type simple-message
          ^message-type action-tie )
}

sp {interpret-failure*apply*interpret-action-tie*terminate*segment
   (state <s> ^name interpret-failure
              ^operator.name interpret-action-tie
              ^topstate.interaction.changes <c>)
-->
   (<c> ^terminate-segment true)
}

   