sp {interaction*propose*interpret-message
   (state <s> ^name interaction
   	      ^message <msg>)
   (<msg> ^
-->
   (<s> ^operator <o> +)
   (<o> ^name interpret-message
        ^message <msg>)
}

sp {interaction*propose*push-segment
   (state <s> ^name interaction
              ^interaction.new-segment-info <info>)
-->
   (<s> ^operator <o> +)
   (<o> ^name push-segment
        ^new-segment-info <info>)
}