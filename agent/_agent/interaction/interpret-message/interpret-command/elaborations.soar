sp {interpret-command*elaborate*allow-substate
   (state <s> ^name interpret-command)
-->
   (<s> ^allow-substate true)
}
              

sp {interpret-command*elaborate*message*from*superoperator
   (state <s> ^name interpret-command
              ^superstate.operator.message <msg>)
-->
   (<s> ^command-message <msg>)
}