sp {interpret-command*propose*create-action-command
   (state <s> ^name interpret-command
              ^command-message.action.id <id>
             -^action-command)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-action-command
        ^action-id <id>)
} 

sp {interpret-command*apply*create-action-command
   (state <s> ^name interpret-command
              ^operator <o>)
   (<o> ^name create-action-command
        ^action-id <id>)
-->
   (<s> ^action-command <cmd>)
   (<cmd> ^id <id>)
}