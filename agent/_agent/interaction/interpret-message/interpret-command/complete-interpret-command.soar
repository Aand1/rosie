sp {interpret-command*propose*complete-interpret-command
   (state <s> ^name interpret-command
              ^action-command <cmd>)
-->
   (<s> ^operator <o> + <)
   (<o> ^name complete-interpret-command)
}

sp {interpret-command*apply*complete-interpret-command
   (state <s> ^name interpret-command
              ^operator.name complete-interpret-command
              ^superstate <ss>
              ^command-message <msg>
              ^action-command <cmd>)
   (<cmd> ^id.name <action-name>)
-->
   (<ss> ^new-segment-info <info>)
   (<info> ^purpose <p>
           ^originator instructor)
   (<p> ^type action
        ^parameters.action-command <cmd>
        ^satisfaction.action-event.type <action-name>)
   (<msg> ^interpreted true)
}
