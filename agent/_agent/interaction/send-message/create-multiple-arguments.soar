sp {send-message*propose*create-multiple-arguments
   (state <s> ^name send-message
              ^message.type multiple-arguments
              ^message.argument-info <arg>
             -^created-message)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-multiple-arguments
        ^argument-info <arg>)
}

sp {send-message*apply*create-multiple-arguments
   (state <s> ^name send-message
              ^operator <o>)
   (<o> ^name create-multiple-arguments
        ^argument-info <arg>)
-->
   (<s> ^created-message <msg>)
   (<msg> ^type multiple-arguments
          ^originator agent
          ^fields.argument-info <arg>)
}