sp {send-message*propose*create-execution-failure
   (state <s> ^name send-message
              ^message.type execution-failure
              ^message.failure-type <type>
              ^message.failure-info <info>
             -^created-message)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-execution-failure
        ^failure-info <info>
        ^failure-type <type>)
}

sp {send-message*apply*create-execution-failure
   (state <s> ^name send-message
              ^operator <o>)
   (<o> ^name create-execution-failure
        ^failure-type <type>
        ^failure-info <info>)
-->
   (<s> ^created-message <msg>)
   (<msg> ^type execution-failure
          ^originator agent
          ^fields <f>)
   (<f> ^failure-type <type>
        ^failure-info <info>)
}