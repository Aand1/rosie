sp {action-space*execute*propose*reset-SNC
   (state <s> ^problem-space.action-space execute
              ^reset-SNC true)
-->
   (<s> ^operator <o> + >)
   (<o> ^name reset-SNC)
}

sp {action-space*execute*apply*reset-SNC
   (state <s> ^problem-space.action-space execute
              ^operator.name reset-SNC
              ^reset-SNC true)
-->
   (<s> ^reset-SNC true -)
}