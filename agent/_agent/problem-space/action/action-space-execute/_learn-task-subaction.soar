sp {action*execute*unproposed*subaction*propose*learn-task-subaction
   (state <s> ^problem-space.name action
              ^problem-space.action.type execute
              ^child-action.execution-operator <sub>
              ^action-operator <action>
              ^action-id <action-id>)
   (<sub> ^name <op-name>
          ^action-handle { <h> <> go-to-next-waypoint1 <> go-to-waypoint1 }
          ^arg1 <sub-arg1>
         -^arg2)
   (<sub-arg1> ^arg-type <type>
           ^id <id>)
   -{(<s> ^operator { <o> <> <sub> } +)
     (<o> ^name <op-name>
          ^action-handle <h>
          ^arg1 <arg1>
         -^arg2)
     (<arg1> ^arg-type <type>
             ^id <id>)}
-->
   (<s> ^operator <o> + >)
   (<o> ^name learn-task-subaction
        ^subaction <sub>
        ^action-operator <action>
        ^action-id <action-id>)

   (<s> ^operator <sub> -)
}
