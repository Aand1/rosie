sp {action-space*execute*SNC*propose*clear-SNC
   (state <s> ^name action-execute-SNC
              ^created-subaction <sub-h>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name clear-SNC)
   }

sp {action-space*execute*SNC*apply*clear-SNC
   (state <s> ^name action-execute-SNC
              ^operator.name clear-SNC
              ^quiescence t
              ^superstate <ss>)
-->
   (<ss> ^reset-SNC true)
}
              