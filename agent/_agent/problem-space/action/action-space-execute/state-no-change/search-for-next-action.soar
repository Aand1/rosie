sp {action*execute*SNC*propose*search-for-next-action
   (state <s> ^name action-execute-SNC
              ^desired <d>)
-->
   (<s> ^operator <o> +)
   (<o> ^name search-for-next-action)
}

sp {action*exectue*SNC*apply*search-for-next-action*interrupt
   (state <s> ^name action-execute-SNC
              ^agent-params.debug-action-search true
              ^operator.name search-for-next-action)
-->
   (interrupt)
}