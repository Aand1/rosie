sp {no-subaction-proposal*elaborate*problem-space
   (state <s> ^name no-subaction-proposal)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action <action>)
}

sp {no-subaction-proposal*propose*report-failure-no-subaction-proposal
   (state <s> ^name no-subaction-proposal)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name report-failure-no-subaction-proposal)
}

