sp {unmatched-action*elaborate*problem-space
   (state <s> ^name unmatched-action)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action <action>)
}

sp {unmatched-action*propose*report-failure-unmatched-action
   (state <s> ^name unmatched-action)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name report-failure-unmatched-action)
}

