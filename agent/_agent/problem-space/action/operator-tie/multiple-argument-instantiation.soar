   (state <s> ^name multiple-argument-instantiations
              ^operator <o>
              ^failure.argument-info.predicates <preds>)
   (<o> ^name report-multiple-arguments
        ^duplicate-info.obj-preds.{ <prop> << color shape size category name >> } <pred>)
