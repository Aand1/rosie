sp {action-space*waypoint-search*elaborate*problem-space
   (state <s> ^superstate.problem-space.action-space waypoint-search
             -^default-problem-space no)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action-space waypoint-search
         ^default-state-copy no
         ^action <action>
         ^learning <learn>
         ^world <world>
         ^perception <perc>)
   (<action> ^type simulate)
   (<world> ^copy-type shallow)
   (<perc> ^attend-to-all-perception no)
}
