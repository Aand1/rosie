sp {copy-action*elaborate*operator*info
   (state <s> ^name copy-action
              ^superstate.operator <ss-op>)
   (<ss-op> ^name copy-action
            ^source-action <src-action>
            ^destination <dest>
            ^copy-name <name>)
-->
   (<ss> ^source-action <src-action>
         ^destination <dest>
         ^copy-name <name>)
}

sp {copy-action*elaborate*object-map*superoperator
   (state <s> ^name copy-action
              ^superstate.operator.object-map <map>)
-->
   (<s> ^object-map <map>)
}

sp {copy-action*elaborate*object-map*none
   (state <s> ^name copy-action
              -^superstate.operator.object-map <map>)
-->
   (<s> ^no-object-map true)
}

# elaborate argument information 
## ENUMERATE: 0-5 arguments
sp {copy-action*elaborate*no-args
   (state <s> ^name copy-action
              ^source-action <a>)
   (<a> -^{ << arg1 arg2 arg3 arg4 arg5 until-clause >> } <arg>)
-->
   (<s> ^num-args 0)
}

sp {copy-action*elaborate*arguments*1*arg
   (state <s> ^name copy-action
              ^source-action <a>)
   (<a> ^{ <n1>        << arg1 arg2 arg3 arg4 arg5 until-clause >> } <a1>
       -^{ <n2> <> <n1> << arg1 arg2 arg3 arg4 arg5 until-clause >> } <a2>)
   (<a1> ^arg-type <t1>)
--> 
   (<s> ^num-args 1
        ^copy-argument <ca1>)
   (<ca1> ^arg-type <t1>
          ^arg-name <n1>
          ^source-arg <a1>)
}

sp {copy-action*elaborate*arguments*2*args
