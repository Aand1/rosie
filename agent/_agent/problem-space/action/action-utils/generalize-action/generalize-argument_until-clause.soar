# Rename the until-clause as a predicate-set

sp {anystate*generalize-argument*elaborate*until-clause*as*predicate-set
  (state <s> generalize-argument <ga>)
  (<ga> ^instance.arg-type until-clause)
-->
  (<ga> ^arg-type predicate-set)
}

sp {anystate*apply*generalize-action*copy*arg-type*until-clause
   (state <s> ^generalize-action.generalize-argument <arg>
              ^operator.name generalize-action)
   (<arg> ^instance.arg-type until-clause
          ^generalized-rep <gen-rep>)
-->
   (<gen-rep> ^arg-type until-clause)
}