sp {action*search*elaborate*problem-space
   (state <s> ^superstate.problem-space <ss-ps>
              ^superstate.operator.action-handle <h>)
   (<ss-ps> ^name action
            ^action.type search)
-->
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action <action>
         ^learning <learn>
         ^world <world>
         ^perception <perc>)
   (<action> ^type search)
   (<world> ^copy-type shallow)
   (<perc> ^attend-to-all-perception no)
}

sp {action*search*elaborate*problem-space*action*propose-action-operators*true
   (state <s> ^problem-space <ps>
              ^action-operator <aop>
              ^superstate.operator <aop>)
   (<ps> ^name action
         ^action <action>)
   (<action> ^type search)
   (<aop> -^search-succeeded true)
-->
   (<action> ^propose-action-operators true)
}
