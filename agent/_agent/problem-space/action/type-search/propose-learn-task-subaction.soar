sp {action*search*success*propose*learn-task-subaction
   (state <s> ^problem-space <ps>
              ^superstate <ss>
              ^action-operator <subaction>
             -^subaction-structure)
   (<ps> ^name action
         ^action.type search)
   (<ss> ^action-operator <action>
         ^action-id <action-id>)
   (<subaction> ^search-succeeded true)
-->
  (<s> ^operator <o> + >)
   (<o> ^name learn-task-subaction
        ^subaction <subaction>
        ^action-operator <action>
        ^action-id <action-id>)
}

sp {action*search*selection*success*evaluation*mark*search-succeeded*true
    (state <s> ^name selection ^evaluation <e1> ^superstate <ss>)
    (<e1> ^superoperator <o1>
          ^symbolic-value { << partial-success success >> <s*1> }
          ^desired <eb>)
    (<ss> ^desired <eb>
          ^problem-space.name action
          ^problem-space.action.type search)
    -->
   (<o1> ^search-succeeded true)
   (interrupt)
}