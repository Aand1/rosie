sp {rosie*propose*halt
   (state <s> ^name rosie
              ^max-sentence-number <n>
              ^current-sentence-number <n>
              ^terminated-sentence <n>)
-->
   (<s> ^operator <op> + !)
   (<op> ^name halt)
}


sp {apply*halt
   (state <s> ^operator.name halt
              ^success-count <sc>
              ^failure-count <fc>)
-->
   (write (crlf) |++++++++++++ All Done +++++++++++|)
   (write (crlf) |Successful Parses: | <sc>)
   (write (crlf) |Failed Parses: | <fc>)
   (halt)
}
