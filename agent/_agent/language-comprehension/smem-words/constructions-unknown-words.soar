

## Unknown Verb
# Command *-U-DP-T - XXX the red block.
smem --add {
           (<VP-T> ^construction |*-U-D|
                   ^structure-type UV
                   ^current-word D
                   ^prior-word U  
                   ^prior-word-first true
                   ^message-type command	
                   ^assigners  <a1> <a2> <a3>
                   ^converts-to  VP)
           (<a1> ^structure-type U
                 ^parent <VP-T>
                 ^referent-type literal
                 ^syntactic-structure action
                 ^defines-new-structure-type V
                 #^semantic-structure action
                 ^required true)
           (<a2> ^structure-type DP
                 ^parent <VP-T>
                 ^syntactic-structure complement
                 ^semantic-structure object
                 ^required true)
           (<a3> ^structure-type PP
                 ^parent <VP-T>
                 ^syntactic-structure complement2
                 ^semantic-structure destination
                 ^optional true)
}


## Unknown Single Word
smem --add {
           (<U-T> ^construction |*-U-T|
                   ^structure-type S
                   ^current-word T
                   ^prior-word U   
                   ^prior-word-first true
                   ^message-type unknown-word	
                   ^assigners <ass1> <ass2>)
           (<ass1> ^structure-type U
                   ^required true
                   ^syntactic-structure head
                   ^parent <U-T>)
           (<ass2> ^structure-type T
                   ^required true
                   ^syntactic-structure terminator
                   ^parent <U-T>)
}

# Green is a color. - Unknown - could be proper noun
smem --add {
           (<sentence> ^construction |*-U-is-DP|
                       ^structure-type CP
                       ^current-word IS-V
                       ^prior-word U
                       ^prior-word-first true
                       ^message-type adjective-definition
                       ^assigners <isa> <isb> <isc> <isd> #<ise>
                       ^referent <iso1>)
           (<isa> ^structure-type U
                  ^before-type IS-V
                  ^parent <sentence>
                  ^syntactic-structure head
                  ^semantic-structure object-feature
                  ^referent-type literal
                  ^required true)
           (<iso1> ^id soar-assign)
           (<isb> ^structure-type IS-V
                  ^after <isa>
                  ^before <isb>
                  ^lexical-item-spelling |is|
                  ^parent <sentence>
                  ^syntactic-structure predicate
                  ^semantic-structure assignment
                  ^before-type T
                  ^required true)
           (<isc> ^structure-type DP
                  ^exclusive <isd>
                  ^parent <sentence>
                  ^relative-position after
                  ^syntactic-structure predicate
                  ^semantic-structure predicate
                  ^optional true)
           (<isd> ^structure-type N   # In case get |Purple is color.|
                  ^exclusive <isc>
                  ^parent <sentence>
                  ^relative-position after
                  ^syntactic-structure predicate
                  ^semantic-structure predicate
                  ^optional true)
#           (<ise> ^structure-type T
#                  ^required true
#                  ^syntactic-structure terminator
#                  ^parent <sentence>)
}