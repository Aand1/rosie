## Determiners: a, an, the, these, those

### Extend so can handle unknown

smem --add {
############################
           ### Determiners
           (<your> ^spelling |your|
                   ^super-type @D1001
                   ^number singular
                   ^specifier definite
                   ^possessive true
                   ^referent @R1027)
           
            (<my> ^spelling |my|
                   ^super-type @D1001
                   ^number singular
                   ^specifier definite
                   ^possessive true) 
         
           (@Z2 ^spelling |the|
                ^super-type @D1001
                ^number singular
                ^specifier definite)
           
            (<these> ^spelling |these|
                  ^super-type @D1001
                  ^number plural
                  ^specifier definite)
            
           (<andthat> ^prior-word |and|
                      ^spelling |and that|
                      ^current-word |that|
                      ^consumes-prior-word false
                      ^super-type @D1001 
                      ^number singular
                      ^demonstrative true
                      ^specifier definite
                      ^attachment immediate)
           
            (<those> ^spelling |those|
                  ^super-type @D1001
                  ^number plural
                  ^specifier definite)
            
            (<their> ^spelling |their|
                  ^super-type @D1001
                  ^number plural
                  ^specifier definite
                  ^possessive true)
                     
           (<a> ^spelling |a|
                  ^super-type @D1001
                  ^number singular
                  ^specifier indefinite)
                      
           (<an> ^spelling |an|
                  ^super-type @D1001
                  ^number singular
                  ^specifier indefinite)
           
           (<any> ^spelling |any|
                  ^super-type @D1001
                  ^number singular
                  ^specifier indefinite)
           
#           (<some> ^spelling |some|
#                  ^super-type @D1001
#                  ^number plural
#                  ^specifier indefinite)
                                
           (<another> ^spelling |another|
                  ^super-type @D1001
                  ^number singular
                  ^specifier indefinite
                  ^specifier distinct)
                      
}

smem --add {
           (@D1001 ^structure-type D
                   ^assigners <d1> <d2> <d3> <d4>
                   ^converts-to DP
                   ^super-converts-to CP)
           (<d1> ^structure-type ADJ
                  ^syntactic-structure adjoin
                  ^repeatable true
                  ^relative-position after
                  ^optional true
                  ^before <d2>
                  ^before <d3>
                  ^before <d4>)
           (<d2> ^structure-type N
                  ^syntactic-structure head 
                  ^relative-position after
                  ^required true
                  ^before <d3>
                  ^before <d4>)
           (<d3>  ^structure-type PP
                  ^syntactic-structure complement
                  ^relative-position after
                  ^optional true
                 # ^directional false
                  ^before <d4>)
           (<d4> ^structure-type RCP
                 ^syntactic-structure complement
                 ^relative-position after
                 ^optional true)
}
           
