smem --add {
           (@D1001 ^structure-type D
                   ^assigners <d1> <d2> <d3>
                   ^converts-to DP)
           (<d1> ^structure-type ADJ
                  ^syntactic-structure adjoin
                  ^repeatable true
                  ^relative-position after
                  ^optional true
                  ^before <d2> 
                  ^before <d3>)
           (<d2> ^structure-type N
                  ^syntactic-structure head 
                  ^relative-position after
                  ^required true 
                  ^before <d3>)
           (<d3> ^structure-type PP
                  ^syntactic-structure complement
                  ^relative-position after
                  ^optional true)
}
           