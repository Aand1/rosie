# Prepositions: above, at, after, behind, for, from, in, into, up, over
#   (always in preposition phrase with a noun)
# Aboard, about, above, across, after, against, along, amid, among, around,
# at, before, behind, below, beneath, beside, between, beyond, by, down, during,
# except, for, from, in, into, like, near, of, off, on, onto, out, over, past,
# since, through, throughout, to, toward, under, underneath, until, unto, up, upon, with, within, without

## Example Prepositions: above, at, after, behind, for, from, in, into, up, over, smaller than
smem --add {
           
   ### Prepositions
           
           (<of> ^spelling |of|
                 ^structure-type P
                 ^relation <of-lti>
                 ^assigners <of1>
                 ^attachment immediate 
                 ^converts-to PP)
           (<of1> ^structure-type DP
                  ^parent <of>
                  ^relative-position after
                 # ^semantic-structure of-relation
                  ^syntactic-structure head
                  ^required true)
           (<of-lti> ^name of1)
           
           (<for> ^spelling |for|
                 ^structure-type P
                 ^relation <for-lti>
                 ^assigners <for1>
                 ^attachment immediate 
                 ^converts-to PP)
           (<for1> ^structure-type DP
                  ^parent <for>
                  ^relative-position after
                  ^syntactic-structure head
                  ^required true)
             (<for-lti> ^name for1)
                      
           (<to> ^spelling |to|
                 ^structure-type P
                 ^relation @R1000 # Same as ON
                 ^assigners <e1>
                 ^directional true
                 ^converts-to  PP)
           (<e1> ^structure-type DP
                 ^parent <to>
                 ^relative-position after
               # ^semantic-structure to-relation
                 ^syntactic-structure head
                 ^required true)
           
           (<on> ^spelling |on|
                 ^structure-type P
                 ^alternative-structure ADJ
                 ^relation @R1000
                 ^assigners <onr1>
                 ^converts-to  PP)
           (<onr1> ^structure-type DP
                   ^parent <on>
                   ^relative-position after
                   ^syntactic-structure head
                   ^optional true)
                      
           (<off> ^spelling |off|
                  ^structure-type P
                  ^alternative-structure ADJ
                  ^relation <off-lti>
                  ^assigners <offr1>
                  ^converts-to  PP)
           (<offr1> ^structure-type DP
                    ^parent <off>
                    ^relative-position after
                    ^syntactic-structure head
                    ^optional true)
           (<off-lti> ^name off1)
                   
           (<in> ^spelling |in|
                 ^structure-type P
                 ^relation @R1001
                 ^assigners <inr2>
                 ^converts-to  PP)
           (<inr2> ^structure-type DP
                   ^parent <in>
                   ^relative-position after
                   ^syntactic-structure head
                   ^required true)
           
           (<inside> ^spelling |inside|
                     ^structure-type P
                     ^relation @R1001 # Same as IN
                     ^assigners <insider2>
                     ^converts-to  PP)
           (<insider2> ^structure-type DP
                       ^parent <inside>
                       ^relative-position after
                       ^syntactic-structure head
                       ^required true)
           
           (<below> ^spelling |below|
                     ^structure-type P
                     ^relation @R1005
                     ^assigners <below1>
                     ^converts-to  PP)
           (<below1> ^structure-type DP
                       ^parent <below>
                       ^relative-position after
                       ^syntactic-structure head
                       ^required true)
           
                      
           (<behind> ^spelling |behind|
                     ^structure-type P
                     ^relation @R1014 
                     ^assigners <behind1>
                     ^converts-to  PP)
           (<behind1> ^structure-type DP
                       ^parent <behind>
                       ^relative-position after
                       ^syntactic-structure head
                       ^required true)
                      
           (<above> ^spelling |above|
                     ^structure-type P
                     ^relation @R1004
                     ^assigners <above1>
                     ^converts-to  PP)
           (<above11> ^structure-type DP
                       ^parent <above>
                       ^relative-position after
                       ^syntactic-structure head
                       ^required true)
                      
           (<into> ^spelling |into|
                   ^structure-type P
                   ^relation @R1001 # Same as IN
                   ^assigners <intor2>
                   ^directional true
                   ^converts-to  PP)
           (<intor2> ^structure-type DP
                     ^parent <into>
                     ^relative-position after
                     ^syntactic-structure head
                     ^required true)
                      
           (<onto> ^spelling |onto|
                   ^structure-type P
                   ^relation @R1000 # Same as ON
                   ^assigners <ontor2>
                   ^directional true
                   ^converts-to PP)
           (<ontor2> ^structure-type DP
                     ^parent <onto>
                     ^relative-position after
                     ^syntactic-structure head
                     ^required true)
                      
           (<until> ^spelling |until|
                    ^structure-type P
                    ^relation <until-lti>
                    ^assigners <until2>
                    ^attachment immediate 
                    ^converts-to  PP)
           (<until2> ^structure-type CP
                     ^parent <until>
                     ^relative-position after
                     ^syntactic-structure head
                     ^semantic-structure arg1
                     ^required true)
           (<until-lti> ^name until1)
                      
           (<up> ^spelling |up|
                 ^structure-type P
                 ^relation <up-lti>)
           (<up-lti> ^name up1)
           
           (<down> ^spelling |down|
                   ^structure-type P
                   ^relation <down-lti>)
           (<down-lti> ^name down1)
           
           (<smaller> ^spelling |smaller|
                   ^structure-type P
                   ^relation @R1006)
           
           (<larger> ^spelling |larger|
                     ^spelling |bigger|
                   ^structure-type P
                   ^relation @R1015)
         
          
}


smem --add {
           (<smaller> ^prior-word |smaller|
                     ^current-word |than|
                     ^spelling |smaller-than|
                     ^structure-type P
                     ^relation @R1006 
                     ^assigners <smaller1>
                     ^converts-to  PP)           
           (<smaller1> ^structure-type DP
                        ^parent <smaller>
                      ^relative-position after
                     ^syntactic-structure head
                     ^required true)
}

smem --add {
           (<larger> ^prior-word |larger|
                     ^prior-word |bigger|                     
                     ^current-word |than|
                     ^spelling |larger-than|
                     ^structure-type P
                     ^relation @R1015
                     ^assigners <larger1>
                     ^converts-to  PP)
           (<larger1> ^structure-type DP
                     ^parent <larger>
                     ^relative-position after
                     ^syntactic-structure head
                     ^required true)
}
smem --add {
		(<left> ^spelling |left|
                   ^structure-type P
                   ^semantic-entity-type relation
                   ^relation @R1003)
}


smem --add {
		(<right> ^spelling |right|
                     ^structure-type P
                     ^semantic-entity-type relation
                     ^relation @R1002)
}

