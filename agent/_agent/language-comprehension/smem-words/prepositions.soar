# Prepositions: above, at, after, behind, for, from, in, into, up, over
#   (always in preposition phrase with a noun)
# Aboard, about, above, across, after, against, along, amid, among, around,
# at, before, behind, below, beneath, beside, between, beyond, by, down, during,
# except, for, from, in, into, like, near, of, off, on, onto, out, over, past,
# since, through, throughout, to, toward, under, underneath, until, unto, up, upon, with, within, without

## Example Prepositions: above, at, after, behind, for, from, in, into, up, over, smaller than

           
   ### Prepositions
           
     
smem --add {            
           (@T9000 ^structure-type P
                   ^assigners <p-lti>
                   ^converts-to PP)
           (<p-lti> ^structure-type DP
                    ^relative-position after
                    ^syntactic-structure head
                    ^required true)
}

smem --add {
           (<of> ^spelling |of|
                 ^super-type @T9000
                 ^relation <of-lti>
                 ^attachment immediate)
           (<of-lti> ^handle of1)
           
           (<for> ^spelling |for|
                 ^super-type @T9000
                 ^relation <for-lti>
                 ^attachment immediate)
           (<for-lti> ^handle for1)
                      
           (<to> ^spelling |to|
                 ^super-type @T9000
                 ^relation @R1000
                 ^directional true)
                      
           (<at> ^spelling |at|
                 ^super-type @T9000
                 ^relation @R1001
                 ^directional true)
                                
           (<from> ^spelling |from|
                 ^super-type @T9000
                 ^relation @R1001
                 ^directional true)
                                   
           (<ontop> ^spelling |ontop|
                 ^super-type @T9000
                 ^relation @R1000
                 ^directional true)
                   
           (<on> ^spelling |on|
                 ^structure-type P
                 ^alternative-structure ADJ
                 ^relation @R1000
                 ^assigners <onr1>
                 ^converts-to PP)
           (<onr1> ^structure-type DP
                   ^relative-position after
                   ^syntactic-structure head
                   ^optional true)
                      
           (<off> #^spelling |off|           ### Need to uncomment to test recovery from missed retrieval Off ADJ (Sentence #33)
                  ^super-type @T9000
                  ^alternative-structure ADJ
                  ^relation <off-lti>)
           (<off-lti> ^handle off1)
                   
           (<in> ^spelling |in|
                 ^super-type @T9000
                 ^relation @R1001)
           
           (<inside> ^spelling |inside|
                     ^super-type @T9000
                     ^relation @R1001) # Same as IN
           
           (<below> ^spelling |below|
                    ^super-type @T9000
                     ^relation @R1005)           
                      
           (<behind> ^spelling |behind|
                     ^super-type @T9000
                     ^relation @R1014)
                      
           (<above> ^spelling |above|
                     ^super-type @T9000
                     ^relation @R1004)
                      
           (<into> ^spelling |into|
                   ^super-type @T9000
                   ^relation @R1001 # Same as IN
                   ^directional true)
                      
           (<onto> ^spelling |onto|
                   ^super-type @T9000
                   ^relation @R1000 # Same as ON
                   ^directional true)
           
           (<holding> ^spelling |holding|
                   ^super-type @T9000
                   ^relation @R1017 # holding1
                   ^directional false)
                      
   # until you detect/see/encounter/sense/hit the wall/intersection/doorway/door
   # until there is a wall/intersection/doorway/door
           (<until> ^spelling |until|
                    ^structure-type P
                    ^relation <until-lti>
                    ^assigners <until2> 
                    ^attachment immediate 
                    ^converts-to UNTIL-PP)
           (<until2> ^structure-type CP
                     ^relative-position after
                     ^syntactic-structure head
                     ^semantic-structure *embed*
                     ^embed-structure until-clause
                     ^required true)
           (<until-lti> ^handle until1)
                      
           (<smaller> ^spelling |smaller|
                      ^structure-type C-ADJ  # Comparative Adjective
                      ^converts-to C-ADJ-P
                      ^relation @R1006)
           
           (<larger> ^spelling |larger|
                     ^spelling |bigger|
                     ^structure-type C-ADJ  # Comparative Adjective
                     ^converts-to C-ADJ-P
                     ^relation @R1015)    
}


smem --add {
           (<smaller> ^prior-word |smaller|
                     ^current-word |than|
                     ^spelling |smaller-than|
                     ^structure-type C-ADJ
                     ^relation @R1006 
                     ^assigners <smaller1>
                     ^converts-to C-ADJ-P)           
           (<smaller1> ^structure-type DP
                      ^relative-position after
                     ^syntactic-structure head
                     ^required true)
}

