           (<minutes> ^spelling |minutes|
                   ^structure-type N
                   ^referent-type literal
                   ^semantic-value <minutes>
                   ^number plural)
           (<minutes> ^id <minutes-id>)
           (<minutes-id> ^name minutes)
           
           (<minute> ^spelling |minute|
                   ^structure-type N
                   ^number singular
                   ^referent-type literal
                   ^semantic-value <minute>)
           (<minute> ^id <minutes-id>)
           (<minute-id> ^name minute)
