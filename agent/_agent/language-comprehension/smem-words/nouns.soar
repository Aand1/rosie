           (<color-lti> ^id <color-name>)
           (<color-name> ^name color
                         ^type visual)
           
           (<location> ^spelling |location|
                       ^structure-type N
                       ^syntactic-referent-type DP
                       ^number singular
                       ^referent nil
                       ^semantic-value <location-lti>)
           (<location-lti> ^id <location-name>)
           (<location-name> ^name location)
           
           (<locations> ^spelling |locations|
