           (<stove> ^spelling |stove|
                    ^structure-type N
                    ^number singular
                    ^object-feature id
                    ^perceptual-feature stove
                    ^on-off-state-change true)
           
           (<garbage> ^spelling |garbage|
                      ^structure-type N
                      ^number singular
                      ^object-feature id
                      ^perceptual-feature garbage)
           
           (<table> ^spelling |table|
                    ^structure-type N
                    ^number singular
                    ^object-feature id
