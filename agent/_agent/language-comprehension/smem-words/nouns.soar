             (<cannibals> ^spelling |cannibals|
                     ^structure-type N
                     ^number plural
                     ^object-feature object-type
                     ^perceptual-feature cannibal1)  
        
             (<missionaries> ^spelling |missionaries|
                     ^structure-type N
                     ^number plural
                     ^object-feature object-type
                     ^perceptual-feature missionary)
           
#           (<tictactoe> ^spelling |tictactoe|
