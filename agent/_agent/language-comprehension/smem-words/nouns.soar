           (<size-lti> ^id <size-name>)
           (<size-name> ^name size
                         ^type visual)
           
           (<block> ^spelling |block|
                    ^structure-type N
                    ^number singular
                    ^object-feature shape
                    ^perceptual-feature block1
                    ^id <block-id>)
           (<block-id> ^name block1
                       ^type shape)
           
           (<blocks> ^spelling |blocks|
                    ^structure-type N
                    ^number plural
                    ^object-feature shape
                    ^perceptual-feature block1
                    ^id <block-id>)
            
           (<triangle> ^spelling |triangle|
                       ^structure-type N
                       ^number singular
                       ^object-feature shape
                       ^perceptual-feature triangle1
                       ^id <triangle-id>)
           (<triangle-id> ^name triangle1
