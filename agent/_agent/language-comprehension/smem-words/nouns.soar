           (<triangles> ^spelling |triangles|
                    ^structure-type N
                    ^number plural
                    ^object-feature shape
                    ^perceptual-feature triangle1
                       ^id <triangle-id>)
                    
           (<sphere> ^spelling |sphere|
                     ^structure-type N
                     ^number singular
                     ^object-feature shape
                     ^perceptual-feature sphere1
                     ^id <sphere-id>)
