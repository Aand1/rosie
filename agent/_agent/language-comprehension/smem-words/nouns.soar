#                        ^semantic-entity-type id
#                        ^number singular
#                        ^referent <ttt>
#                        ^semantic-structure <ttt>
#                        ^converts-to  DP)
#           (<ttt> ^id tictactoe)
           
           (<right> ^spelling |right-noun|
                    ^prior-word |the|
                    ^current-word |right|
                    ^structure-type N
                    ^consumes-prior-word false
                    ^semantic-structure relation
                    ^semantic-entity-type relation
                    ^number singular
                    ^relation <right-of1> 
                    ^referent nil)
           (<right-of1> ^name right-of1)
           
           (<left> ^spelling |left-noun|
                    ^prior-word |the|
                    ^current-word |left|
                    ^structure-type N
                    ^consumes-prior-word false
                    ^semantic-structure relation
                    ^semantic-entity-type relation
                    ^number singular
                    ^relation <left-of1> 
                    ^referent nil)
           (<left-of1> ^name left-of1)
                     
           (<middle> ^spelling |middle-noun|
                    ^prior-word D
                    ^current-word |middle|
                    ^structure-type N
