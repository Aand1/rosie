    ## clean           
           (<clean> ^spelling |clean|
                    ^referent @A1014
                    ^super-type @T997)
 
    ## set           
           (<set> ^spelling |set|
                    ^referent @A1013
                    ^super-type @T997)
                     
    ## activate           
           (<activate> ^spelling |activate|
                    ^referent @A1005
                    ^super-type @T997)
                          
    ## STORE
          (<store> ^spelling |store|
                   ^referent @A1009
                   ^super-type @T996)         
                                    
    ## Point
          (<point> ^spelling |point|
                   ^referent @A1002
                   ^super-type @T995)
          
}
