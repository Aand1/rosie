                  ^optional true)
           (<isd> ^structure-type THIS
                  ^exclusive <isc>
                  ^relative-position after
                  ^syntactic-structure head
                  ^semantic-structure arg1
                  ^specifier definite                  
                  ^demonstrative true
                  ^optional true)           
           (<ise> ^structure-type QT
                  ^required true
                  ^syntactic-structure terminator)
}

