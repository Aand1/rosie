
## ... that is in the stove.
smem --add {
           (<that-is> ^spelling |that|
                   ^structure-type RPN   # relative pronoun
                   ^assigners <isb> <isc> <isd> 
                   ^attachment immediate  ## Maybe implied by being a relative clause
                   ^relation-source <isd>
                   ^can-be-part-of |this-one|
                   ^converts-to  PP)  ### Not true but gets me started - should be RC and add DP and PP so can have PP and any number of RCs - The green ball on the floor that is bouncy
           (<isb> ^structure-type IS-V
                  ^parent <that-is>
                  ^syntactic-structure nil
                  ^required true)
           (<isc> ^structure-type ADV
                  ^after <isb>
                  ^before <isd>
                  ^parent <that-is> 
                  ^syntactic-structure complement
                  ^semantic-structure modifier
                  ^optional true)
          (<isd> ^structure-type PP
                  ^after <isb>
                  ^parent <that-is>
                  ^syntactic-structure nil
                  ^required true) 
}