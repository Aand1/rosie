smem --add {
           
############################
           ### Special Nouns
           
           (<this> ^spelling |this|
                   ^structure-type THIS  
                   ^semantic-entity-type object
                   ^number singular
                   ^demonstrative true)
           
           (<what> ^spelling |what|
                   ^structure-type QP
                   ^semantic-entity-type object
                   ^number singular
                   ^preset-semantic-structure <whatpss>
                   ^question true
                   ^referent nil)
           (<whatpss> ^semantic-structure agent
                      ^syntactic-structure head
                      ^referent <sgwhatobj>)
           (<sgwhatobj> ^object <sgwhat>)
           (<sgwhat> ^id |What?|)
           
           (<where> ^spelling |where|
                    ^structure-type QP
                    ^semantic-entity-type object
                    ^number singular
                    ^preset-semantic-structure <wherepss>
                    ^question true
                    ^referent nil)
           (<wherepss> ^semantic-structure agent
                       ^syntactic-structure head
                       ^referent <sgwhereobj>)
           (<sgwhereobj> ^object <sgwhere>)
           (<sgwhere> ^id |Where?|)
           
           (<one> ^spelling |one|
                  ^structure-type DP
                  ^semantic-entity-type object
                  ^number singular)
}