## Unknown Single Word in constructions-unknown-words

smem --add {
           (<yeah> ^spelling-single-word |yeah|
                   ^structure-type CP
                   ^message-type yes)

           (<ok> ^spelling-single-word |ok|
                 ^structure-type CP
                 ^message-type yes)

           (<yes> ^spelling-single-word |yes|
                  ^structure-type CP
                  ^message-type yes)

           (<no> ^spelling-single-word |no|
                 ^structure-type CP
                 ^message-type no)

           (<nope> ^spelling-single-word |nope|
                   ^structure-type CP
                   ^message-type no)

           (<done> ^spelling-single-word |done|
                   ^structure-type CP
                   ^message-type finished)
           
           (<fini> ^spelling-single-word |finished|
                   ^structure-type CP
                   ^message-type finished)
           
           
           (<rest> ^spelling-single-word |restart|
                   ^structure-type CP
                   ^message-type restart)
           
		   (<task-completed> ^handle task-completed)
           
           (<is-done> ^spelling |done|
                      ^structure-type ADJ
                      ^referent <task-completed>) 
           
           (<is-finished> 
                      ^spelling |finished|
                      ^structure-type ADJ
                      ^referent <task-completed>) 

           (<is-over> ^spelling |over|
                      ^structure-type ADJ
                      ^referent <task-completed>) 


}