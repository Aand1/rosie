                   ^converts-to DP)
           (<CN1> ^structure-type ADJ
                   ^syntactic-structure adjoin
                   ^repeatable true
                   ^relative-position after
                   ^optional true
                   ^before <CN2>)
           (<CN2> ^structure-type N
                   ^syntactic-structure head 
                   ^relative-position after
                   ^required true
                   ^before <CN3>)
           (<CN3> ^structure-type PP
                   ^syntactic-structure complement
                   ^relative-position after
                   ^optional true)
                                 
           
           ## Just like a determiner
           (<zero> ^spelling |zero|
                   ^super-type @T10000  
                   ^number plural
                   ^constraint <c0>)
           (<c0> ^quantity <count0-id>)
                                            
           (<one> ^spelling |one|
                   ^super-type @T10000
                   ^number singular
                   ^constraint <c1>)
           (<c1> ^quantity <count1-id>)
                      
