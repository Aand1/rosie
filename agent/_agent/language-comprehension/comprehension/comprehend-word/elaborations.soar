sp {comprehend-word*elaborate*substate
   (state <s> ^name comprehend-word
             -^name comprehend-construction
              ^superstate.operator <so>)
   (<so> ^name comprehend-word
         ^current-word <cw>)
-->
   (force-learn <s>)
   (<s> ^current-word <cw>
        ^created-assigners 0
        ^retrievals 0)
}


sp {comprehend-word*compare*elaborations
   (state <s> ^name comprehend-word
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name << retrieve-semantic-substructure >>) #create-assigner 
   (<op2> ^name create-receiver)
-->
   (<s> ^operator <op1> > <op2>)
}

sp {comprehend-word*expand-structure*retrieved-lexical-item 
   (state <s> ^name << comprehend-word comprehend-construction >>
              ^segment.comprehension-structure.retrieved-lexical-item <x>)
-->
   (<s> ^retrieved-lexical-item <x>)
}

sp {comprehend-word*expand-structure*c-s
   (state <s> ^name << comprehend-word comprehend-construction >>
              ^segment.comprehension-structure.retrieved-lexical-item <x>)
-->
   (<s> ^expand <x>)
}

sp {comprehend-word*expand-structure*super-type*c-s
   (state <s> ^name << comprehend-word comprehend-construction >>
              ^segment.comprehension-structure.retrieved-lexical-item.super-type <x>)
-->
   (<s> ^expand <x>)
}

