sp {comprehend-word*elaborate*substate
   (state <s> ^name comprehend-word
             -^name comprehend-construction
              ^superstate.segment <seg>)
   (<seg> ^current-word <cw>)
-->
   (<s> ^current-word <cw>)
}

sp {comprehend-word*compare*elaborations
   (state <s> ^name comprehend-word
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name << create-assigner-item process-preset-semantic-structure retrieve-semantic-substructure >>)
   (<op2> ^name create-receiver-item)
-->
   (<s> ^operator <op1> > <op2>)
}
