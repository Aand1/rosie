sp {comprehend-word*propose*retry-create-receiver
   (state <s> ^name << comprehend-word comprehend-construction >>
              ^segment <seg>
              ^current-word <cw>
              ^retrieved-lexical-item <id>)
   (<seg> ^retry <retry>)
  -{(<seg> ^comprehension-structure.not-merged-receiver <uri>)
    (<uri> ^lt <id>
           ^current-word <cw>)}
  -(<retry> ^id <id>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name retry-create-receiver
         ^retry <retry>)
}
sp {comprehend-word*compare*retry-create-receiver
   (state <s> ^name << comprehend-word comprehend-construction >>
              ^operator <op1> +
                        <op2> +)
   (<op1> ^name retry-create-receiver)
   (<op2> ^name << retrieve-semantic-substructure remove-stale-receive retrieve-semantic-substructure >>)
-->
   (<s> ^operator <op1> < <op2>)
}

sp {apply*retry-create-receiver*create-item
#  :interrupt
   (state <s> ^operator <op>
              ^current-word <cw>
              ^retrieved-lexical-item <id>
              ^segment <seg>
             -^name comprehend-construction)
   (<seg> ^retry <retry>
          ^comprehension-structure <cs>
          ^parse-failed-receiver <pfr>)
   (<op> ^name retry-create-receiver
         ^retry <retry>)
   (<id> ^structure-type <stype>
        -^constraint)
   (<retry> ^structure-type { <> <stype> <old-type> } 
            ^lt <old-id>)
-->
   (<seg> ^retry <retry> -
          ^parse-failed-receiver <pfr> -
          ^parse-failed-end-of-sentence true -)
   (<cw> ^processed true)
   (<retry> ^lt <id>
                <old-id> -
            ^structure-type <stype>
                            <old-type> -)}

