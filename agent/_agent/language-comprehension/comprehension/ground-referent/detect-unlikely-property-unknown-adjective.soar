sp {propose*detect-unlikely-property-unknown-adjective
   (state <s> ^name ground-referent
              ^constraint-object.adjoin <adj>
              ^property-constraint.property <prop>)
   (<adj> ^lt <lli>)
   (<lli> -^object-feature
          -^unlikely-object-feature <prop>)
-->
   (<s> ^operator <o> +, >, =)
   (<o> ^name detect-unlikely-property-unknown-adjective
        ^lt <lli>)}

sp {apply*detect-unlikely-property-unknown-adjective
   (state <s> ^operator <op>
              ^property-constraint.property <prop>)
   (<op> ^name detect-unlikely-property-unknown-adjective
         ^lt <lli>)
   (<lli> ^spelling <word>)
-->
   (write (crlf) <lli> |: | <word> | is probably not a | <prop>)
   (<lli> ^unlikely-object-feature <prop>)
   (<s> ^store-concept <lli>)
}
