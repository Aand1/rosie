sp {anystate*apply*index-scene-objects*demonstrative*mark*generated
   (state <s> ^operator <o>)
   (<o> ^name index-scene-objects
        ^candidate-set <cand>
        ^specifier demonstrative)
-->
   (<cand> ^generated-candidates true)
}

sp {anystate*apply*index-scene-objects*demonstrative*create*candidates
   (state <s> ^operator <o>
              ^top-state.world <world>)
   (<world> ^object <obj>
            ^pointed-object <id>)
   (<obj> ^id <id>)
   (<o> ^name index-scene-objects
        ^candidate-set <cand>
        ^specifier demonstrative)           
-->
   (write (crlf) |Found: | <obj>)
   (<cand> ^proto-referent.object <obj>)
}