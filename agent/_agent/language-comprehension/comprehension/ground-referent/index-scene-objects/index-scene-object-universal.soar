sp {anystate*apply*index-scene-objects*universal*mark*generated
   (state <s> ^operator <o>)
   (<o> ^name index-scene-objects
        ^candidate-set <cand>
        ^specifier universal)
-->
   (<cand> ^generated-candidates true)
}

sp {anystate*apply*index-scene-objects*universal*create*candidates
   (state <s> ^operator <o>
              ^topstate.world.object <obj>)
   (<o> ^name index-scene-objects
        ^candidate-set <cand>
        ^specifier universal)
-->
   (<cand> ^referent <candidate>)
   (<candidate> ^object <obj>
                ^score 1)
}