sp {anystate*apply*index-scene-objects*personal*mark*generated
   (state <s> ^operator <o>)
   (<o> ^name index-scene-objects
        ^candidate-set <cand>
        ^specifier personal)
-->
   (<cand> ^generated-candidates true)
}

sp {anystate*apply*index-scene-objects*personal*create*candidates
   (state <s> ^operator <o>
              ^topstate.interaction.referents.objects.object <obj>)
   (<o> ^name index-scene-objects
        ^candidate-set <cand>
        ^specifier personal)           
-->
   (<cand> ^referent <candidate>)
   (<candidate> ^object <obj>)
}