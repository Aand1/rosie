sp {anystate*apply*index-scene-objects-it*mark*generated
   (state <s> ^operator <o>)
   (<o> ^name index-scene-objects
        ^candidate-set <cand>
        ^specifier it)
-->
   (<cand> ^generated-candidates true)
}

sp {anystate*apply*index-scene-objects-it*create*candidates
   (state <s> ^operator <o>
              ^top-state.saved-object-as-it <obj>)
   (<o> ^name index-scene-objects
        ^candidate-set <cand>
        ^specifier it)           
-->
   (write (crlf) |Found it: | <obj>)
   (<cand> ^proto-referent.object <obj>)
}