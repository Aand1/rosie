sp {index-object-properties*propose*satisfy-property-constraint
   (state <s> ^name index-object-properties
              ^candidate-set <cand>
              ^property-constraint <pc>)
   (<cand> -^satisfied-property <pc>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name satisfy-property-constraint
        ^candidate-set <cand>
        ^property-constraint <pc>)
}

sp {index-object-properties*apply*satisfy-property-constraint*mark*satisfied
   (state <s> ^name index-object-properties
              ^operator <o>)
   (<o> ^name satisfy-property-constraint
        ^candidate-set <cands>
        ^property-constraint <pc>)
-->
   (<cands> ^satisfied-property <pc>)
}

sp {index-object-properties*apply*satisfy-property-constraint*remove*violator
   (state <s> ^name index-object-properties
              ^operator <o>)
   (<o> ^name satisfy-property-constraint
        ^candidate-set <cands>
        ^property-constraint <pc>)
   (<cands> ^proto-referent <c>)
   (<c> ^violates <pc>)
-->
