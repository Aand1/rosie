sp {ground-referent*elaborate*substate1
   (state <s> ^name ground-referent
              ^superstate.operator <so>)
   (<so> ^name ground-referent
         ^constraints <cons>)
-->
   (<s> ^constraint-object <cons>)
}

sp {ground-referent*elaborate*constraint-object-proto-referent
   (state <s> ^name ground-referent
              ^constraint-object <cons>)
-->
   (<s> ^candidate-set <cons>)
}

sp {ground-referent*elaborate*constraint-object-proto-referent*generated
   (state <s> ^name ground-referent
              ^candidate-set <cons>)
  -(<cons> -^proto-referent <r>)
-->
   (<cons> ^generated-candidates true)
}

sp {ground-referent*elaborate*substate2
   (state <s> ^name ground-referent
              ^superstate.operator <so>)
   (<so> ^name ground-referent
         ^constraints.constraint <con>)
   (<con> ^property.id.name <property>
          ^name <value>)
-->
   (<s> ^property-constraint <pc>)
   (<pc> ^property <property>
         ^value <value>)
}
sp {ground-referent*elaborate*substate3
   (state <s> ^name ground-referent
              ^superstate.operator <so>)
   (<so> ^name ground-referent
         ^constraints.constraint <con>)
   (<con> ^property.name <property>
          ^name <value>)
-->
   (<s> ^property-constraint <pc>)
   (<pc> ^property <property>
         ^value <value>)
}

sp {ground-referent*elaborate*substate4
  # :interrupt
   (state <s> ^name ground-referent
              ^superstate.operator <so>)
   (<so> ^name ground-referent
         ^constraints.constraint <con>)
   (<con> ^{ << movable category >> <type> } <value>)
-->
   (<s> ^property-constraint <pc>)
   (<pc> ^property <type>
         ^value <value>)
}
