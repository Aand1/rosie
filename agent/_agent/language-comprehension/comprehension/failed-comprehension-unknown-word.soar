sp {comprehension*propose*failed-comprehension-unknown-word
   (state <s> ^name comprehension
              ^segment <seg>)
   (<seg> ^original-sentence <is>
          ^not-merged-receiver <uri>
          ^not-merged-assigner <uai>)
   (<is> ^processing-result failure
        -^processed-sentence true)
   (<uri> ^structure-type U)
   (<uai> ^structure-type <> U
          ^lt.required true)
-->
   (<s> ^operator <op> + =)
   (<op> ^name failed-comprehension-unknown-word
         ^not-merged-receiver <uri>
         ^not-merged-assigner <uai>)
}


sp {comprehension*apply*failed-comprehension-unknown-word
   :interrupt
   (state <s> ^operator <op>
              ^smem.command <cmd>)
   (<op> ^name failed-comprehension-unknown-word
         ^not-merged-receiver <uri>
         ^not-merged-assigner <uai>)
   (<uai> ^structure-type <new>)
   (<uri> ^lt <lli>)
   (<lli> ^structure-type U)
--> 
   (<cmd> ^store <lli>)
   (write (crlf) |Guess that unknown type must be: | <new>)
   (<uri> ^structure-type U -
                          <new>)
   (<lli> ^structure-type U -
                          <new>)
}