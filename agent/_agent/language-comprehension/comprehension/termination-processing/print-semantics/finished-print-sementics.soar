sp {print-semantics*propose*finished-print-sementics
   (state <s> ^name print-semantics)
-->
   (<s> ^operator <op> + =, <)
   (<op> ^name finished-print-sementics)
}
sp {apply*finished-print-sementics
   (state <s> ^operator <op>
              ^superstate <ss>)
   (<op> ^name finished-print-sementics)
-->
   # (cmd watch 1)
   (write (crlf) |----------------------
   |)
   (<ss> ^printed-semantics true)
}
