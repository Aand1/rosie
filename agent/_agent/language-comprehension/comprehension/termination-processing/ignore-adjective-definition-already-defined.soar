sp {termination-processing*propose*ignore-adjective-definition-already-defined
   :interrupt
   (state <s> ^name termination-processing
             -^stored-message <sem>
              ^segment.semantics <sem>)
   (<sem> ^message-type adjective-definition
          ^object-feature <ofih>)
   (<ofih> ^object-feature <pred>
           ^perceptual-feature <pf>)
   -->
   (<s> ^operator <op> + =, >)
   (<op> ^name ignore-adjective-definition-already-defined)
}
sp {termination-processing*apply*adjective-already-exists*ignore
   (state <s> ^operator.name ignore-adjective-definition-already-defined
              ^segment.semantics <sem>)
   (<sem> ^message-type adjective-definition)
-->
   (<sem> ^message-type adjective-definition -
                       ignore)
   (write (crlf) |Adjective already defined. Ignore.|)
}