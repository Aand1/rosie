           
sp {language-comprehension*propose*initialize-rosie
   (state <s> ^name rosie
             -^world
              ^top-state.world-usage internal)
-->
   (<s> ^operator <op> + =, !)
   (<op> ^name initialize-rosie)
}

sp {apply*initialize-rosie*sentence-number
   (state <s> ^name rosie
              ^operator.name initialize-rosie)
-->
   (<s> ^current-sentence-number 6
        ^min-sentence-number 1
        ^failure-count 0
        ^success-count 0)}

sp {apply*initialize-rosie*initialize-world
   (state <s> ^operator.name initialize-rosie)
-->
   (<s> ^world <world>)
   (<world> ^object <obj0> <obj1> <obj2> <obj3> <obj4> <obj5> <obj6> <obj7>
                    <loc1> <loc2> <loc3> <loc4> <loc5> <loc6>
            ^prepositions <on1> <in1> <left-of1> <right-of1>)
   (<obj0> ^type object
           ^id self)
   (<obj1>
           ^id large-orange-triangle1
           ^property <type-object>
           ^property <color-orange>
           ^property <shape-triangle>
           ^property <size-large>
           ^property <movable>)
   (<type-object> ^name type
                  ^value <obj-val>)
   (<obj-val> ^label object)
   (<color-orange> ^name color
                   ^type visual
                   ^value <orange-val>)
   (<orange-val> ^label orange1
                 ^confidence 1)
   (<shape-triangle> ^name shape
                     ^type visual
                     ^value <triangle-val>)
   (<triangle-val> ^label triangle1
                   ^confidence 1)
   (<size-large> ^name size
                 ^type visual
                 ^value <large-val>)
   (<large-val> ^label large1
                ^confidence 1)
   (<movable> ^name movable
              ^type visual
              ^value <true>)
   (<true> ^label true
           ^confidence 1)
   (<obj2> ^id small-red-triangle1
           ^property <type-object>
           ^property <color-red>
           ^property <shape-triangle>
           ^property <size-small>
           ^property <movable>)
   (<color-red> ^name color
                ^type visual
                ^value <red-val>)
   (<red-val> ^label red1
              ^confidence 1)
   (<size-small> ^name size
                 ^type visual
                 ^value <small-val>)
   (<small-val> ^label small1
                ^confidence 1)
   (<obj3> ^id medium-green-block1
           ^property <type-object>
           ^property <color-green>
           ^property <shape-block>
           ^property <size-medium>
           ^property <movable>)
   (<color-green> ^name color
                  ^type visual
                  ^value <green-val>)
   (<green-val> ^label green1
                ^confidence 1)
   (<shape-block> ^name shape
                  ^type visual
                  ^value <block-val>)
   (<block-val> ^label block1
                ^confidence 1)
   (<size-medium> ^name size
                  ^type visual
                  ^value <medium-val>)
   (<medium-val> ^label medium
                 ^confidence 1)
   (<obj4> ^id large-green-block1
           ^property <type-object>
           ^property <color-green>
           ^property <shape-block>
           ^property <size-large>
           ^property <movable>)
   (<obj5> ^id large-green-sphere
           ^property <type-object>
           ^property <color-green>
           ^property <shape-sphere>
           ^property <size-large>
           ^property <movable>
                     )
   (<shape-sphere> ^name shape
                   ^type visual
                   ^value <sphere-val>)
   (<sphere-val> ^label sphere1
                 ^confidence 1)
   (<obj6> ^id small-orange-triangle1
           ^property <type-object>
           ^property <color-orange>
           ^property <shape-triangle>
           ^property <size-small>
           ^property <movable>
                     )  
   (<color-purple> ^name color
                  ^type visual
                  ^value <purple-val>)
   (<purple-val> ^label purple1
                ^confidence 1)
   (<obj7> ^id medium-purple-triangle1
           ^property <type-object>
           ^property <color-purple>
           ^property <shape-triangle>
           ^property <size-medium>
           ^property <movable>)  
   (<loc1> ^id pantry1
           ^property <type-location>
                     <pantry-name>
                     <not-movable>)
   (<type-location> ^name type
                    ^value <loc-value>)
   (<loc-value> ^label location)
   (<pantry-name> ^name name
                  ^type linguistic
                  ^value <pantry-val>)
   (<pantry-val> ^label pantry1 ^word pantry ^confidence high)
   (<not-movable> ^name movable
                  ^type visual
                  ^value <false>)
   (<false> ^label false)
   (<loc2> ^id stove1
           ^property <type-location>
                     <stove-name>
                     <not-movable>)
   (<stove-name> ^name name
                 ^type linguistic
                 ^value <stove-val>)
   (<stove-val> ^label stove1 ^word stove ^confidence high)
   (<loc3> ^id garbage1
           ^property <type-location>
                     <garbage-name>
                     <not-movable>)
   (<garbage-name> ^name name
                   ^type linguistic
                   ^value <garbage-val>)
   (<garbage-val> ^label garbage1 ^word garbage ^confidence high)
   (<loc4> ^id sink1
           ^property <type-location>
                     <sink-name>
                     <not-movable>)
   (<sink-name> ^name name
                ^type linguistic
                ^value <sink-val>)
   (<sink-val> ^label sink1 ^word sink ^confidence high)
   (<loc5> ^id kitchen1
           ^property <type-location>
                     <kitchen-name>
                     <not-movable>)
   (<kitchen-name> ^name name
                   ^type linguistic
                   ^value <kitchen-val>)
   (<kitchen-val> ^label kitchen1 ^word kitchen ^confidence high)
   (<loc6>  ^id table1
            ^property <type-location>
                      <table-name>
                      <not-movable>)
   (<table-name> ^name name
                 ^type linguistic
                 ^value <table-val>)
   (<table-val> ^label table1 ^word table ^confidence high)
   (<on1> ^name on1
          ^pair <on-stove>
                <on-triangle>)
   (<in1> ^name in1
          ^pair <in-pantry>)
   (<right-of1> ^name left-of1 
                ^pair <green1>)
   (<left-of1> ^name right-of1
               ^pair <green2>)
# The medium green block <obj3> is on the stove <loc2>
   (<on-stove> ^a medium-green-block1
               ^b stove1)
   # The small red triangle <obj2> is in the pantry <loc1>
   (<in-pantry> ^a small-red-triangle1
                ^b pantry1)
# The medium green block <obj3> is to the left of the large green block <obj4>
   (<green1> ^a medium-green-block1
             ^b large-green-block1)
   (<green2> ^a large-green-block1
             ^b medium-green-block1)
# The large orange triangle <obj1> is on the small red triangle <obj2>
   (<on-triangle> ^a large-orange-triangle1
                  ^b small-red-triangle1)
}
