
sp {language-comprehension*propose*initialize-rosie
   (state <s> ^name rosie
             -^world
              ^top-state.world-usage internal)
-->
   (<s> ^operator <op> + =, !)
   (<op> ^name initialize-rosie)
}

sp {apply*initialize-rosie*initialize-world
   (state <s> ^operator.name initialize-rosie)
-->
   (<s> ^world <world>)
   (<world> ^objects <objects>
            ^predicates <predicates>
            ^robot <robot>)
   (<robot> ^handle rosie
            ^item-type object 
            ^arm.action wait
            ^predicates.handle rosie)  ### NEW STUFF FOR ROSIE
   (<objects> ^object <obj0> <obj1> <obj2> <obj3> <obj4> <obj5> <obj6> <obj7> <obj8> # <obj9>
                      <loc1> <loc2> <loc3> <loc4> 
                      <loc5> <loc6> <loc7>)

   (<predicates> ^predicate <on1> <in1> <left-of1> <right-of1> <pointed>)
   (<obj0> ^type object
           ^handle self
           ^predicates <plot0>)
   (<plot0> ^type object)
   #This is what is pointed at
   (<obj1> ^handle large-orange-triangle1 ^predicates <plot1> ^item-type object)
   (<plot1> ^category block1 ^color orange1 ^object-id large-orange-triangle1 ^shape triangle1 ^size large1
           ^status live ^movable true ^indicated pointed)
   (<obj2> ^handle small-red-triangle1  ^item-type object
           ^predicates <plot2>)
   (<plot2> ^category block1 ^color red1 ^object-id small-red-triangle1 ^shape triangle1 ^size small1
           ^status live ^movable true)
   (<obj3> ^handle medium-green-block1 ^item-type object
           ^predicates <plot3>)
   (<plot3> ^category block1 ^color green1 ^object-id medium-green-block1 ^shape rectangle1 ^size medium1
           ^status live ^movable true)
   (<obj4> ^handle large-green-block1 ^item-type object
           ^predicates <plot4>)
   (<plot4> ^category block1 ^color green1 ^object-id large-green-block1 ^shape rectangle1 ^size large1
            ^status live ^movable true)
   (<obj5> ^handle large-green-sphere1 ^item-type object
           ^predicates <plot5>)
   (<plot5> ^category block1 ^color green1 ^object-id large-green-sphere1 ^shape sphere1 ^size large1
            ^status live ^movable true)
   (<obj6> ^handle small-orange-triangle1 ^item-type object
           ^predicates <plot6>)
   (<plot6> ^category block1 ^color orange1 ^object-id large-orange-triangle1 ^shape triangle1 ^size small1
           ^status live ^movable true)
   (<obj7> ^handle medium-purple-triangle1 ^item-type object
           ^predicates <plot7>)
   (<plot7> ^category block1 ^color purple1 ^object-id medium-purple-triangle1 ^shape triangle1 ^size medium1
           ^status live ^movable true)
   (<obj8> ^handle small-green-box1 ^item-type object
           ^predicates <plot8>)
   (<plot8> ^category block1 ^color green1 ^object-id small-green-box1 ^shape box1 ^size small1
           ^status live ^movable true)
#  (<obj9> ^handle small-soda1 ^item-type object
#           ^predicates <plot9>)
#   (<plot9> ^category block1 ^color red1 ^object-id small-soda1 ^shape soda1 ^size small1
#           ^status live ^movable true)

   (<loc1> ^handle 1 ^item-type object
           ^predicates <ploc1>)
   (<ploc1> ^category location1 ^object-id 1 ^name pantry ^door closed ^status live)

   (<loc2> ^handle 2 ^item-type object
           ^predicates <ploc2>)
   (<ploc2> ^category location1 ^object-id 2 ^name stove ^door closed ^heat off ^status live)

   (<loc3> ^handle 3 ^item-type object
           ^predicates <ploc3>)
   (<ploc3> ^category location1 ^object-id 3 ^name garbage ^door closed ^status live)
   
   (<loc4> ^handle 4 ^item-type object
           ^predicates <ploc4>)
   (<ploc4> ^category location1 ^object-id 4 ^name sink ^status live)
    
   (<loc5> ^handle 5 ^item-type object
           ^predicates <ploc5>)
   (<ploc5> ^category location1 ^object-id 5 ^name kitchen ^status live)  
   
   (<loc6> ^handle 6 ^item-type object
           ^predicates <ploc6>)
   (<ploc6> ^category location1 ^object-id 6 ^name table ^status live)
   (<loc7> ^handle 7 ^item-type object
           ^predicates <ploc7>)
   (<ploc7> ^category location1 ^object-id 7 ^name waypoint ^status live)
   
   (<on1> ^handle on1 ^item-type predicate
          ^predicate-info @R1001
          ^instance <on-1> 
          ^instance <on-2>)
 # The medium green block <obj3> is on the stove <loc2>
   (<on-1> ^1 medium-green-block1 ^2 2)  # 
 # The large orange triangle <obj1> is on the small red triangle <obj2>
   (<on-2> ^1 large-orange-triangle1 ^2 small-red-triangle1)

   # The small red triangle <obj2> is in the pantry <loc1>
   (<in1> ^handle in1 ^predicate-info @R1001 ^instance <in-1> ^item-type predicate)
   (<in-1> ^1 small-red-triangle1 ^2 1)
  
 # The medium green block <obj3> is to the left of the large green block <obj4>
  (<left-of1> ^handle left-of1 ^predicate-info @R1003 ^instance <left-1> ^item-type predicate)
  (<left-1> ^1 medium-green-block1 ^2 large-green-block1)  
  
  (<right-of1> ^handle right-of1 ^predicate-info @R1002 ^instance <right-1> ^item-type predicate)
  (<right-1> ^1 large-green-block1 ^2 medium-green-block1)  
}
