sp {attend-to-scene*propose*complete-attend-to-scene
   (state <s> ^name attend-to-scene)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-attend-to-scene
        ^category perception)
}

sp {attend-to-scene*apply*complete-attend-to-scene
   (state <s> ^name attend-to-scene
              ^operator.name complete-attend-to-scene
              ^superstate.operator <o>)
-->
   (<o> ^status complete)
}