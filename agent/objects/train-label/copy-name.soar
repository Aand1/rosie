sp {train-label*propose*copy-name
   (state <s> ^name train-label
              ^sense <sense>
              ^example-object <obj>)
   (<sense> ^property.name name
            ^word.word <name>)
   (<obj> ^property <prop>)
   (<prop> ^name name
          -^value.word <name>)
-->
   (<s> ^operator <o> +)
   (<o> ^name copy-name
        ^category object-processing
        ^object-name <name>)
}

sp {train-label*apply*copy-name*copy
   (state <s> ^name train-label
              ^operator <op>
              ^example-object.property <prop>)
   (<op> ^name copy-name
         ^object-name <name>)
   (<prop> ^name name)
-->
   (<prop> ^value <val>)
   (<val> ^word <name>
          ^label <name>
          ^confidence high)
}