sp {top-state*propose*filter-objects
   (state <s> ^superstate nil
              ^attended true)
-->
   (<s> ^operator <o> +)
   (<o> ^name filter-objects
        ^category object-processing)
   (interrupt)
}

sp {filter-objects*propose*create-filter
   (state <s> ^name filter-objects
             -^created-filter)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-filter
        ^category object-processing)
}

sp {filter-objects*apply*create-filter
   (state <s> ^name filter-objects
              ^operator.name create-filter
              ^topstate.svs <svs>)
   (<svs> ^command <cmd>)
-->
   (<cmd> ^extract_once <filter>)
   (<filter> ^type has-property
             ^a <all-nodes> 
             ^property-name color
             ^property-value color-blue)
   (<all-nodes> ^type all_nodes)
}